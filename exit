[32m+[m[32m    public class CodigoMovimento_BancoBrasil: AbstractCodigoMovimento, ICodigoMovimento[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        #region Construtores[m[41m [m
[32m+[m[41m[m
[32m+[m		[32mpublic CodigoMovimento_BancoBrasil()[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mtry[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mcatch (Exception ex)[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao carregar objeto", ex);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public CodigoMovimento_BancoBrasil(int codigo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.carregar(codigo);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao carregar objeto", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m#endregion[m[41m [m
[32m+[m[41m[m
[32m+[m[32m        #region Metodos Privados[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void carregar(int codigo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.Banco = new Banco_Brasil();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch ((EnumCodigoMovimento_BancoBrasil)codigo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case  EnumCodigoMovimento_BancoBrasil.EntradaConfirmada:[m[41m[m
[32m+[m[32m                        this.Codigo = (int)EnumCodigoMovimento_BancoBrasil.EntradaConfirmada;[m[41m[m
[32m+[m[32m                        this.Descricao = "Entrada confirmada";[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case EnumCodigoMovimento_BancoBrasil.EntradaRejeitada:[m[41m[m
[32m+[m[32m                        this.Codigo = (int)EnumCodigoMovimento_BancoBrasil.EntradaRejeitada;[m[41m[m
[32m+[m[32m                        this.Descricao = "Entrada rejeitada";[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case EnumCodigoMovimento_BancoBrasil.TransferenciaCarteiraEntrada:[m[41m[m
[32m+[m[32m                        this.Codigo = (int)EnumCodigoMovimento_BancoBrasil.TransferenciaCarteiraEntrada;[m[41m[m
[32m+[m[32m                        this.Descricao = "Transferência de carteira/entrada";[m[41m[m
[32m+[m[32m                        break;[m[41m[m
