[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IEspecieDocumento.Sigla; }[m[41m[m
[32m+[m[32m            set { _IEspecieDocumento.Sigla = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string Especie[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IEspecieDocumento.Especie; }[m[41m[m
[32m+[m[32m            set { _IEspecieDocumento.Especie = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Métodos Privados[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void InstanciaEspecieDocumento(int codigoBanco, int codigoEspecie)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                switch (codigoBanco)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //341 - Itaú[m[41m[m
[32m+[m[32m                    case 341:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_Itau(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //356 - BankBoston[m[41m[m
[32m+[m[32m                    case 479:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_BankBoston(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //422 - Safra[m[41m[m
[32m+[m[32m                    case 1:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_BancoBrasil(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //237 - Bradesco[m[41m[m
[32m+[m[32m                    case 237:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_Bradesco(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case 356:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_Real(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case 33:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_Santander(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case 347:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_Sudameris(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case 104:[m[41m[m
[32m+[m[32m                        _IEspecieDocumento = new EspecieDocumento_Caixa(codigoEspecie);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        throw new Exception("Código do banco não implementando: " + codigoBanco);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a execução da transação.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static EspeciesDocumento CarregaTodas(int codigoBanco)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (codigoBanco)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case 1:[m[41m[m
[32m+[m[32m                        return EspecieDocumento_BancoBrasil.CarregaTodas();[m[41m[m
[32m+[m[32m                    case 237:[m[41m[m
[32m+[m[32m                        return EspecieDocumento_Bradesco.CarregaTodas();[m[41m[m
[32m+[m[32m                    case 341:[m[41m[m
[32m+[m[32m                        return EspecieDocumento_Itau.CarregaTodas();[m[41m[m
[32m+[m[32m                    case 356:[m[41m[m
[32m+[m[32m                        return EspecieDocumento_Itau.CarregaTodas();[m[41m[m
[32m+[m[32m                    case 104:[m[41m[m
[32m+[m[32m                        return EspecieDocumento_Caixa.CarregaTodas();[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        throw new Exception("Espécies do Documento não implementado para o banco : " + codigoBanco);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao listar objetos", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static string ValidaSigla(IEspecieDocumento especie)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return especie.Sigla;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Empty;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static int ValidaCodigo(IEspecieDocumento especie)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return especie.Codigo;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return 0;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
