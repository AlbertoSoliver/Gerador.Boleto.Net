[32m+[m[32m                        this.NumeroCarteira = (int)EnumCarteiras_Itau.SemRegistroEmissaoEntregaCarne;[m[41m[m
[32m+[m[32m                        this.Codigo = "I";[m[41m[m
[32m+[m[32m                        this.Tipo = "S";[m[41m[m
[32m+[m[32m                        this.Descricao = "Sem registro, com emissão e entrega - Carnê";[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case EnumCarteiras_Itau.SemRegistroEmissaoEntrega15Posicoes:[m[41m[m
[32m+[m[32m                        this.NumeroCarteira = (int)EnumCarteiras_Itau.SemRegistroEmissaoEntrega15Posicoes;[m[41m[m
