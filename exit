[32m+[m[41m[m
[32m+[m[32m        public void Valida()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            //Validações básicas, caso ainda tenha implementada na classe do banco.ValidaBoleto()[m[41m[m
[32m+[m[32m            if (this.Cedente == null)[m[41m[m
[32m+[m[32m                throw new Exception("Cedente não cadastrado.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui o nome do banco ao local de pagamento[m[41m[m
[32m+[m[32m            //Comentada por duplicidade no nome do banco[m[41m[m
[32m+[m[32m            //this.LocalPagamento += this.Banco.Nome + "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do processamento é valida[m[41m[m
[32m+[m[32m            if (this.DataProcessamento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                this.DataProcessamento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do documento é valida[m[41m[m
[32m+[m[32m            if (this.DataDocumento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                this.DataDocumento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            this.Banco.ValidaBoleto(this);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public void FormataCampos()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.QuantidadeMoeda = 0;[m[41m[m
[32m+[m[32m                this.Banco.FormataCodigoBarra(this);[m[41m[m
[32m+[m[32m                this.Banco.FormataLinhaDigitavel(this);[m[41m[m
[32m+[m[32m                this.Banco.FormataNossoNumero(this);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a formatação dos campos.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
