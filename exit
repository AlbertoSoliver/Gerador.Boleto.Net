[32m+[m[41m[m
[32m+[m[32m        private string GerarImagem()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string address = webBrowser.Url.ToString();[m[41m[m
[32m+[m[32m            int width = 670;[m[41m[m
[32m+[m[32m            int height = 805;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int webBrowserWidth = 670;[m[41m[m
[32m+[m[32m            int webBrowserHeight = 805;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            Bitmap bmp = WebsiteThumbnailImageGenerator.GetWebSiteThumbnail(address, webBrowserWidth, webBrowserHeight, width, height);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string file = Path.Combine(Environment.CurrentDirectory, "boleto.bmp");[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m            bmp.Save(file);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return file;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
