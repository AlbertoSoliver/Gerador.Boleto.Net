[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarHeaderRemessaCNAB400(string numeroConvenio, Cedente cedente)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _brancos = new string(' ', 91);[m[41m[m
[32m+[m[32m                string _complemento = new string(' ', 187);[m[41m                [m
[32m+[m[32m                string _header;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _header = "01REMESSA01COBRANCA       009";[m[41m[m
[32m+[m[32m                _header += numeroConvenio.ToString();[m[41m[m
[32m+[m[32m                _header += "         ";[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength(cedente.Nome, 30, 30, ' ', 0, true, true, false).ToUpper();[m[41m[m
[32m+[m[32m                _header += "341";[m[41m[m
[32m+[m[32m                _header += "BANKBOSTON     ";[m[41m[m
[32m+[m[32m                _header += DateTime.Now.ToString("ddMMyy");[m[41m[m
[32m+[m[32m                _header += "01600BPI";[m[41m[m
[32m+[m[32m                _header += _brancos;[m[41m[m
[32m+[m[32m                _header += "        ";[m[41m[m
[32m+[m[32m                _header += _complemento;[m[41m[m
[32m+[m[32m                _header += "000001";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _header = Utils.SubstituiCaracteresEspeciais(_header);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar HEADER do arquivo de remessa do CNAB400.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region DETALHE[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// DETALHE do arquivo CNAB[m[41m[m
[32m+[m[32m        /// Gera o DETALHE do arquivo remessa de acordo com o lay-out informado[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override string GerarDetalheRemessa(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _detalhe = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                base.GerarDetalheRemessa(boleto, numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        _detalhe = GerarDetalheRemessaCNAB240();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        _detalhe = GerarDetalheRemessaCNAB400(boleto, numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _detalhe;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do DETALHE arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarDetalheRemessaCNAB240()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarDetalheRemessaCNAB400(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                base.GerarDetalheRemessa(boleto, numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string _brancos1 = new string(' ', 20);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // USO DO BANCO - Identificação da operação no Banco (posição 87 a 107)[m[41m[m
[32m+[m[32m                string identificaOperacaoBanco = new string(' ', 21);[m[41m[m
[32m+[m[32m                string usoBanco = new string(' ', 10);[m[41m[m
[32m+[m[32m                string _brancos = new string(' ', 40);[m[41m[m
[32m+[m[32m                string _detalhe;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _detalhe = "1";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Tipo de inscrição da empresa[m[41m[m
[32m+[m[32m                // 01 - CPF DO CEDENTE[m[41m[m
[32m+[m[32m                // 02 - CNPJ DO CEDENTE[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (boleto.Cedente.CPFCNPJ.Length <= 11)[m[41m[m
[32m+[m[32m                    _detalhe += "01";[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    _detalhe += "02";[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(boleto.Cedente.CPFCNPJ.ToString(), 14, 14, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _detalhe += _brancos1;[m[41m[m
[32m+[m[32m                _detalhe += _brancos1 + "     ";[m[41m[m
[32m+[m[32m                _detalhe += "000000000";[m[41m[m
[32m+[m[32m                _detalhe += "00000000000 "; // Valor por dia de antecipação[m[41m[m
[32m+[m[32m                _detalhe += "R$  ";[m[41m[m
[32m+[m[32m                _detalhe += _brancos1;[m[41m               [m
[32m+[m[32m                Carteira_BankBoston  _carteira = new Carteira_BankBoston(Convert.ToInt32(boleto.Carteira));[m[41m[m
[32m+[m[32m                _detalhe += _carteira.Tipo;[m[41m[m
[32m+[m[32m                _detalhe += "01";[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(boleto.NumeroDocumento, 10, 10, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _detalhe += boleto.DataVencimento.ToString("ddMMyy");[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(Convert.ToString(boleto.ValorBoleto * 100), 13, 13, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _detalhe += "479     ";[m[41m[m
[32m+[m[32m                _detalhe += boleto.EspecieDocumento.Sigla;[m[41m[m
[32m+[m[32m                _detalhe += "N";[m[41m[m
[32m+[m[32m                _detalhe += DateTime.Now.ToString("ddMMyy");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (boleto.Instrucoes.Count > 1)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _detalhe += Utils.FitStringLength(boleto.Instrucoes[0].Codigo.ToString(), 2, 2, '0', 0, true, true, true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if ((boleto.Instrucoes[0].Codigo == 2) || (boleto.Instrucoes[0].Codigo == 3))[m[41m[m
[32m+[m[32m                        _detalhe += "05";[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        _detalhe += "00";[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(Convert.ToString(boleto.JurosMora * 100), 13, 13, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _detalhe += boleto.DataVencimento.ToString("ddMMyy");[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(Convert.ToString(boleto.ValorDesconto * 100), 13, 13, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(Convert.ToString(boleto.IOF * 100), 13, 13, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(Convert.ToString(boleto.Abatimento * 100), 13, 13, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                if (boleto.Sacado.CPFCNPJ.Length > 11)[m[41m[m
[32m+[m[32m                    _detalhe += "01";  // CPF[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    _detalhe += "02"; // CNPJ[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(boleto.Sacado.CPFCNPJ, 14, 14, '0', 0, true, true, true).ToUpper();[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(boleto.Sacado.Nome, 30, 30, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength((boleto.Sacado.Endereco.End + " " + boleto.Sacado.Endereco.Numero + " - " + boleto.Sacado.Endereco.Complemento + " " + boleto.Sacado.Endereco.Bairro), 37, 37, ' ', 0, true, true, true).ToUpper();[m[41m[m
[32m+[m[32m                _detalhe += "               ";[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(boleto.Sacado.Endereco.CEP, 8, 8, ' ', 0, true, true, false).ToUpper(); ;[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(boleto.Sacado.Endereco.Cidade, 15, 15, ' ', 0, true, true, false).ToUpper();[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(boleto.Sacado.Endereco.UF, 2, 2, ' ', 0, true, true, false).ToUpper();[m[41m[m
[32m+[m[32m                _detalhe += _brancos;[m[41m[m
[32m+[m[32m                _detalhe += "   ";[m[41m[m
[32m+[m[32m                _detalhe += Utils.FitStringLength(numeroRegistro.ToString(), 6, 6, '0', 0, true, true, true);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _detalhe = Utils.SubstituiCaracteresEspeciais(_detalhe);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _detalhe;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar DETALHE do arquivo CNAB400.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion DETALHE[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region TRAILER[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// TRAILER do arquivo CNAB[m[41m[m
[32m+[m[32m        /// Gera o TRAILER do arquivo remessa de acordo com o lay-out informado[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override string GerarTrailerRemessa(int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _trailer = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                base.GerarTrailerRemessa(numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        _trailer = GerarTrailerRemessa240();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        _trailer = GerarTrailerRemessa400(numeroRegistro);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _trailer;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarTrailerRemessa240()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarTrailerRemessa400(int numeroRegistro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _complemento = new string(' ', 393);[m[41m[m
[32m+[m[32m                string _trailer;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _trailer = "9";[m[41m[m
[32m+[m[32m                _trailer += _complemento;[m[41m[m
[32m+[m[32m                _trailer += Utils.FitStringLength(numeroRegistro.ToString(), 6, 6, '0', 0, true, true, true); // Número sequencial do registro no arquivo.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _trailer = Utils.SubstituiCaracteresEspeciais(_trailer);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _trailer;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do registro TRAILER do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de processamento do arquivo CNAB400[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
