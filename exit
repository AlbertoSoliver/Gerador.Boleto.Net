[32m+[m[32m        protected override void Dispose(bool disposing)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (disposing && (components != null))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                components.Dispose();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            base.Dispose(disposing);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
