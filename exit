[32m+[m[32m                    case 104:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Caixa();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //341 - Itaú[m[41m[m
[32m+[m[32m                    case 341:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Itau();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //356 - Real[m[41m[m
[32m+[m[32m                    case 275:[m[41m[m
[32m+[m[32m                    case 356:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Real();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //422 - Safra[m[41m[m
[32m+[m[32m                    case 422:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Safra();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //237 - Bradesco[m[41m[m
[32m+[m[32m                    case 237:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Bradesco();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //347 - Sudameris[m[41m[m
[32m+[m[32m                    case 347:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Sudameris();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //353 - Santander[m[41m[m
[32m+[m[32m                    case 353:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Santander();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //070 - BRB[m[41m[m
[32m+[m[32m                    case 70:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_BRB();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //479 - BankBoston[m[41m[m
[32m+[m[32m                    case 479:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_BankBoston();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //001 - Banco do Brasil[m[41m[m
[32m+[m[32m                    case 1:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Brasil();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //399 - HSBC[m[41m[m
[32m+[m[32m                    case 399:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_HSBC();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //003 - HSBC[m[41m[m
[32m+[m[32m                    case 3:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Basa();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //33 - Unibanco[m[41m[m
[32m+[m[32m                    case 33:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Santander();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //41 - Banrisul[m[41m[m
[32m+[m[32m                    case 41:[m[41m[m
[32m+[m[32m                        _IBanco = new Banco_Banrisul();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        throw new Exception("Código do banco não implementando: " + codigoBanco);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a execução da transação.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
