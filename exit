[32m+[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 5));[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        if (boleto.Cedente.Convenio.ToString().Length != 6)[m[41m[m
[32m+[m[32m                            throw new NotImplementedException(string.Format("Para a carteira {0} e o tipo da modalidade 21, o número do convênio são de 6 posições", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 17);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                /*[m[41m[m
[32m+[m[32m                  * Convênio de 4 posições[m[41m[m
[32m+[m[32m                  * Nosso Número com 11 posições[m[41m[m
[32m+[m[32m                  */[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 4)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.NossoNumero.Length > 7)[m[41m[m
[32m+[m[32m                        throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 7 de posições para o nosso número [{1}]", boleto.Carteira, boleto.NossoNumero));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 7));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 11);[m[41m[m
