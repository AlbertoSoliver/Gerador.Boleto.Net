[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.IO;[m[41m[m
[32m+[m[32musing System.Collections.Generic;[m[41m[m
[32m+[m[32musing System.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public class ArquivoRetornoCNAB240 : AbstractArquivoRetorno, IArquivoRetorno[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private Stream _streamArquivo;[m[41m[m
[32m+[m[32m        private string _caminhoArquivo;[m[41m[m
[32m+[m[32m        private List<DetalheRetornoCNAB240> _listaDetalhes = new List<DetalheRetornoCNAB240>();[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Propriedades[m[41m[m
[32m+[m[32m        public string CaminhoArquivo[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _caminhoArquivo; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        public Stream StreamArquivo[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _streamArquivo; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        public List<DetalheRetornoCNAB240> ListaDetalhes[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _listaDetalhes; }[m[41m[m
[32m+[m[32m            set { _listaDetalhes = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion Propriedades[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Construtores[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public ArquivoRetornoCNAB240()[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m[32m            this.TipoArquivo = TipoArquivo.CNAB240;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public ArquivoRetornoCNAB240(Stream streamArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.TipoArquivo = TipoArquivo.CNAB240;[m[41m[m
[32m+[m[32m            _streamArquivo = streamArquivo;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public ArquivoRetornoCNAB240(string caminhoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.TipoArquivo = TipoArquivo.CNAB240;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            _streamArquivo = new StreamReader(caminhoArquivo).BaseStream;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de instância[m[41m[m
[32m+[m[32m        public void LerArquivoRetorno(IBanco banco)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            LerArquivoRetorno(banco, StreamArquivo);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void LerArquivoRetorno(IBanco banco, Stream arquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StreamReader stream = new StreamReader(arquivo, System.Text.Encoding.UTF8);[m[41m[m
[32m+[m[32m                string linha = "";[m[41m[m
[32m+[m[32m                DetalheRetornoCNAB240 detalheRetorno = new DetalheRetornoCNAB240();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Lendo o arquivo[m[41m[m
[32m+[m[32m                linha = stream.ReadLine();[m[41m[m
[32m+[m[32m                OnLinhaLida(null, linha, EnumTipodeLinhaLida.HeaderDeArquivo);[m[41m[m
[32m+[m[32m                detalheRetorno.HeaderArquivo.LerHeaderDeArquivoCNAB240(linha);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Próxima linha (DETALHE)[m[41m[m
[32m+[m[32m                linha = stream.ReadLine();[m[41m[m
[32m+[m[32m                OnLinhaLida(null, linha, EnumTipodeLinhaLida.HeaderDeLote);[m[41m[m
[32m+[m[32m                linha = stream.ReadLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                while (linha.Substring(7, 1) == "3")[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (linha.Substring(13, 1) == "W")[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        OnLinhaLida(detalheRetorno, linha, EnumTipodeLinhaLida.DetalheSegmentoW);[m[41m[m
[32m+[m[32m                        detalheRetorno.SegmentoW.LerDetalheSegmentoWRetornoCNAB240(linha);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        OnLinhaLida(detalheRetorno, linha, EnumTipodeLinhaLida.DetalheSegmentoT);[m[41m[m
[32m+[m[32m                        detalheRetorno.SegmentoT.LerDetalheSegmentoTRetornoCNAB240(linha);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        linha = stream.ReadLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        OnLinhaLida(detalheRetorno, linha, EnumTipodeLinhaLida.DetalheSegmentoU);[m[41m[m
[32m+[m[32m                        detalheRetorno.SegmentoU.LerDetalheSegmentoURetornoCNAB240(linha);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    ListaDetalhes.Add(detalheRetorno);[m[41m[m
[32m+[m[32m                    linha = stream.ReadLine();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                OnLinhaLida(null, linha, EnumTipodeLinhaLida.TraillerDeLote);[m[41m[m
[32m+[m[32m                linha = stream.ReadLine();[m[41m[m
[32m+[m[32m                OnLinhaLida(null, linha, EnumTipodeLinhaLida.TraillerDeArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                stream.Close();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao ler arquivo.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
