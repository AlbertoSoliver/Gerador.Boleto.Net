[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Collections.Generic;[m[41m[m
[32m+[m[32musing System.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    #region Enumerado[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public enum EnumEspecieDocumento_BancoBrasil[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        Cheque = 1, //CH  CHEQUE[m[41m[m
[32m+[m[32m        DuplicataMercantil = 2, //DM  DUPLICATA MERCANTIL[m[41m[m
[32m+[m[32m        DuplicataMercantilIndicacao = 3, //DMI  DUPLICATA MERCANTIL P/ INDICAÇÃO[m[41m[m
[32m+[m[32m        DuplicataServico = 4, //DS   DUPLICATA DE SERVIÇO[m[41m[m
[32m+[m[32m        DuplicataServicoIndicacao = 5, //DSI   DUPLICATA DE SERVIÇO P/ INDICAÇÃO[m[41m[m
[32m+[m[32m        DuplicataRural = 6, //DR  DUPLICATA RURAL[m[41m[m
[32m+[m[32m        LetraCambio = 7, //LC  LETRA DE CAMBIO[m[41m[m
[32m+[m[32m        NotaCreditoComercial = 8, //NCC  NOTA DE CRÉDITO COMERCIAL[m[41m[m
[32m+[m[32m        NotaCreditoExportacao = 9, //NCE  NOTA DE CRÉDITO A EXPORTAÇÃO[m[41m[m
[32m+[m[32m        NotaCreditoIndustrial = 10, //NCI  NOTA DE CRÉDITO INDUSTRIAL[m[41m[m
[32m+[m[32m        NotaCreditoRural = 11, //NCR  NOTA DE CRÉDITO RURAL[m[41m[m
[32m+[m[32m        NotaPromissoria = 12, //NP  NOTA PROMISSÓRIA[m[41m[m
[32m+[m[32m        NotaPromissoriaRural = 13, //NPR NOTA PROMISSÓRIA RURAL[m[41m[m
[32m+[m[32m        TriplicataMercantil = 14, //TM  TRIPLICATA MERCANTIL[m[41m[m
[32m+[m[32m        TriplicataServico = 15, //TS   TRIPLICATA DE SERVIÇO[m[41m[m
[32m+[m[32m        NotaSeguro = 16, //NS  NOTA DE SEGURO[m[41m[m
[32m+[m[32m        Recibo = 17, //RC  RECIBO[m[41m[m
[32m+[m[32m        Fatura = 18, //FAT  FATURA[m[41m[m
[32m+[m[32m        NotaDebito = 19, //ND   NOTA DE DÉBITO[m[41m[m
[32m+[m[32m        ApoliceSeguro = 20, //AP   APÓLICE DE SEGURO[m[41m[m
[32m+[m[32m        MensalidadeEscolar = 21, //ME  MENSALIDADE ESCOLAR[m[41m[m
[32m+[m[32m        ParcelaConsorcio = 22, //PC   PARCELA DE CONSÓRCIO[m[41m[m
[32m+[m[32m        Outros = 23 //OUTROS[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    #endregion[m[41m[m
[32m+[m[41m[m
