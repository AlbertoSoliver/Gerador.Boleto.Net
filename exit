[32m+[m[32m{[m[41m[m
[32m+[m[32m    internal class ArquivoRemessaCNAB240 : AbstractArquivoRemessa, IArquivoRemessa[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Construtores[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public ArquivoRemessaCNAB240()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.TipoArquivo = TipoArquivo.CNAB240;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de instância[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void GerarArquivoRemessa(string numeroConvenio, IBanco banco, Cedente cedente, Boletos boletos, Stream arquivo, int numeroArquivoRemessa)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                int numeroRegistro = 3;[m[41m[m
[32m+[m[32m                int numeroRegistroDetalhe = 1;[m[41m[m
[32m+[m[32m                string strline;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                StreamWriter incluiLinha = new StreamWriter(arquivo);[m[41m[m
[32m+[m[32m                strline = banco.GerarHeaderRemessa(cedente, TipoArquivo.CNAB240);[m[41m[m
[32m+[m[32m                incluiLinha.WriteLine(strline);[m[41m[m
[32m+[m[32m                OnLinhaGerada(null, strline, EnumTipodeLinha.HeaderDeArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                strline = banco.GerarHeaderLoteRemessa(numeroConvenio, cedente, numeroArquivoRemessa, TipoArquivo.CNAB240);[m[41m[m
[32m+[m[32m                incluiLinha.WriteLine(strline);[m[41m[m
[32m+[m[32m                OnLinhaGerada(null, strline, EnumTipodeLinha.HeaderDeLote);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (banco.Codigo != 341)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    foreach (Boleto boleto in boletos)[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        boleto.Banco = banco;[m[41m[m
[32m+[m[32m                        strline = boleto.Banco.GerarDetalheSegmentoPRemessa(boleto, numeroRegistroDetalhe, numeroConvenio);[m[41m[m
[32m+[m[32m                        incluiLinha.WriteLine(strline);[m[41m[m
[32m+[m[32m                        OnLinhaGerada(boleto, strline, EnumTipodeLinha.DetalheSegmentoP);[m[41m[m
[32m+[m[32m                        numeroRegistro++;[m[41m[m
[32m+[m[32m                        numeroRegistroDetalhe++;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        strline = boleto.Banco.GerarDetalheSegmentoQRemessa(boleto, numeroRegistroDetalhe, TipoArquivo.CNAB240);[m[41m[m
[32m+[m[32m                        incluiLinha.WriteLine(strline);[m[41m[m
[32m+[m[32m                        OnLinhaGerada(boleto, strline, EnumTipodeLinha.DetalheSegmentoQ);[m[41m[m
[32m+[m[32m                        numeroRegistro++;[m[41m[m
[32m+[m[32m                        numeroRegistroDetalhe++;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        if (boleto.ValorMulta > 0)[m[41m[m
[32m+[m[32m                        {[m[41m[m
[32m+[m[32m                            strline = boleto.Banco.GerarDetalheSegmentoRRemessa(boleto, numeroRegistroDetalhe, TipoArquivo.CNAB240);[m[41m[m
[32m+[m[32m                            incluiLinha.WriteLine(strline);[m[41m[m
[32m+[m[32m                            OnLinhaGerada(boleto, strline, EnumTipodeLinha.DetalheSegmentoR);[m[41m[m
[32m+[m[32m                            numeroRegistro++;[m[41m[m
[32m+[m[32m                            numeroRegistroDetalhe++;[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    numeroRegistro--;[m[41m[m
[32m+[m[32m                    strline = banco.GerarTrailerLoteRemessa(numeroRegistro);[m[41m[m
[32m+[m[32m                    incluiLinha.WriteLine(strline);[m[41m[m
[32m+[m[32m                    OnLinhaGerada(null, strline, EnumTipodeLinha.TraillerDeLote);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    numeroRegistro++;[m[41m[m
[32m+[m[32m                    numeroRegistro++;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    strline = banco.GerarTrailerArquivoRemessa(numeroRegistro);[m[41m[m
[32m+[m[32m                    incluiLinha.WriteLine(strline);[m[41m[m
[32m+[m[32m                    OnLinhaGerada(null, strline, EnumTipodeLinha.TraillerDeArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    incluiLinha.Close();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    foreach (Boleto boleto in boletos)[m[41m[m
