[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (value == null)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    throw new ArgumentException("Erro. O CPF/CNPJ do cliente não pode ser nulo.");[m[41m[m
