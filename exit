[32m+[m[32m        {[m[41m[m
[32m+[m[32m            EspeciesDocumento especiesDocumento = new EspeciesDocumento();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            foreach (EnumEspecieDocumento_Caixa item in Enum.GetValues(typeof(EnumEspecieDocumento_Caixa)))[m[41m[m
[32m+[m[32m                especiesDocumento.Add(new EspecieDocumento_Caixa((int)item));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return especiesDocumento;[m[41m[m
[32m+[m[32m        }[m[41m[m
