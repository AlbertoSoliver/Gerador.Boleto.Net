[32m+[m[32m        }[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        private void button1_Click(object sender, EventArgs e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                ChamarBanco();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message, "Alerta de Erro", MessageBoxButtons.OK, MessageBoxIcon.Error);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void ChamarBanco()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (radioButtonItau.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonItau.Tag);[m[41m[m
[32m+[m[32m                else if (radioButtonSudameris.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonSudameris.Tag);[m[41m[m
[32m+[m[32m                else if (radioButtonSafra.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonSafra.Tag);[m[41m[m
[32m+[m[32m                else if (radioButtonReal.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonReal.Tag);[m[41m[m
[32m+[m[32m                else if (radioButtonHsbc.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonHsbc.Tag);[m[41m[m
[32m+[m[32m                else if (radioButtonBancoBrasil.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonBancoBrasil.Tag);[m[41m[m
[32m+[m[32m                else if (radioButtonBradesco.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonBradesco.Tag);[m[41m[m
[32m+[m[32m                else if (radioButtonCaixa.Checked)[m[41m[m
[32m+[m[32m                    CodigoBanco = Convert.ToInt16(radioButtonCaixa.Tag);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                BackgroundWorker backgroundWorker = new BackgroundWorker();[m[41m[m
[32m+[m[32m                backgroundWorker.DoWork += new DoWorkEventHandler(backgroundWorker_DoWork);[m[41m[m
[32m+[m[32m                backgroundWorker.RunWorkerCompleted += new RunWorkerCompletedEventHandler(backgroundWorker_RunWorkerCompleted);[m[41m[m
[32m+[m[32m                backgroundWorker.RunWorkerAsync();[m[41m[m
[32m+[m[32m                _progresso = new Progresso();[m[41m[m
[32m+[m[32m                _progresso.ShowDialog();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message , "Alerta de Erro", MessageBoxButtons.OK, MessageBoxIcon.Error);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
