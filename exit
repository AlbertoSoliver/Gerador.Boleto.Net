[32m+[m[41m[m
[32m+[m[32m        public override void FormataNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            boleto.NossoNumero = string.Format("{0:0000}{1}", boleto.Cedente.Codigo, boleto.NumeroDocumento);[m[41m[m
[32m+[m[32m        }[m[41m[m
