[32m+[m[32m                    m_BrowserHeight = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            public Bitmap GenerateWebSiteThumbnailImage()[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Thread m_thread = new Thread(new ThreadStart(_GenerateWebSiteThumbnailImage));[m[41m[m
[32m+[m[32m                m_thread.SetApartmentState(ApartmentState.STA);[m[41m[m
[32m+[m[32m                m_thread.Start();[m[41m[m
[32m+[m[32m                m_thread.Join();[m[41m[m
[32m+[m[32m                return m_Bitmap;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            private void _GenerateWebSiteThumbnailImage()[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                WebBrowser m_WebBrowser = new WebBrowser();[m[41m[m
[32m+[m[32m                m_WebBrowser.ScrollBarsEnabled = false;[m[41m[m
[32m+[m[32m                m_WebBrowser.Navigate(m_Url);[m[41m[m
[32m+[m[32m                m_WebBrowser.DocumentCompleted += new WebBrowserDocumentCompletedEventHandler(WebBrowser_DocumentCompleted);[m[41m[m
[32m+[m[32m                while (m_WebBrowser.ReadyState != WebBrowserReadyState.Complete)[m[41m[m
[32m+[m[32m                    Application.DoEvents();[m[41m[m
[32m+[m[32m                m_WebBrowser.Dispose();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            private void WebBrowser_DocumentCompleted(object sender, WebBrowserDocumentCompletedEventArgs e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                WebBrowser m_WebBrowser = (WebBrowser)sender;[m[41m[m
[32m+[m[32m                m_WebBrowser.ClientSize = new Size(this.m_BrowserWidth, this.m_BrowserHeight);[m[41m[m
[32m+[m[32m                m_WebBrowser.ScrollBarsEnabled = false;[m[41m[m
[32m+[m[32m                m_Bitmap = new Bitmap(m_WebBrowser.Bounds.Width, m_WebBrowser.Bounds.Height);[m[41m[m
[32m+[m[32m                m_WebBrowser.BringToFront();[m[41m[m
[32m+[m[32m                m_WebBrowser.DrawToBitmap(m_Bitmap, m_WebBrowser.Bounds);[m[41m[m
[32m+[m[32m                m_Bitmap = (Bitmap)m_Bitmap.GetThumbnailImage(m_ThumbnailWidth, m_ThumbnailHeight, null, IntPtr.Zero);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
