[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Collections;[m[41m[m
[32m+[m[32musing System.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    #region Enumerado[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public enum EnumCarteiras_BancoBrasil[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        CobrancaSimples = 1,[m[41m[m
[32m+[m[32m        CobrancaVinculada = 2,[m[41m[m
[32m+[m[32m        CobrancaCaucionada = 3,[m[41m[m
[32m+[m[32m        CobrancaDescontada = 4,[m[41m[m
[32m+[m[32m        CobrançaDiretaEspecialCarteira17 = 7,[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    #endregion[m[41m [m
[32m+[m[41m[m
[32m+[m[32m    public class Carteira_BancoBrasil: AbstractCarteira, ICarteira[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Construtores[m[41m [m
[32m+[m[41m[m
[32m+[m		[32mpublic Carteira_BancoBrasil()[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mtry[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m[32m                this.Banco = new Banco(1);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mcatch (Exception ex)[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao carregar objeto", ex);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public Carteira_BancoBrasil(int carteira)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.carregar(carteira);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao carregar objeto", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m#endregion[m[41m [m
[32m+[m[41m[m
[32m+[m[32m        #region Metodos Privados[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void carregar(int carteira)[m[41m[m
