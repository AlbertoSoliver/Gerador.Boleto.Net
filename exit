[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Drawing;[m[41m[m
[32m+[m[32musing System.Drawing.Imaging;[m[41m[m
[32m+[m[32musing System.IO;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public abstract class BarCodeBase[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        #region Variables[m[41m[m
[32m+[m[32m        private string _code;[m[41m[m
[32m+[m[32m        private int _height;[m[41m[m
[32m+[m[32m        private int _digits;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private int _thin;[m[41m[m
[32m+[m[32m        private int _full;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected int xPos = 0;[m[41m[m
[32m+[m[32m        protected int yPos = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string _contenttype;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected Brush BLACK = Brushes.Black;[m[41m[m
[32m+[m[32m        protected Brush WHITE = Brushes.White;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Property[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// The Barcode.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string Code[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return _code;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return "";[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _code = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _code = null;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// The width of the thin bar (pixels).[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public int Width[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return _thin;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return 1;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    int temp = value;[m[41m[m
[32m+[m[32m                    _thin = temp;[m[41m[m
[32m+[m[32m                    //					_half = temp * 2;[m[41m[m
[32m+[m[32m                    _full = temp * 3;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    int temp = 1;[m[41m[m
[32m+[m[32m                    _thin = temp;[m[41m[m
[32m+[m[32m                    //					_half = temp * 2;[m[41m[m
[32m+[m[32m                    _full = temp * 3;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// The Height of barcode (pixels).[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public int Height[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return _height;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return 15;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _height = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _height = 1;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Number of digits of the barcode.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public int Digits[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return _digits;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return 0;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _digits = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _digits = 0;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Content type of code. Default: image/jpeg[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string ContentType[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (_contenttype == null)[m[41m[m
[32m+[m[32m                        return "image/jpeg";[m[41m[m
[32m+[m[32m                    return _contenttype;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return "image/jpeg";[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _contenttype = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _contenttype = "image/jpeg";[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        protected int Thin[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return _thin;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return 1;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        protected int Full[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                try[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return _full;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                catch[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return 3;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[32m        protected virtual byte[] toByte(Bitmap bitmap)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            MemoryStream mstream = new MemoryStream();[m[41m[m
[32m+[m[32m            ImageCodecInfo myImageCodecInfo = GetEncoderInfo(ContentType);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            EncoderParameter myEncoderParameter0 = new EncoderParameter(Encoder.Quality, (long)100);[m[41m[m
[32m+[m[32m            EncoderParameters myEncoderParameters = new EncoderParameters(1);[m[41m[m
[32m+[m[32m            myEncoderParameters.Param[0] = myEncoderParameter0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            bitmap.Save(mstream, myImageCodecInfo, myEncoderParameters);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return mstream.GetBuffer();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        private static ImageCodecInfo GetEncoderInfo(string mimeType)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int j;[m[41m[m
[32m+[m[32m            ImageCodecInfo[] encoders;[m[41m[m
[32m+[m[32m            encoders = ImageCodecInfo.GetImageEncoders();[m[41m[m
[32m+[m[32m            for (j = 0; j < encoders.Length; ++j)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (encoders[j].MimeType == mimeType)[m[41m[m
[32m+[m[32m                    return encoders[j];[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return null;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        protected virtual void DrawPattern(ref Graphics g, string Pattern)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int tempWidth;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = 0; i < Pattern.Length; i++)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (Pattern[i] == '0')[m[41m[m
[32m+[m[32m                    tempWidth = _thin;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    tempWidth = _full;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (i % 2 == 0)[m[41m[m
[32m+[m[32m                    g.FillRectangle(BLACK, xPos, yPos, tempWidth, _height);[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    g.FillRectangle(WHITE, xPos, yPos, tempWidth, _height);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                xPos += tempWidth;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[1mdiff --git a/Boleto.Net/Util/C2of5i.cs b/Boleto.Net/Util/C2of5i.cs[m
[1mnew file mode 100644[m
[1mindex 0000000..763050b[m
[1m--- /dev/null[m
[1m+++ b/Boleto.Net/Util/C2of5i.cs[m
[36m@@ -0,0 +1,140 @@[m
[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Drawing;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public class C2of5i : BarCodeBase[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        #region variables[m[41m[m
[32m+[m[32m        private string[] cPattern = new string[100];[m[41m[m
[32m+[m[32m        private const string START = "0000";[m[41m[m
[32m+[m[32m        private const string STOP = "1000";[m[41m[m
[32m+[m[32m        private Bitmap bitmap;[m[41m[m
[32m+[m[32m        private Graphics g;[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Constructor[m[41m[m
[32m+[m[32m        public C2of5i()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Code 2 of 5 intrelaced Constructor[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="Code">The string that contents the numeric code</param>[m[41m[m
[32m+[m[32m        /// <param name="BarWidth">The Width of each bar</param>[m[41m[m
[32m+[m[32m        /// <param name="Height">The Height of each bar</param>[m[41m[m
[32m+[m[32m        public C2of5i(string Code, int BarWidth, int Height)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.Code = Code;[m[41m[m
[32m+[m[32m            this.Height = Height;[m[41m[m
[32m+[m[32m            this.Width = BarWidth;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Code 2 of 5 intrelaced Constructor[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="Code">The string that contents the numeric code</param>[m[41m[m
[32m+[m[32m        /// <param name="BarWidth">The Width of each bar</param>[m[41m[m
[32m+[m[32m        /// <param name="Height">The Height of each bar</param>[m[41m[m
[32m+[m[32m        /// <param name="Digits">Number of digits of code</param>[m[41m[m
[32m+[m[32m        public C2of5i(string Code, int BarWidth, int Height, int Digits)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.Code = Code;[m[41m[m
[32m+[m[32m            this.Height = Height;[m[41m[m
[32m+[m[32m            this.Width = BarWidth;[m[41m[m
[32m+[m[32m            this.Digits = Digits;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void FillPatern()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int f;[m[41m[m
[32m+[m[32m            string strTemp;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (cPattern[0] == null)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                cPattern[0] = "00110";[m[41m[m
[32m+[m[32m                cPattern[1] = "10001";[m[41m[m
[32m+[m[32m                cPattern[2] = "01001";[m[41m[m
[32m+[m[32m                cPattern[3] = "11000";[m[41m[m
[32m+[m[32m                cPattern[4] = "00101";[m[41m[m
[32m+[m[32m                cPattern[5] = "10100";[m[41m[m
[32m+[m[32m                cPattern[6] = "01100";[m[41m[m
[32m+[m[32m                cPattern[7] = "00011";[m[41m[m
[32m+[m[32m                cPattern[8] = "10010";[m[41m[m
[32m+[m[32m                cPattern[9] = "01010";[m[41m[m
[32m+[m[32m                //Create a draw pattern for each char from 0 to 99[m[41m[m
[32m+[m[32m                for (int f1 = 9; f1 >= 0; f1--)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    for (int f2 = 9; f2 >= 0; f2--)[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        f = f1 * 10 + f2;[m[41m[m
[32m+[m[32m                        strTemp = "";[m[41m[m
[32m+[m[32m                        for (int i = 0; i < 5; i++)[m[41m[m
[32m+[m[32m                        {[m[41m[m
[32m+[m[32m                            strTemp += cPattern[f1][i].ToString() + cPattern[f2][i].ToString();[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                        cPattern[f] = strTemp;[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Generate the Bitmap of Barcode.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns>Return System.Drawing.Bitmap</returns>[m[41m[m
[32m+[m[32m        public Bitmap ToBitmap()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int i;[m[41m[m
[32m+[m[32m            string ftemp;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            xPos = 0;[m[41m[m
[32m+[m[32m            yPos = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (this.Digits == 0)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.Digits = this.Code.Length;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (this.Digits % 2 > 0) this.Digits++;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            while (this.Code.Length < this.Digits || this.Code.Length % 2 > 0)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.Code = "0" + this.Code;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int _width = (2 * Full + 3 * Thin) * (Digits) + 7 * Thin + Full;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            bitmap = new Bitmap(_width, Height);[m[41m[m
[32m+[m[32m            g = Graphics.FromImage(bitmap);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Start Pattern[m[41m[m
[32m+[m[32m            DrawPattern(ref g, START);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Draw code[m[41m[m
[32m+[m[32m            this.FillPatern();[m[41m[m
[32m+[m[32m            while (this.Code.Length > 0)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                i = Convert.ToInt32(this.Code.Substring(0, 2));[m[41m[m
[32m+[m[32m                if (this.Code.Length > 2)[m[41m[m
[32m+[m[32m                    this.Code = this.Code.Substring(2, this.Code.Length - 2);[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    this.Code = "";[m[41m[m
[32m+[m[32m                ftemp = cPattern[i];[m[41m[m
[32m+[m[32m                DrawPattern(ref g, ftemp);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Stop Patern[m[41m[m
[32m+[m[32m            DrawPattern(ref g, STOP);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return bitmap;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Returns the byte array of Barcode[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns>byte[]</returns>[m[41m[m
[32m+[m[32m        public byte[] ToByte()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return base.toByte(ToBitmap());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
