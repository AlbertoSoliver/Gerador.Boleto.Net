[32m+[m[32m        [Browsable(true), Description("Gerar arquivo de remessa")][m[41m[m
[32m+[m[32m        public bool GerarArquivoRemessa[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return Utils.ToBool(ViewState["GerarArquivoRemessa"]); }[m[41m[m
[32m+[m[32m            set { ViewState["GerarArquivoRemessa"] = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m [m
[32m+[m[32m        /// Mostra o termo "Contra Apresentação" na data de vencimento do boleto[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public bool MostrarContraApresentacaoNaDataVencimento[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return Utils.ToBool(ViewState["MCANDV"]); }[m[41m[m
[32m+[m[32m            set { ViewState["MCANDV"] = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion Propriedades[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m [m
[32m+[m[32m        /// Retorna o campo para a 1 linha da instrucao.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public List<IInstrucao> Instrucoes[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return _instrucoes;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                _instrucoes = value;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
