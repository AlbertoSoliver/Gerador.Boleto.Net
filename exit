[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string CPFCNPJ[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m                   [m
[32m+[m[32m                //return _cpfcnpj.Replace(".", "").Replace("-", "").Replace("/", "");[m[41m[m
