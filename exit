[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    internal enum TipoOcorrencia[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m      //Ocorrências para arquivo remessa[m[41m[m
[32m+[m[32m      RemRegistrar, //Registrar o título no banco[m[41m[m
[32m+[m[32m      RemBaixar, //Baixar o título no banco[m[41m[m
[32m+[m[32m      RemDebitarEmConta,[m[41m[m
[32m+[m[32m      RemConcederAbatimento,[m[41m[m
