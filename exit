[32m+[m[32m        public AbstractArquivoRemessa(TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            switch (tipoArquivo)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                    _arquivoRemessa = new ArquivoRemessaCNAB240();[m[41m[m
[32m+[m[32m                    _arquivoRemessa.LinhaDeArquivoGerada += new EventHandler<LinhaDeArquivoGeradaArgs>(_arquivoRemessa_LinhaDeArquivoGerada);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                    _arquivoRemessa = new ArquivoRemessaCNAB400();[m[41m[m
[32m+[m[32m                    _arquivoRemessa.LinhaDeArquivoGerada += new EventHandler<LinhaDeArquivoGeradaArgs>(_arquivoRemessa_LinhaDeArquivoGerada);[m[41m[m
