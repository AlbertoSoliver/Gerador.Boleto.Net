[32m+[m[32m        #endregion Propriedades[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static string UrlLogo(int banco)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            Page page = System.Web.HttpContext.Current.CurrentHandler as Page;[m[41m[m
[32m+[m[32m            return page.ClientScript.GetWebResourceUrl(typeof(BoletoBancario), "BoletoNet.Imagens." + Utils.FormatCode(banco.ToString(), 3) + ".jpg");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Override[m[41m[m
[32m+[m[32m        protected override void OnPreRender(EventArgs e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string alias = "BoletoNet.BoletoImpressao.BoletoNet.css";[m[41m[m
[32m+[m[32m            string csslink = "<link rel=\"stylesheet\" type=\"text/css\" href=\"" +[m[41m[m
[32m+[m[32m                Page.ClientScript.GetWebResourceUrl(typeof(BoletoBancario), alias) + "\" />";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LiteralControl include = new LiteralControl(csslink);[m[41m[m
[32m+[m[32m            Page.Header.Controls.Add(include);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            base.OnPreRender(e);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected override void OnLoad(EventArgs e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        [System.Security.Permissions.PermissionSet(System.Security.Permissions.SecurityAction.Demand, Name = "Execution")][m[41m[m
[32m+[m[32m        protected override void Render(HtmlTextWriter output)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (_ibanco == null)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                output.Write("<b>Erro gerando o boleto bancário: faltou definir o banco.</b>");[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            string urlImagemLogo = Page.ClientScript.GetWebResourceUrl(typeof(BoletoBancario), "BoletoNet.Imagens." + Utils.FormatCode(_ibanco.Codigo.ToString(), 3) + ".jpg");[m[41m[m
[32m+[m[32m            string urlImagemBarra = Page.ClientScript.GetWebResourceUrl(typeof(BoletoBancario), "BoletoNet.Imagens.barra.gif");[m[41m[m
[32m+[m[32m            //string urlImagemBarraInterna = Page.ClientScript.GetWebResourceUrl(typeof(BoletoBancario), "BoletoNet.Imagens.barrainterna.gif");[m[41m[m
[32m+[m[32m            //string urlImagemCorte = Page.ClientScript.GetWebResourceUrl(typeof(BoletoBancario), "BoletoNet.Imagens.corte.gif");[m[41m[m
[32m+[m[32m            //string urlImagemPonto = Page.ClientScript.GetWebResourceUrl(typeof(BoletoBancario), "BoletoNet.Imagens.ponto.gif");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui os valores ao html do boleto bancário[m[41m[m
[32m+[m[32m            //output.Write(MontaHtml(urlImagemCorte, urlImagemLogo, urlImagemBarra, urlImagemPonto, urlImagemBarraInterna,[m[41m[m
[32m+[m[32m            //    "<img src=\"ImagemCodigoBarra.ashx?code=" + Boleto.CodigoBarra.Codigo + "\" alt=\"Código de Barras\" />"));[m[41m[m
[32m+[m[32m            output.Write(MontaHtml(urlImagemLogo, urlImagemBarra, "<img src=\"ImagemCodigoBarra.ashx?code=" + Boleto.CodigoBarra.Codigo + "\" alt=\"Código de Barras\" />"));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion Override[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Html[m[41m[m
[32m+[m[32m        public string GeraHtmlInstrucoes()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string titulo = "Instruções de Impressão";[m[41m[m
[32m+[m[32m                string instrucoes = "Imprimir em impressora jato de tinta (ink jet) ou laser em qualidade normal. (Não use modo econômico).<br>Utilize folha A4 (210 x 297 mm) ou Carta (216 x 279 mm) - Corte na linha indicada<br>";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                html.Append(Html.Instrucoes);[m[41m[m
[32m+[m[32m                html.Append("<br />");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return html.ToString()[m[41m[m
[32m+[m[32m                    .Replace("@TITULO", titulo)[m[41m[m
[32m+[m[32m                    .Replace("@INSTRUCAO", instrucoes);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a execução da transação.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string GeraHtmlCarne(string telefone, string htmlBoleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            html.Append(Html.Carne);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return html.ToString()[m[41m[m
[32m+[m[32m                .Replace("@TELEFONE", telefone)[m[41m[m
[32m+[m[32m                .Replace("#BOLETO#", htmlBoleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GeraHtmlReciboSacado()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboSacadoParte1);[m[41m[m
[32m+[m[32m                html.Append("<br />");[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboSacadoParte2);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboSacadoParte3);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboSacadoParte4);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboSacadoParte5);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboSacadoParte6);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboSacadoParte7);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (Instrucoes.Count == 0)[m[41m[m
[32m+[m[32m                    html.Append(Html.ReciboSacadoParte8);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                MontaInstrucoes();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return html.ToString().Replace("@INSTRUCOES", _instrucoesHtml);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a execução da transação.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GeraHtmlReciboCedente()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte1);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte2);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte3);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte4);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte5);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte6);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte7);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte8);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte9);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte10);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte11);[m[41m[m
[32m+[m[32m                html.Append(Html.ReciboCedenteParte12);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Para Banco Itaú, o texto "(Texto de responsabilidade do cedente)" deve ser[m[41m[m
[32m+[m[32m                //"(Todas as informações deste bloqueto são de exclusiva responsabilidade do cedente)".[m[41m[m
[32m+[m[32m                if (Boleto.Banco.Codigo == 341)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    html.Replace("(Texto de responsabilidade do cedente)", "(Todas as informações deste bloqueto são de exclusiva responsabilidade do cedente)");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Para carteira "18-019" do Banco do Brasil, a ficha de compensação não possui código da carteira[m[41m[m
[32m+[m[32m                //na formatação do campo.[m[41m[m
[32m+[m[32m                if (Boleto.Banco.Codigo == 1 & Boleto.Carteira.Equals("18-019"))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    html.Replace("Carteira /", "");[m[41m[m
[32m+[m[32m                    html.Replace("@NOSSONUMERO", "@NOSSONUMEROBB");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                MontaInstrucoes();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return html.ToString().Replace("@INSTRUCOES", _instrucoesHtml);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro na execução da transação.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string HtmlComprovanteEntrega[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                html.Append(Html.ComprovanteEntrega1);[m[41m[m
[32m+[m[32m                html.Append(Html.ComprovanteEntrega2);[m[41m[m
[32m+[m[32m                html.Append(Html.ComprovanteEntrega3);[m[41m[m
[32m+[m[32m                html.Append(Html.ComprovanteEntrega4);[m[41m[m
[32m+[m[32m                html.Append(Html.ComprovanteEntrega5);[m[41m[m
[32m+[m[32m                html.Append(Html.ComprovanteEntrega6);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if(MostrarComprovanteEntregaLivre)[m[41m[m
[32m+[m[32m                    html.Append(Html.ComprovanteEntrega71);[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    html.Append(Html.ComprovanteEntrega7);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                html.Append("<br />");[m[41m[m
[32m+[m[32m                return html.ToString();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void MontaInstrucoes()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (string.IsNullOrEmpty(_instrucoesHtml))[m[41m[m
[32m+[m[32m                if (Boleto.Instrucoes.Count > 0)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _instrucoesHtml = string.Empty;[m[41m[m
[32m+[m[32m                    //Flavio(fhlviana@hotmail.com) - retirei a tag <span> de cada instrução por não ser mais necessáras desde que dentro[m[41m[m
[32m+[m[32m                    //da div que contem as instruções a classe cpN se aplica, que é a mesma, em conteudo, da classe cp[m[41m[m
[32m+[m[32m                    foreach (IInstrucao instrucao in Boleto.Instrucoes)[m[41m[m
[32m+[m[32m                        _instrucoesHtml += string.Format("{0}<br />", instrucao.Descricao);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    _instrucoesHtml = Strings.Left(_instrucoesHtml, _instrucoesHtml.Length - 6);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        //private string MontaHtml(string urlImagemCorte, string urlImagemLogo, string urlImagemBarra, string urlImagemPonto, string urlImagemBarraInterna, string imagemCodigoBarras)[m[41m[m
[32m+[m[32m        private string MontaHtml(string urlImagemLogo, string urlImagemBarra, string imagemCodigoBarras)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Oculta o cabeçalho das instruções do boleto[m[41m[m
[32m+[m[32m            if (!OcultarInstrucoes)[m[41m[m
[32m+[m[32m                html.Append(GeraHtmlInstrucoes());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (!FormatoCarne)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                //Mostra o comprovante de entrega[m[41m[m
[32m+[m[32m                if (MostrarComprovanteEntrega | MostrarComprovanteEntregaLivre)[m[41m[m
[32m+[m[32m                    html.Append(HtmlComprovanteEntrega);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Oculta o recibo do sacabo do boleto[m[41m[m
[32m+[m[32m                if (!OcultarReciboSacado)[m[41m[m
[32m+[m[32m                    html.Append(GeraHtmlReciboSacado());[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string sacado = "";[m[41m[m
[32m+[m[32m            //Flavio(fhlviana@hotmail.com) - adicionei a possibilidade de o boleto não ter, necessáriamente, que informar o CPF ou CNPJ do sacado.[m[41m[m
[32m+[m[32m            //Formata o CPF/CNPJ(se houver) e o Nome do Sacado para apresentação[m[41m[m
[32m+[m[32m            if (Sacado.CPFCNPJ == string.Empty)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                sacado = Sacado.Nome;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (Sacado.CPFCNPJ.Length <= 11)[m[41m[m
[32m+[m[32m                    sacado = string.Format("{0}  CPF: {1}", Sacado.Nome, Utils.FormataCPF(Sacado.CPFCNPJ));[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    sacado = string.Format("{0}  CNPJ: {1}", Sacado.Nome, Utils.FormataCNPJ(Sacado.CPFCNPJ));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            String infoSacado = Sacado.InformacoesSacado.GeraHTML(false);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Caso não oculte o Endereço do Sacado,[m[41m[m
[32m+[m[32m            if (!OcultarEnderecoSacado)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                String enderecoSacado = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (Sacado.Endereco.CEP == String.Empty)[m[41m[m
[32m+[m[32m                    enderecoSacado = string.Format("{0} - {1}/{2}", Sacado.Endereco.Bairro, Sacado.Endereco.Cidade, Sacado.Endereco.UF);[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    enderecoSacado = string.Format("{0} - {1}/{2} - CEP: {3}", Sacado.Endereco.Bairro,[m[41m[m
[32m+[m[32m                    Sacado.Endereco.Cidade, Sacado.Endereco.UF, Utils.FormataCEP(Sacado.Endereco.CEP));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (Sacado.Endereco.End != string.Empty && enderecoSacado != string.Empty)[m[41m[m
[32m+[m[32m                    if (infoSacado == string.Empty)[m[41m[m
[32m+[m[32m                        infoSacado += InfoSacado.Render(Sacado.Endereco.End, enderecoSacado, false);[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        infoSacado += InfoSacado.Render(Sacado.Endereco.End, enderecoSacado, true);[m[41m[m
[32m+[m[32m                //"Informações do Sacado" foi introduzido para possibilitar que o boleto na informe somente o endereço do sacado[m[41m[m
[32m+[m[32m                //como em outras situaçoes onde se imprime matriculas, codigos e etc, sobre o sacado.[m[41m[m
[32m+[m[32m                //Sendo assim o endereço do sacado passa a ser uma Informaçao do Sacado que é adicionada no momento da renderização[m[41m[m
[32m+[m[32m                //de acordo com a flag "OcultarEnderecoSacado"[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string agenciaConta = Utils.FormataAgenciaConta(Cedente.ContaBancaria.Agencia, Cedente.ContaBancaria.DigitoAgencia, Cedente.ContaBancaria.Conta, Cedente.ContaBancaria.DigitoConta);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Trecho adicionado por Fabrício Nogueira de Almeida :fna - fnalmeida@gmail.com - 09/12/2008[m[41m[m
[32m+[m[32m            /* Esse código foi inserido pq no campo Agência/Cod Cedente, estava sendo impresso sempre a agência / número da conta[m[41m[m
[32m+[m[32m             * No boleto da caixa que eu fiz, coloquei no método validarBoleto um trecho para calcular o dígito do cedente, e adicionei esse atributo na classe cedente[m[41m[m
[32m+[m[32m             * O trecho abaixo testa se esse digito foi calculado, se foi insere no campo Agencia/Cod Cedente, a agência e o código com seu digito[m[41m[m
[32m+[m[32m             * caso contrário mostra a agência / conta, como era anteriormente.[m[41m[m
[32m+[m[32m             * Com esse código ele ira atender as necessidades do boleto caixa e não afetará os demais[m[41m[m
[32m+[m[32m             * Caso queira que apareça o Agência/cod. cedente para outros boletos, basta calcular e setar o digito, como foi feito no boleto Caixa[m[41m [m
[32m+[m[32m             */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string agenciaCodigoCedente;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (!Cedente.DigitoCedente.Equals(-1))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (!Cedente.ContaBancaria.OperacaConta.Equals(""))[m[41m[m
[32m+[m[32m                    agenciaCodigoCedente = string.Format("{0}/{1}.{2}-{3}", Cedente.ContaBancaria.Agencia, Cedente.ContaBancaria.OperacaConta, Utils.FormatCode(Cedente.Codigo.ToString(), 6), Cedente.DigitoCedente.ToString());[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    agenciaCodigoCedente = string.Format("{0}/{1}-{2}", Cedente.ContaBancaria.Agencia, Utils.FormatCode(Cedente.Codigo.ToString(), 6), Cedente.DigitoCedente.ToString());[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                agenciaCodigoCedente = agenciaConta;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (!FormatoCarne)[m[41m[m
[32m+[m[32m                html.Append(GeraHtmlReciboCedente());[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                html.Append(GeraHtmlCarne("", GeraHtmlReciboCedente()));[m[41m [m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string dataVencimento = Boleto.DataVencimento.ToString("dd/MM/yyyy");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (MostrarContraApresentacaoNaDataVencimento)[m[41m[m
[32m+[m[32m                dataVencimento = "Contra Apresentação";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return html[m[41m[m
[32m+[m[32m                .Replace("@CODIGOBANCO", Utils.FormatCode(_ibanco.Codigo.ToString(), 3))[m[41m[m
[32m+[m[32m                .Replace("@DIGITOBANCO", _ibanco.Digito.ToString())[m[41m[m
[32m+[m[32m                //.Replace("@URLIMAGEMBARRAINTERNA", urlImagemBarraInterna)[m[41m[m
[32m+[m[32m                //.Replace("@URLIMAGEMCORTE", urlImagemCorte)[m[41m[m
[32m+[m[32m                //.Replace("@URLIMAGEMPONTO", urlImagemPonto)[m[41m[m
[32m+[m[32m                .Replace("@URLIMAGEMLOGO", urlImagemLogo)[m[41m[m
[32m+[m[32m                .Replace("@URLIMAGEMBARRA", urlImagemBarra)[m[41m[m
[32m+[m[32m                .Replace("@LINHADIGITAVEL", Boleto.CodigoBarra.LinhaDigitavel)[m[41m[m
[32m+[m[32m                .Replace("@LOCALPAGAMENTO", Boleto.LocalPagamento)[m[41m[m
[32m+[m[32m                .Replace("@DATAVENCIMENTO", dataVencimento)[m[41m[m
[32m+[m[32m                .Replace("@CEDENTE", Cedente.Nome)[m[41m[m
[32m+[m[32m                .Replace("@DATADOCUMENTO", Boleto.DataDocumento.ToString("dd/MM/yyyy"))[m[41m[m
[32m+[m[32m                .Replace("@NUMERODOCUMENTO", Boleto.NumeroDocumento)[m[41m[m
[32m+[m[32m                .Replace("@ESPECIEDOCUMENTO", EspecieDocumento.ValidaSigla(Boleto.EspecieDocumento))[m[41m[m
[32m+[m[32m                .Replace("@DATAPROCESSAMENTO", Boleto.DataProcessamento.ToString("dd/MM/yyyy"))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Implementação para o Banco do Brasil[m[41m[m
[32m+[m[32m                //Variável inserida para atender às especificações da carteira "18-019" do Banco do Brasil[m[41m[m
[32m+[m[32m                //apenas para a ficha de compensação.[m[41m[m
[32m+[m[32m                //Como a variável não existirá se não for a carteira "18-019", não foi colocado o [if].[m[41m[m
[32m+[m[32m                .Replace("@NOSSONUMEROBB", Boleto.Banco.Codigo == 1 & Boleto.Carteira.Equals("18-019") ? Boleto.NossoNumero.Substring(3) : string.Empty)[m[41m[m
[32m+[m[32m                #endregion Implementação para o Banco do Brasil[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                .Replace("@NOSSONUMERO", Boleto.NossoNumero)[m[41m[m
[32m+[m[32m                .Replace("@CARTEIRA", (MostrarCodigoCarteira ? string.Format("{0} - {1}", Boleto.Carteira.ToString(), new Carteira_Santander(Utils.ToInt32(Boleto.Carteira)).Codigo) : Boleto.Carteira.ToString()))[m[41m[m
[32m+[m[32m                .Replace("@ESPECIE", Boleto.Especie)[m[41m[m
[32m+[m[32m                .Replace("@QUANTIDADE", (Boleto.QuantidadeMoeda == 0 ? "" : Boleto.QuantidadeMoeda.ToString()))[m[41m[m
[32m+[m[32m                .Replace("@VALORDOCUMENTO", Boleto.ValorMoeda)[m[41m[m
[32m+[m[32m                .Replace("@=VALORDOCUMENTO", (Boleto.ValorBoleto == 0 ? "" : Boleto.ValorBoleto.ToString("R$ ##,##0.00")))[m[41m[m
[32m+[m[32m                .Replace("@VALORCOBRADO", (Boleto.ValorCobrado == 0 ? "" : Boleto.ValorCobrado.ToString("R$ ##,##0.00")))[m[41m[m
[32m+[m[32m                .Replace("@OUTROSACRESCIMOS", "")[m[41m[m
[32m+[m[32m                .Replace("@OUTRASDEDUCOES", "")[m[41m[m
[32m+[m[32m                .Replace("@DESCONTOS", (Boleto.ValorDesconto == 0 ? "" : Boleto.ValorDesconto.ToString("R$ ##,##0.00")))[m[41m[m
[32m+[m[32m                .Replace("@AGENCIACONTA", agenciaCodigoCedente)[m[41m[m
[32m+[m[32m                .Replace("@SACADO", sacado)[m[41m[m
[32m+[m[32m                .Replace("@INFOSACADO", infoSacado)[m[41m[m
[32m+[m[32m                .Replace("@AGENCIACODIGOCEDENTE", agenciaCodigoCedente)[m[41m[m
[32m+[m[32m                .Replace("@CPFCNPJ", Cedente.CPFCNPJ)[m[41m[m
[32m+[m[32m                .Replace("@MORAMULTA", (Boleto.ValorMulta == 0 ? "" : Boleto.ValorMulta.ToString("R$ ##,##0.00")))[m[41m[m
[32m+[m[32m                .Replace("@AUTENTICACAOMECANICA", "")[m[41m[m
[32m+[m[32m                .Replace("@USODOBANCO", Boleto.UsoBanco)[m[41m[m
[32m+[m[32m                .Replace("@IMAGEMCODIGOBARRA", imagemCodigoBarras)[m[41m[m
[32m+[m[32m                .Replace("@ACEITE", Boleto.Aceite).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion Html[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Geração do Html OffLine[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Função utilizada para gerar o html do boleto sem que o mesmo esteja dentro de uma página Web.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="srcLogo">Local apontado pela imagem de logo.</param>[m[41m[m
[32m+[m[32m        /// <param name="srcBarra">Local apontado pela imagem de barra.</param>[m[41m[m
[32m+[m[32m        /// <param name="srcCodigoBarra">Local apontado pela imagem do código de barras.</param>[m[41m[m
[32m+[m[32m        /// <returns>StringBuilder conténdo o código html do boleto bancário.</returns>[m[41m[m
[32m+[m[32m        protected StringBuilder HtmlOffLine(string textoNoComecoDoEmail, string srcLogo, string srcBarra, string srcCodigoBarra)[m[41m[m
[32m+[m[32m        {//protected StringBuilder HtmlOffLine(string srcCorte, string srcLogo, string srcBarra, string srcPonto, string srcBarraInterna, string srcCodigoBarra)[m[41m[m
[32m+[m[32m            this.OnLoad(EventArgs.Empty);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[32m            HtmlOfflineHeader(html);[m[41m[m
[32m+[m[32m            if (textoNoComecoDoEmail != null && textoNoComecoDoEmail != "")[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                html.Append(textoNoComecoDoEmail);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            html.Append(MontaHtml(srcLogo, srcBarra, "<img src=\"" + srcCodigoBarra + "\" alt=\"Código de Barras\" />"));[m[41m[m
[32m+[m[32m            HtmlOfflineFooter(html);[m[41m[m
[32m+[m[32m            return html;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Monta o Header de um email com pelo menos um boleto dentro.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="saida">StringBuilder onde o conteudo sera salvo.</param>[m[41m[m
[32m+[m[32m        protected static void HtmlOfflineHeader(StringBuilder html)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            html.Append("<!DOCTYPE html PUBLIC \"-//W3C//DTD XHTML 1.0 Transitional//EN\" \"http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd\">\n");[m[41m[m
[32m+[m[32m            html.Append("<html xmlns=\"http://www.w3.org/1999/xhtml\">\n");[m[41m[m
[32m+[m[32m            html.Append("<head>");[m[41m[m
[32m+[m[32m            html.Append("    <title>Boleto.Net</title>\n");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Css[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Stream stream = Assembly.GetExecutingAssembly().GetManifestResourceStream("BoletoNet.BoletoImpressao.BoletoNet.css");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                using (StreamReader sr = new StreamReader(stream))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    html.Append("<style>\n");[m[41m[m
[32m+[m[32m                    html.Append(sr.ReadToEnd());[m[41m[m
[32m+[m[32m                    html.Append("</style>\n");[m[41m[m
[32m+[m[32m                    sr.Close();[m[41m[m
[32m+[m[32m                    sr.Dispose();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Css[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            html.Append("     </head>\n");[m[41m[m
[32m+[m[32m            html.Append("<body>\n");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Monta o Footer de um email com pelo menos um boleto dentro.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="saida">StringBuilder onde o conteudo sera salvo.</param>[m[41m[m
[32m+[m[32m        protected static void HtmlOfflineFooter(StringBuilder saida)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            saida.Append("</body>\n");[m[41m[m
[32m+[m[32m            saida.Append("</html>\n");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Junta varios boletos em uma unica AlternateView, para todos serem mandados juntos no mesmo email[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="arrayDeBoletos">Array contendo os boletos a serem mesclados</param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        public static AlternateView GeraHtmlDeVariosBoletosParaEmail(BoletoBancario[] arrayDeBoletos)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return GeraHtmlDeVariosBoletosParaEmail( null , arrayDeBoletos );[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Junta varios boletos em uma unica AlternateView, para todos serem mandados juntos no mesmo email[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="textoNoComecoDoEmail">Texto em HTML a ser adicionado no comeco do email</param>[m[41m[m
[32m+[m[32m        /// <param name="arrayDeBoletos">Array contendo os boletos a serem mesclados</param>[m[41m[m
[32m+[m[32m        /// <returns>AlternateView com os dados de todos os boleto.</returns>[m[41m[m
[32m+[m[32m        public static AlternateView GeraHtmlDeVariosBoletosParaEmail(string textoNoComecoDoEmail , BoletoBancario[] arrayDeBoletos )[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            var corpoDoEmail = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            var linkedResources = new List<LinkedResource>();[m[41m[m
[32m+[m[32m            HtmlOfflineHeader(corpoDoEmail);[m[41m[m
[32m+[m[32m            if (textoNoComecoDoEmail != null && textoNoComecoDoEmail != "")[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                corpoDoEmail.Append(textoNoComecoDoEmail);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            foreach (var umBoleto in arrayDeBoletos)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (umBoleto != null)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    LinkedResource lrImagemLogo;[m[41m[m
[32m+[m[32m                    LinkedResource lrImagemBarra;[m[41m[m
[32m+[m[32m                    LinkedResource lrImagemCodigoBarra;[m[41m[m
[32m+[m[32m                    umBoleto.GeraGraficosParaEmailOffLine(out lrImagemLogo, out lrImagemBarra, out lrImagemCodigoBarra);[m[41m[m
[32m+[m[32m                    var theOutput = umBoleto.MontaHtml([m[41m[m
[32m+[m[32m                        "cid:" + lrImagemLogo.ContentId,[m[41m[m
[32m+[m[32m                        "cid:" + lrImagemBarra.ContentId,[m[41m[m
[32m+[m[32m                        "<img src=\"cid:" + lrImagemCodigoBarra.ContentId + "\" alt=\"Código de Barras\" />");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    corpoDoEmail.Append(theOutput);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    linkedResources.Add(lrImagemLogo);[m[41m[m
[32m+[m[32m                    linkedResources.Add(lrImagemBarra);[m[41m[m
[32m+[m[32m                    linkedResources.Add(lrImagemCodigoBarra);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            HtmlOfflineFooter(corpoDoEmail);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            AlternateView av = AlternateView.CreateAlternateViewFromString(corpoDoEmail.ToString(), Encoding.Default, "text/html");[m[41m[m
[32m+[m[32m            foreach (var theResource in linkedResources)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                av.LinkedResources.Add(theResource);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return av;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Função utilizada gerar o AlternateView necessário para enviar um boleto bancário por e-mail.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns>AlternateView com os dados do boleto.</returns>[m[41m[m
[32m+[m[32m        public AlternateView HtmlBoletoParaEnvioEmail()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return HtmlBoletoParaEnvioEmail(null);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Função utilizada gerar o AlternateView necessário para enviar um boleto bancário por e-mail.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="textoNoComecoDoEmail">Texto (em HTML) a ser incluido no começo do Email.</param>[m[41m[m
[32m+[m[32m        /// <returns>AlternateView com os dados do boleto.</returns>[m[41m[m
[32m+[m[32m        public AlternateView HtmlBoletoParaEnvioEmail(string textoNoComecoDoEmail)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            LinkedResource lrImagemLogo;[m[41m[m
[32m+[m[32m            LinkedResource lrImagemBarra;[m[41m[m
[32m+[m[32m            LinkedResource lrImagemCodigoBarra;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            GeraGraficosParaEmailOffLine(out lrImagemLogo, out lrImagemBarra, out lrImagemCodigoBarra);[m[41m[m
[32m+[m[32m            StringBuilder html = HtmlOffLine(textoNoComecoDoEmail , "cid:" + lrImagemLogo.ContentId, "cid:" + lrImagemBarra.ContentId, "cid:" + lrImagemCodigoBarra.ContentId);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            AlternateView av = AlternateView.CreateAlternateViewFromString(html.ToString(), Encoding.Default, "text/html");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            av.LinkedResources.Add(lrImagemLogo);[m[41m[m
[32m+[m[32m            av.LinkedResources.Add(lrImagemBarra);[m[41m[m
[32m+[m[32m            av.LinkedResources.Add(lrImagemCodigoBarra);[m[41m[m
[32m+[m[32m            return av;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Gera as tres imagens necessárias para o Boleto[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="lrImagemLogo">O Logo do Banco</param>[m[41m[m
[32m+[m[32m        /// <param name="lrImagemBarra">A Barra Horizontal</param>[m[41m[m
[32m+[m[32m        /// <param name="lrImagemCodigoBarra">O Código de Barras</param>[m[41m[m
[32m+[m[32m        void GeraGraficosParaEmailOffLine(out LinkedResource lrImagemLogo, out LinkedResource lrImagemBarra, out LinkedResource lrImagemCodigoBarra)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.OnLoad(EventArgs.Empty);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            var randomSufix = new Random().Next().ToString(); // para podermos colocar no mesmo email varios boletos diferentes[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            Stream stream = Assembly.GetExecutingAssembly().GetManifestResourceStream("BoletoNet.Imagens." + Utils.FormatCode(_ibanco.Codigo.ToString(), 3) + ".jpg");[m[41m[m
[32m+[m[32m           lrImagemLogo = new LinkedResource(stream, MediaTypeNames.Image.Jpeg);[m[41m[m
[32m+[m[32m            lrImagemLogo.ContentId = "logo" + randomSufix;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            MemoryStream ms = new MemoryStream(Utils.ConvertImageToByte(Html.barra));[m[41m[m
[32m+[m[32m            lrImagemBarra = new LinkedResource(ms, MediaTypeNames.Image.Gif);[m[41m[m
[32m+[m[32m            lrImagemBarra.ContentId = "barra" + randomSufix; ;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            C2of5i cb = new C2of5i(Boleto.CodigoBarra.Codigo, 1, 50, Boleto.CodigoBarra.Codigo.Length);[m[41m[m
[32m+[m[32m            ms = new MemoryStream(Utils.ConvertImageToByte(cb.ToBitmap()));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            lrImagemCodigoBarra = new LinkedResource(ms, MediaTypeNames.Image.Gif);[m[41m[m
[32m+[m[32m            lrImagemCodigoBarra.ContentId = "codigobarra" + randomSufix; ;[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Função utilizada para gravar em um arquivo local o conteúdo do boleto. Este arquivo pode ser aberto em um browser sem que o site esteja no ar.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="fileName">Path do arquivo que deve conter o código html.</param>[m[41m[m
[32m+[m[32m        public void MontaHtmlNoArquivoLocal(string fileName)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            using (FileStream f = new FileStream(fileName, FileMode.Create))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StreamWriter w = new StreamWriter(f, System.Text.Encoding.Default);[m[41m[m
[32m+[m[32m                w.Write(MontaHtml());[m[41m[m
[32m+[m[32m                w.Close();[m[41m[m
[32m+[m[32m                f.Close();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Monta o Html do boleto bancário[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <returns>string</returns>[m[41m[m
[32m+[m[32m        public string MontaHtml()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return MontaHtml(null);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Monta o Html do boleto bancário[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="fileName">Caminho do arquivo</param>[m[41m[m
[32m+[m[32m        /// <returns>Html do boleto gerado</returns>[m[41m[m
[32m+[m[32m        public string MontaHtml(string fileName)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (fileName == null)[m[41m[m
[32m+[m[32m                fileName = System.IO.Path.GetTempPath();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            this.OnLoad(EventArgs.Empty);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //string fnCorte = fileName + @"BoletoNetCorte.gif";[m[41m[m
[32m+[m[32m            //if (!System.IO.File.Exists(fnCorte))[m[41m[m
[32m+[m[32m            //    Html.corte.Save(fnCorte);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string fnLogo = fileName + @"BoletoNet" + Utils.FormatCode(_ibanco.Codigo.ToString(), 3) + ".jpg";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (!System.IO.File.Exists(fnLogo))[m[41m[m
[32m+[m[32m                Image.FromStream(Assembly.GetExecutingAssembly().GetManifestResourceStream("BoletoNet.Imagens." + Utils.FormatCode(_ibanco.Codigo.ToString(), 3) + ".jpg")).Save(fnLogo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //string fnPonto = fileName + @"BoletoNetPonto.gif";[m[41m[m
[32m+[m[32m            //if (!System.IO.File.Exists(fnPonto))[m[41m[m
[32m+[m[32m            //    Html.ponto.Save(fnPonto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //string fnBarraInterna = fileName + @"BoletoNetBarraInterna.gif";[m[41m[m
[32m+[m[32m            //if (!File.Exists(fnBarraInterna))[m[41m[m
[32m+[m[32m            //    Html.barrainterna.Save(fnBarraInterna);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string fnBarra = fileName + @"BoletoNetBarra.gif";[m[41m[m
[32m+[m[32m            if (!File.Exists(fnBarra))[m[41m[m
[32m+[m[32m                Html.barra.Save(fnBarra);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string fnCodigoBarras = System.IO.Path.GetTempFileName();[m[41m[m
[32m+[m[32m            C2of5i cb = new C2of5i(Boleto.CodigoBarra.Codigo, 1, 50, Boleto.CodigoBarra.Codigo.Length);[m[41m[m
[32m+[m[32m            cb.ToBitmap().Save(fnCodigoBarras);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //return HtmlOffLine(fnCorte, fnLogo, fnBarra, fnPonto, fnBarraInterna, fnCodigoBarras).ToString();[m[41m[m
[32m+[m[32m            return HtmlOffLine( null , fnLogo, fnBarra, fnCodigoBarras).ToString();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion Geração do Html OffLine[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public System.Drawing.Image GeraImagemCodigoBarras(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            C2of5i cb = new C2of5i(boleto.CodigoBarra.Codigo, 1, 50, boleto.CodigoBarra.Codigo.Length);[m[41m[m
[32m+[m[32m            return cb.ToBitmap();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
