[32m+[m[32m        public void ForceClose()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            _allowClose = true;[m[41m[m
[32m+[m[32m            _allowClose = false;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
