[32m+[m[32m        # region Métodos de processamento do arquivo retorno CNAB240[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static string Mod11BancoBrasil(string value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            #region Trecho do manual DVMD11.doc[m[41m[m
[32m+[m[32m            /*[m[41m [m
[32m+[m[32m            Multiplicar cada algarismo que compõe o número pelo seu respectivo multiplicador (PESO).[m[41m[m
[32m+[m[32m            Os multiplicadores(PESOS) variam de 9 a 2.[m[41m[m
[32m+[m[32m            O primeiro dígito da direita para a esquerda deverá ser multiplicado por 9, o segundo por 8 e assim sucessivamente.[m[41m[m
[32m+[m[32m            O resultados das multiplicações devem ser somados:[m[41m[m
[32m+[m[32m            72+35+24+27+4+9+8=179[m[41m[m
[32m+[m[32m            O total da soma deverá ser dividido por 11:[m[41m[m
[32m+[m[32m            179 / 11=16[m[41m[m
[32m+[m[32m            RESTO=3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            Se o resto da divisão for igual a 10 o D.V. será igual a X.[m[41m [m
[32m+[m[32m            Se o resto da divisão for igual a 0 o D.V. será igual a 0.[m[41m[m
[32m+[m[32m            Se o resto for menor que 10, o D.V.  será igual ao resto.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            No exemplo acima, o dígito verificador será igual a 3[m[41m[m
[32m+[m[32m            */[m[41m[m
[32m+[m[32m            #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            /* d - Dígito[m[41m[m
[32m+[m[32m             * s - Soma[m[41m[m
[32m+[m[32m             * p - Peso[m[41m[m
[32m+[m[32m             * b - Base[m[41m[m
[32m+[m[32m             * r - Resto[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string d;[m[41m[m
[32m+[m[32m            int s = 0, p = 9, b = 2;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = value.Length - 1; i >= 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                s += (int.Parse(value[i].ToString()) * p);[m[41m[m
[32m+[m[32m                if (p == b)[m[41m[m
[32m+[m[32m                    p = 9;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    p--;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int r = (s % 11);[m[41m[m
[32m+[m[32m            if (r == 10)[m[41m[m
[32m+[m[32m                d = "X";[m[41m[m
[32m+[m[32m            else if (r == 0)[m[41m[m
[32m+[m[32m                d = "0";[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                d = r.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return d;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
