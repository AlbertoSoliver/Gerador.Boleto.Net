[32m+[m[32m    /// </author>[m[41m    [m
[32m+[m[32m    internal class Banco_Real : AbstractBanco, IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private string _dacNossoNumero = string.Empty;[m[41m[m
[32m+[m[32m        private int _dacContaCorrente = 0;[m[41m[m
[32m+[m[32m        private int _dacBoleto = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Classe responsavel em criar os campos do Banco Banco_Real.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        internal Banco_Real()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.Codigo = 356;[m[41m[m
[32m+[m[32m            this.Digito = 5;[m[41m[m
[32m+[m[32m            this.Nome = "Banco Real";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (boleto.Carteira != "57")[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Carteira não implementada. Carteiras implementadas 57.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Formata o tamanho do número da agência[m[41m[m
[32m+[m[32m            if (boleto.Cedente.ContaBancaria.Conta.Length < 4)[m[41m[m
[32m+[m[32m                throw new Exception("Número da agência inválido");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Formata o tamanho do número da conta corrente[m[41m[m
[32m+[m[32m            if (boleto.Cedente.ContaBancaria.Conta.Length < 7)[m[41m[m
[32m+[m[32m                boleto.Cedente.ContaBancaria.Conta = Utils.FormatCode(boleto.Cedente.ContaBancaria.Conta, 7);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Formata o tamanho do número de nosso número[m[41m[m
[32m+[m[32m            if (boleto.NossoNumero.Length < 13)[m[41m[m
[32m+[m[32m                boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 13);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Calcula o DAC do Nosso Número[m[41m[m
[32m+[m[32m            _dacNossoNumero = CalcularDigitoNossoNumero(boleto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Calcula o DAC da Conta Corrente[m[41m[m
[32m+[m[32m            _dacContaCorrente = Mod10(boleto.Cedente.ContaBancaria.Agencia + boleto.Cedente.ContaBancaria.Conta);[m[41m[m
[32m+[m[32m            boleto.Cedente.ContaBancaria.DigitoConta = _dacContaCorrente.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui o nome do banco ao local de pagamento[m[41m[m
[32m+[m[32m            boleto.LocalPagamento += Nome;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se o nosso número é válido[m[41m[m
[32m+[m[32m            if (Utils.ToInt64(boleto.NossoNumero) == 0)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Nosso número inválido");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do processamento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataProcessamento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataProcessamento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do documento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataDocumento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataDocumento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m            FormataNossoNumero(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            //throw new NotImplementedException("Função do fomata nosso número não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNumeroDocumento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função do fomata número do documento não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        ///<summary>[m[41m[m
[32m+[m[32m        /// Campo Livre[m[41m[m
[32m+[m[32m        ///    20 a 23 - 4 - Agência Cedente[m[41m[m
[32m+[m[32m        ///    24 a 30 - 7 - Conta[m[41m[m
[32m+[m[32m        ///    31 a 31 - 1 - Digito da Conta[m[41m[m
[32m+[m[32m        ///    32 a 44 - 13 - Número do Nosso Número[m[41m[m
[32m+[m[32m        ///</summary>[m[41m[m
[32m+[m[32m        public string CampoLivre(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return boleto.Cedente.ContaBancaria.Agencia + boleto.Cedente.ContaBancaria.Conta + Mod10(boleto.NossoNumero + boleto.Cedente.ContaBancaria.Agencia + boleto.Cedente.ContaBancaria.Conta) + boleto.NossoNumero;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Calcula o digito do Nosso Numero[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string CalcularDigitoNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int dig;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            dig = Mod10(boleto.NossoNumero.Substring(0, 9) + boleto.Cedente.ContaBancaria.Agencia + boleto.Cedente.ContaBancaria.Conta);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return dig.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //throw new NotImplementedException("Função do calcular digito do nosso número não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// A linha digitável será composta por cinco campos:[m[41m[m
[32m+[m[32m        ///      1º campo[m[41m[m
[32m+[m[32m        ///          composto pelo código de Banco, código da moeda, as cinco primeiras posições do campo[m[41m [m
[32m+[m[32m        ///          livre e o dígito verificador deste campo;[m[41m[m
[32m+[m[32m        ///      2º campo[m[41m[m
[32m+[m[32m        ///          composto pelas posições 6ª a 15ª do campo livre e o dígito verificador deste campo;[m[41m[m
[32m+[m[32m        ///      3º campo[m[41m[m
[32m+[m[32m        ///          composto pelas posições 16ª a 25ª do campo livre e o dígito verificador deste campo;[m[41m[m
[32m+[m[32m        ///      4º campo[m[41m[m
[32m+[m[32m        ///          composto pelo dígito verificador do código de barras, ou seja, a 5ª posição do código de[m[41m [m
[32m+[m[32m        ///          barras;[m[41m[m
[32m+[m[32m        ///      5º campo[m[41m[m
[32m+[m[32m        ///          composto pelo valor nominal do documento com supressão de Zeros e sem edição.[m[41m[m
[32m+[m[32m        ///          Quando se tratar de valor zerado, a representação deverá ser 000 (três Zeros).[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //AAABC.CCCCX DDDDD.DDDDDY EEEEE.EEEEEZ K VVVVVVVVVVVVVV[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string LD = string.Empty; //Linha Digitável[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Campo 1[m[41m[m
[32m+[m[32m            string AAA = Utils.FormatCode(boleto.Banco.Codigo.ToString(), 3);[m[41m[m
[32m+[m[32m            string B = boleto.Moeda.ToString();[m[41m[m
[32m+[m[32m            string CCCCC = CampoLivre(boleto).Substring(0, 5);[m[41m[m
[32m+[m[32m            string X = Mod10(AAA + B + CCCCC).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD = string.Format("{0}{1}{2}.", AAA, B, CCCCC.Substring(0, 1));[m[41m[m
[32m+[m[32m            LD += string.Format("{0}{1} ", CCCCC.Substring(1, 4), X);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 2[m[41m[m
[32m+[m[32m            string DDDDDD = CampoLivre(boleto).Substring(5, 10);[m[41m[m
[32m+[m[32m            string Y = Mod10(DDDDDD).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += string.Format("{0}.", DDDDDD.Substring(0, 5));[m[41m[m
[32m+[m[32m            LD += string.Format("{0}{1} ", DDDDDD.Substring(5, 5), Y);[m[41m[m
[32m+[m[32m            #endregion Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string EEEEE = CampoLivre(boleto).Substring(15, 10);[m[41m[m
[32m+[m[32m            string Z = Mod10(EEEEE).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += string.Format("{0}.", EEEEE.Substring(0, 5));[m[41m[m
[32m+[m[32m            LD += string.Format("{0}{1} ", EEEEE.Substring(5, 5), Z);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string K = _dacBoleto.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += string.Format(" {0} ", K);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 5[m[41m[m
[32m+[m[32m            string VVVVVVVVVVVVVV;[m[41m[m
[32m+[m[32m            if (boleto.ValorBoleto != 0)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                VVVVVVVVVVVVVV = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m                VVVVVVVVVVVVVV = FatorVencimento(boleto) + Utils.FormatCode(VVVVVVVVVVVVVV, 10);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                VVVVVVVVVVVVVV = "000";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += VVVVVVVVVVVVVV;[m[41m[m
[32m+[m[32m            #endregion Campo 5[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel = LD;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///	O código de barra para cobrança contém 44 posições dispostas da seguinte forma:[m[41m[m
[32m+[m[32m        ///    01 a 03 - 3 - Identificação  do  Banco[m[41m[m
[32m+[m[32m        ///    04 a 04 - 1 - Código da Moeda[m[41m[m
[32m+[m[32m        ///    05 a 05  1 - Dígito verificador do Código de Barras[m[41m[m
[32m+[m[32m        ///    06 a 19 - 14 - Valor[m[41m[m
[32m+[m[32m        ///    20 a 44  25 - Campo Livre[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string valorBoleto = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m            valorBoleto = Utils.FormatCode(valorBoleto, 14);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                    Codigo,[m[41m[m
[32m+[m[32m                    boleto.Moeda,[m[41m[m
[32m+[m[32m                    FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                    valorBoleto.Substring(4, 10),[m[41m[m
[32m+[m[32m                    boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                    boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                    Mod10(boleto.NossoNumero + boleto.Cedente.ContaBancaria.Agencia + boleto.Cedente.ContaBancaria.Conta),[m[41m[m
[32m+[m[32m                    boleto.NossoNumero[m[41m[m
[32m+[m[32m            );[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            _dacBoleto = Mod11(Strings.Left(boleto.CodigoBarra.Codigo, 4) + Strings.Right(boleto.CodigoBarra.Codigo, 39), 9, 0);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = Strings.Left(boleto.CodigoBarra.Codigo, 4) + _dacBoleto + Strings.Right(boleto.CodigoBarra.Codigo, 39);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarDetalheRemessa(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarHeaderRemessa(string numeroConvenio, Cedente cedente, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarTrailerRemessa(int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
