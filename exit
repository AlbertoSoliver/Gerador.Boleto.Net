[32m+[m[32m                string _trailerLote;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _trailerLote = "00100015         ";[m[41m[m
[32m+[m[32m                _trailerLote += Utils.FitStringLength(numeroRegistro.ToString(), 6, 6, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _trailerLote += _brancos92;[m[41m[m
[32m+[m[32m                _trailerLote += _brancos125;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _trailerLote = Utils.SubstituiCaracteresEspeciais(_trailerLote);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _trailerLote;[m[41m[m
