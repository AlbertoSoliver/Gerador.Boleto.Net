[32m+[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se o nosso número é válido[m[41m[m
[32m+[m[32m            if (Utils.ToInt64(boleto.NossoNumero) == 0)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Nosso número inválido");[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se o tamanho para o NossoNumero são 10 dígitos[m[41m[m
[32m+[m[32m            if (boleto.NossoNumero.Length > 10)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("A quantidade de dígitos do nosso número para a carteira " + boleto.Carteira + ", são 10 números.");[m[41m[m
[32m+[m[32m            else if (boleto.NossoNumero.Length < 10)[m[41m[m
[32m+[m[32m                boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (boleto.Carteira != "CNR")[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Carteira não implementada. Utilize a carteira CNR.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui o nome do banco ao local de pagamento[m[41m[m
[32m+[m[32m            boleto.LocalPagamento += Nome + "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do processamento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataProcessamento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataProcessamento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do documento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataDocumento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataDocumento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            FormataNossoNumero(boleto);[m[41m[m
[32m+[m[32m            FormataNumeroDocumento(boleto);[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion IBanco Members[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos privados[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private int Mod11_CodigoBarra(string value, int Base)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int Digito, Soma = 0, Peso = 2;[m[41m[m
[32m+[m[32m            for (int i = value.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Soma = Soma + (Convert.ToInt32(Strings.Mid(value, i, 1)) * Peso);[m[41m[m
[32m+[m[32m                if (Peso == Base)[m[41m[m
[32m+[m[32m                    Peso = 2;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    Peso = Peso + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            if (((Soma % 11) == 0) || ((Soma % 11) == 10) || ((Soma % 11) == 1))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Digito = 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Digito = 11 - (Soma % 11);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return Digito;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private int Mod10_LinhaDigitavel(string seq)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int Digito, Soma = 0, Peso = 2, m1;[m[41m[m
[32m+[m[32m            string m2;[m[41m[m
[32m+[m[32m            for (int i = seq.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                m1 = (Convert.ToInt32(Strings.Mid(seq, i, 1)) * Peso);[m[41m[m
[32m+[m[32m                m2 = m1.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                for (int j = 1; j <= m2.Length; j++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    Soma += Convert.ToInt32(Strings.Mid(m2, j, 1));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (Peso == 2)[m[41m[m
[32m+[m[32m                    Peso = 1;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    Peso = Peso + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            Digito = ((10 - (Soma % 10)) % 10);[m[41m[m
[32m+[m[32m            return Digito;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
