[32m+[m[32m        #region Métodos de Instância[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public void LerDetalheRetornoCNAB400(string registro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                int dataOcorrencia = Utils.ToInt32(registro.Substring(110, 6));[m[41m[m
[32m+[m[32m                int dataVencimento = Utils.ToInt32(registro.Substring(146, 6));[m[41m[m
[32m+[m[32m                int dataCredito = Utils.ToInt32(registro.Substring(295, 6));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _codigoInscricao = Utils.ToInt32(registro.Substring(1, 2));[m[41m[m
[32m+[m[32m                _numeroInscricao = registro.Substring(3, 14);[m[41m[m
[32m+[m[32m                _agencia = Utils.ToInt32(registro.Substring(17, 4));[m[41m[m
[32m+[m[32m                _conta = Utils.ToInt32(registro.Substring(23, 5));[m[41m[m
[32m+[m[32m                _dacConta = Utils.ToInt32(registro.Substring(28, 1));[m[41m[m
[32m+[m[32m                _usoEmpresa = registro.Substring(37, 25);[m[41m[m
[32m+[m[32m                _nossoNumero = Utils.ToString(registro.Substring(85, 8));[m[41m[m
[32m+[m[32m                _dacNossoNumero = Utils.ToInt32(registro.Substring(93, 1));[m[41m[m
[32m+[m[32m                _carteira = registro.Substring(107, 1);[m[41m[m
[32m+[m[32m                _codigoOcorrencia = Utils.ToInt32(registro.Substring(108, 2));[m[41m[m
[32m+[m[32m                _dataOcorrencia = Utils.ToDateTime(dataOcorrencia.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                _numeroDocumento = registro.Substring(116, 10);[m[41m[m
[32m+[m[32m                _nossoNumero = Utils.ToString(registro.Substring(126, 9));[m[41m[m
[32m+[m[32m                _dataVencimento = Utils.ToDateTime(dataVencimento.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                double valorTitulo = Convert.ToInt64(registro.Substring(152, 13));[m[41m[m
[32m+[m[32m                _valorTitulo = valorTitulo / 100;[m[41m[m
[32m+[m[32m                _codigoBanco = Utils.ToInt32(registro.Substring(165, 3));[m[41m[m
[32m+[m[32m                _agenciaCobradora = Utils.ToInt32(registro.Substring(168, 4));[m[41m[m
[32m+[m[32m                _especie = Utils.ToInt32(registro.Substring(173, 2));[m[41m[m
[32m+[m[32m                double tarifaCobranca = Convert.ToUInt64(registro.Substring(174, 13));[m[41m[m
[32m+[m[32m                _tarifaCobranca = tarifaCobranca / 100;[m[41m[m
[32m+[m[32m                // 26 brancos[m[41m[m
[32m+[m[32m                double iof = Convert.ToUInt64(registro.Substring(214, 13));[m[41m[m
[32m+[m[32m                _iof = iof / 100;[m[41m[m
[32m+[m[32m                double valorAbatimento = Convert.ToUInt64(registro.Substring(227, 13));[m[41m[m
[32m+[m[32m                _valorAbatimento = valorAbatimento / 100;[m[41m[m
[32m+[m[32m                double valorPrincipal = Convert.ToUInt64(registro.Substring(253, 13));[m[41m[m
[32m+[m[32m                _valorPrincipal = valorPrincipal / 100;[m[41m[m
[32m+[m[32m                double jurosMora = Convert.ToUInt64(registro.Substring(266, 13));[m[41m[m
[32m+[m[32m                _jurosMora = jurosMora / 100;[m[41m[m
[32m+[m[32m                _dataOcorrencia = Utils.ToDateTime(dataOcorrencia.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                // 293 - 3 brancos[m[41m[m
[32m+[m[32m                _dataCredito = Utils.ToDateTime(dataCredito.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                _instrucaoCancelada = Utils.ToInt32(registro.Substring(301, 4));[m[41m[m
[32m+[m[32m                // 306 - 6 brancos[m[41m[m
[32m+[m[32m                // 311 - 13 zeros[m[41m[m
[32m+[m[32m                _nomeSacado = registro.Substring(324, 30);[m[41m[m
[32m+[m[32m                // 354 - 23 brancos[m[41m[m
[32m+[m[32m                _erros = registro.Substring(377, 8);[m[41m[m
[32m+[m[32m                // 377 - Registros rejeitados ou alegação do sacado[m[41m[m
[32m+[m[32m                // 386 - 7 brancos[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _codigoLiquidacao = registro.Substring(392, 2);[m[41m[m
[32m+[m[32m                _numeroSequencial = Utils.ToInt32(registro.Substring(394, 6));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao ler detalhe do arquivo de RETORNO / CNAB 400.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static string PrimeiroCaracter(string retorno)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return retorno.Substring(0, 1);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao desmembrar registro.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
