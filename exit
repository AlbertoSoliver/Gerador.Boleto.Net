[32m+[m[32m                //Código do Raterio[m[41m[m
[32m+[m[32m                detalhe.CodigoRateio = Utils.ToInt32(registro.Substring(140, 4));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Data Ocorrência no Banco[m[41m[m
[32m+[m[32m                int dataOcorrencia = Utils.ToInt32(registro.Substring(110, 8));[m[41m[m
[32m+[m[32m                detalhe.DataOcorrencia = Utils.ToDateTime(dataOcorrencia.ToString("##-##-####"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Data Vencimento do Título[m[41m[m
[32m+[m[32m                int dataVencimento = Utils.ToInt32(registro.Substring(148, 8));[m[41m[m
[32m+[m[32m                detalhe.DataVencimento = Utils.ToDateTime(dataVencimento.ToString("##-##-####"));[m[41m[m
[32m+[m[41m[m
