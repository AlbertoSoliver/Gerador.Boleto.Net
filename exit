[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    internal class Banco_HSBC : AbstractBanco, IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        #region Variáveis[m[41m[m
[32m+[m[32m        private string dacBoleto;[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Construtores[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal Banco_HSBC()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.Codigo = 399;[m[41m[m
[32m+[m[32m                this.Digito = 9;[m[41m[m
[32m+[m[32m                this.Nome = "HSBC";[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao instanciar objeto.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de Instância[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Validações particulares do banco Itaú[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            FormataNossoNumero(boleto);[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Métodos de formatação do boleto[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o código de barras[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <example>[m[41m[m
[32m+[m[32m        /// DE ATÉ TAMANHO CONTEÚDO[m[41m[m
[32m+[m[32m        /// -----------------------[m[41m[m
[32m+[m[32m        /// 01 03 03 Código do HSBC na Câmara de Compensação, igual a 399.[m[41m[m
[32m+[m[32m        /// 04 04 01 Tipo de Moeda (9 para moeda Real ou 0 para Moeda Variável).[m[41m[m
[32m+[m[32m        /// 05 05 01 Dígito de Autoconferência (DAC).[m[41m[m
[32m+[m[32m        /// 06 09 04 Fator de Vencimento.[m[41m[m
[32m+[m[32m        /// 10 19 10 Valor do Documento. Se Moeda Variável, o valor deverá ser igual a zeros.[m[41m[m
[32m+[m[32m        /// 20 26 07 Código do Cedente[m[41m[m
[32m+[m[32m        /// 27 39 13 Número Bancário, igual ao Código do Documento, sem os dígitos verificadores e tipo identificador.[m[41m[m
[32m+[m[32m        /// 40 43 04 Data de Vencimento no Formato Juliano.[m[41m[m
[32m+[m[32m        /// 44 44 01 Código do Produto CNR, igual a 2.[m[41m[m
[32m+[m[32m        /// </example>[m[41m[m
[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}"[m[41m[m
[32m+[m[32m                , Codigo.ToString()[m[41m[m
[32m+[m[32m                , boleto.Moeda[m[41m[m
[32m+[m[32m                , FatorVencimento(boleto)[m[41m[m
[32m+[m[32m                , boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "").PadLeft(10, '0')[m[41m[m
[32m+[m[32m                , boleto.Cedente.Codigo.ToString()[m[41m[m
[32m+[m[32m                , boleto.NossoNumero.Substring(0, 13)[m[41m[m
[32m+[m[32m                , boleto.DataVencimento.DayOfYear.ToString("000") + boleto.DataVencimento.ToString("yy").Substring(1, 1)[m[41m[m
[32m+[m[32m                , "2");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            dacBoleto = DVDAC(boleto.CodigoBarra.Codigo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = Strings.Left(boleto.CodigoBarra.Codigo, 4) + dacBoleto + Strings.Right(boleto.CodigoBarra.Codigo, 39);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata a linha digitavel[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            //AAABC.CCCCd CCDDD.DDDDDd DDDDD.EEEE2d d FFFFVVVVVVVVVV[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string parte1, parte2;[m[41m[m
[32m+[m[32m            string Grupo1, Grupo2, Grupo3, Grupo4, Grupo5;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string cd = boleto.Cedente.Codigo.ToString().Length < 7 ? "0000000" : boleto.Cedente.Codigo.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            parte1 = "399" + boleto.Moeda.ToString() + boleto.Cedente.Codigo.ToString().Substring(0, 1);[m[41m[m
[32m+[m[32m            parte2 = cd.Substring(1, 4);[m[41m[m
[32m+[m[32m            Grupo1 = string.Format("{0}.{1}{2} ", parte1, parte2, DVLinhaDigitavel(parte1 + parte2));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            parte1 = cd.ToString().Substring(5, 2) + boleto.NossoNumero.Substring(0, 3);[m[41m[m
[32m+[m[32m            parte2 = boleto.NossoNumero.Substring(3, 5);[m[41m[m
[32m+[m[32m            Grupo2 = string.Format("{0}.{1}{2} ", parte1, parte2, DVLinhaDigitavel(parte1 + parte2));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            parte1 = boleto.NossoNumero.Substring(8, 5);[m[41m[m
[32m+[m[32m            parte2 = boleto.DataVencimento.DayOfYear.ToString("000") + boleto.DataVencimento.ToString("yy").Substring(1, 1) + "2";[m[41m[m
[32m+[m[32m            Grupo3 = string.Format("{0}.{1}{2} ", parte1, parte2, DVLinhaDigitavel(parte1 + parte2));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            Grupo4 = string.Format("{0} ", dacBoleto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 5[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            parte1 = FatorVencimento(boleto) + boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "").PadLeft(10, '0');[m[41m[m
[32m+[m[32m            Grupo5 = string.Format("{0}", parte1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel = Grupo1 + Grupo2 + Grupo3 + Grupo4 + Grupo5;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Realiza formatação do NossoNumero de acordo com as regras do HSBC[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void FormataNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string dv1, dv2;[m[41m[m
[32m+[m[32m            long soma;[m[41m[m
[32m+[m[32m            boleto.NossoNumero = boleto.NossoNumero.PadLeft(13, '0');[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            dv1 = DVNossoNumero(boleto.NossoNumero);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            soma = Convert.ToInt64(boleto.NossoNumero + dv1 + "4") + boleto.Cedente.Codigo + int.Parse(boleto.DataVencimento.ToString("ddMMyy"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            dv2 = DVNossoNumero(soma.ToString());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.NossoNumero += dv1 + "4" + dv2;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNumeroDocumento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Métodos de geração do arquivo remessa CNAB400[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region HEADER[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// HEADER do arquivo CNAB[m[41m[m
[32m+[m[32m        /// Gera o HEADER do arquivo remessa de acordo com o lay-out informado[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override string GerarHeaderRemessa(string numeroConvenio, Cedente cedente, TipoArquivo tipoArquivo)[m[41m[m
