[32m+[m[32m            string _sacadoNome = registro.Substring(148, 40); //27[m[41m[m
[32m+[m[32m            string _cnab28 = registro.Substring(188, 10); //28[m[41m[m
[32m+[m[32m            string _valorTarifasCustas = registro.Substring(198, 13); //29[m[41m[m
