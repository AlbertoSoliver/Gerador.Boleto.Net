[32m+[m[32m                    c.Codigo = Convert.ToInt32(dtoced.conta.ToString());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    double valorboleto = Convert.ToDouble(ValorBoletotextBox.Text);[m[41m[m
[32m+[m[32m                    Boleto b = new Boleto(vencimento, valorboleto, "57", "92082835", c);[m[41m[m
[32m+[m[32m                    b.NumeroDocumento = "1008073";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    b.Sacado = new Sacado(dtosac.cpfcnpj, dtosac.nome);[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.End = dtoend.End;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Bairro = dtoend.Bairro;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Cidade = dtoend.Cidade;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.CEP = dtoend.CEP;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.UF = dtoend.UF;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Instrucao instr = new Instrucao(001);[m[41m[m
[32m+[m[32m                    instr.Descricao = "Não Receber após o vencimento";[m[41m[m
[32m+[m[32m                    b.Instrucoes.Add(instr);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb.Boleto = b;[m[41m[m
[32m+[m[32m                    bb.Boleto.Valida();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boletos.Add(bb);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                GeraLayout(boletos);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region BOLETO HSBC[m[41m[m
[32m+[m[32m        public void GeraBoletoHsbc(int qtde)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                CedenteDTO dtoced = new CedenteDTO();[m[41m[m
[32m+[m[32m                dtoced.nome = NOMEtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.cpfcnpj = CPFtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.agencia = AGtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.conta = CCtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                SacadoDTO dtosac = new SacadoDTO();[m[41m[m
[32m+[m[32m                dtosac.cpfcnpj = cpfsactextBox.Text;[m[41m[m
[32m+[m[32m                dtosac.nome = nomesactextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                EnderecoDTO dtoend = new EnderecoDTO();[m[41m[m
[32m+[m[32m                dtoend.End = endtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Bairro = bairrotextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Cidade = CidadetextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.CEP = CEPtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.UF = UFtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Cria o boleto, e passa os parâmetros usuais[m[41m[m
[32m+[m[32m                BoletoBancario bb;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                List<BoletoBancario> boletos = new List<BoletoBancario>();[m[41m[m
[32m+[m[32m                for (int i = 0; i < qtde; i++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb = new BoletoBancario();[m[41m[m
[32m+[m[32m                    bb.CodigoBanco = _codigoBanco;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    DateTime _dia = DateTime.Now;[m[41m[m
[32m+[m[32m                    DateTime vencimento = Convert.ToDateTime(_dia.AddDays(5).ToString("dd/MM/yyyy"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Cedente c = new Cedente(dtoced.cpfcnpj, dtoced.nome, dtoced.agencia, dtoced.conta);[m[41m[m
[32m+[m[32m                    // Código fornecido pela agencia, NÃO é o numero da conta[m[41m[m
[32m+[m[32m                    c.Codigo = Convert.ToInt32(dtoced.conta.ToString());// 7 posicoes[m[41m                [m
[32m+[m[41m[m
[32m+[m[32m                    double valorboleto = Convert.ToDouble(ValorBoletotextBox.Text);[m[41m[m
[32m+[m[32m                    Boleto b = new Boleto(vencimento, valorboleto, "CNR", "0000321585", c); //cod documento[m[41m[m
[32m+[m[32m                    b.NumeroDocumento = "0000032548956"; // nr documento[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    b.Sacado = new Sacado(dtosac.cpfcnpj, dtosac.nome);[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.End = dtoend.End;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Bairro = dtoend.Bairro;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Cidade = dtoend.Cidade;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.CEP = dtoend.CEP;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.UF = dtoend.UF;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Instrucao instr = new Instrucao(001);[m[41m[m
[32m+[m[32m                    instr.Descricao = "Não Receber após o vencimento";[m[41m[m
[32m+[m[32m                    b.Instrucoes.Add(instr);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb.Boleto = b;[m[41m[m
[32m+[m[32m                    bb.Boleto.Valida();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boletos.Add(bb);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                GeraLayout(boletos);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region BOLETO BANCO DO BRASIL[m[41m[m
[32m+[m[32m        public void GeraBoletoBB(int qtde)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                CedenteDTO dtoced = new CedenteDTO();[m[41m[m
[32m+[m[32m                dtoced.nome = NOMEtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.cpfcnpj = CPFtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.agencia = AGtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.conta = CCtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                SacadoDTO dtosac = new SacadoDTO();[m[41m[m
[32m+[m[32m                dtosac.cpfcnpj = cpfsactextBox.Text;[m[41m[m
[32m+[m[32m                dtosac.nome = nomesactextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                EnderecoDTO dtoend = new EnderecoDTO();[m[41m[m
[32m+[m[32m                dtoend.End = endtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Bairro = bairrotextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Cidade = CidadetextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.CEP = CEPtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.UF = UFtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Cria o boleto, e passa os parâmetros usuais[m[41m[m
[32m+[m[32m                BoletoBancario bb;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                List<BoletoBancario> boletos = new List<BoletoBancario>();[m[41m[m
[32m+[m[32m                for (int i = 0; i < qtde; i++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb = new BoletoBancario();[m[41m[m
[32m+[m[32m                    bb.CodigoBanco = _codigoBanco;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    DateTime _dia = DateTime.Now;[m[41m[m
[32m+[m[32m                    DateTime vencimento = Convert.ToDateTime(_dia.AddDays(5).ToString("dd/MM/yyyy"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Cedente c = new Cedente(dtoced.cpfcnpj, dtoced.nome, dtoced.agencia, dtoced.conta);[m[41m[m
[32m+[m[32m                    c.Codigo = Convert.ToInt32(dtoced.conta.ToString());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    double valorboleto = Convert.ToDouble(ValorBoletotextBox.Text);[m[41m[m
[32m+[m[32m                    Boleto b = new Boleto(vencimento, valorboleto, "18", "12345678901", c);[m[41m[m
[32m+[m[32m                    b.NumeroDocumento = "12345678901";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    b.Sacado = new Sacado(dtosac.cpfcnpj, dtosac.nome);[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.End = dtoend.End;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Bairro = dtoend.Bairro;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Cidade = dtoend.Cidade;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.CEP = dtoend.CEP;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.UF = dtoend.UF;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Instrucao instr = new Instrucao(001);[m[41m[m
[32m+[m[32m                    instr.Descricao = "Não Receber após o vencimento";[m[41m[m
[32m+[m[32m                    b.Instrucoes.Add(instr);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb.Boleto = b;[m[41m[m
[32m+[m[32m                    bb.Boleto.Valida();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boletos.Add(bb);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                GeraLayout(boletos);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region BOLETO BRADESCO[m[41m[m
[32m+[m[32m        public void GeraBoletoBradesco(int qtde)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                CedenteDTO dtoced = new CedenteDTO();[m[41m[m
[32m+[m[32m                dtoced.nome = NOMEtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.cpfcnpj = CPFtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.agencia = AGtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.conta = CCtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                SacadoDTO dtosac = new SacadoDTO();[m[41m[m
[32m+[m[32m                dtosac.cpfcnpj = cpfsactextBox.Text;[m[41m[m
[32m+[m[32m                dtosac.nome = nomesactextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                EnderecoDTO dtoend = new EnderecoDTO();[m[41m[m
[32m+[m[32m                dtoend.End = endtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Bairro = bairrotextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Cidade = CidadetextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.CEP = CEPtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.UF = UFtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Cria o boleto, e passa os parâmetros usuais[m[41m[m
[32m+[m[32m                BoletoBancario bb;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                List<BoletoBancario> boletos = new List<BoletoBancario>();[m[41m[m
[32m+[m[32m                for (int i = 0; i < qtde; i++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb = new BoletoBancario();[m[41m[m
[32m+[m[32m                    bb.CodigoBanco = _codigoBanco;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    DateTime _dia = DateTime.Now;[m[41m[m
[32m+[m[32m                    DateTime vencimento = Convert.ToDateTime(_dia.AddDays(5).ToString("dd/MM/yyyy"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Cedente c = new Cedente(dtoced.cpfcnpj, dtoced.nome, dtoced.agencia, dtoced.conta);[m[41m[m
[32m+[m[32m                    c.Codigo = Convert.ToInt32(dtoced.conta.ToString());[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    double valorboleto = Convert.ToDouble(ValorBoletotextBox.Text);[m[41m[m
[32m+[m[32m                    Boleto b = new Boleto(vencimento, valorboleto, "02", "01000000001", c);[m[41m[m
[32m+[m[32m                    b.NumeroDocumento = "01000015235";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    b.Sacado = new Sacado(dtosac.cpfcnpj, dtosac.nome);[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.End = dtoend.End;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Bairro = dtoend.Bairro;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Cidade = dtoend.Cidade;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.CEP = dtoend.CEP;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.UF = dtoend.UF;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Instrucao instr = new Instrucao(001);[m[41m[m
[32m+[m[32m                    instr.Descricao = "Não Receber após o vencimento";[m[41m[m
[32m+[m[32m                    b.Instrucoes.Add(instr);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb.Boleto = b;[m[41m[m
[32m+[m[32m                    bb.Boleto.Valida();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boletos.Add(bb);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                GeraLayout(boletos);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Eventos do BackgroundWorker[m[41m[m
[32m+[m[32m        private void backgroundWorker_DoWork(object sender, DoWorkEventArgs e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                switch (CodigoBanco)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case 1: // Banco do Brasil[m[41m[m
[32m+[m[32m                        GeraBoletoBB(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case 347: // Sudameris[m[41m[m
[32m+[m[32m                        GeraBoletoSudameris(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case 422: // Safra[m[41m[m
[32m+[m[32m                        GeraBoletoSafra(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case 341: // Itau[m[41m[m
[32m+[m[32m                        GeraBoletoItau(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case 356: // Real[m[41m[m
[32m+[m[32m                        GeraBoletoReal(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case 399: // Hsbc[m[41m[m
[32m+[m[32m                        GeraBoletoItau(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case 237: // Bradesco[m[41m[m
[32m+[m[32m                        GeraBoletoBradesco(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case 104: // Caixa[m[41m[m
[32m+[m[32m                        GeraBoletoCaixa(Convert.ToInt32(numericUpDown.Value));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message, "Alerta de Erro", MessageBoxButtons.OK, MessageBoxIcon.Error);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void backgroundWorker_RunWorkerCompleted(object sender, RunWorkerCompletedEventArgs e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                _progresso.Close();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Cria um formulário com um componente WebBrowser dentro[m[41m[m
[32m+[m[32m                _impressaoBoleto.webBrowser.Navigate(_arquivo);[m[41m[m
[32m+[m[32m                _impressaoBoleto.ShowDialog();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion Eventos do BackgroundWorker[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Load, Button Chamar Bancos[m[41m[m
[32m+[m[32m        public Main()[m[41m[m
