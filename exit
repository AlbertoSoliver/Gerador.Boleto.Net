[32m+[m[32m              </xsd:sequence>[m[41m[m
[32m+[m[32m              <xsd:attribute name="name" type="xsd:string" use="required" />[m[41m[m
[32m+[m[32m            </xsd:complexType>[m[41m[m
[32m+[m[32m          </xsd:element>[m[41m[m
[32m+[m[32m        </xsd:choice>[m[41m[m
