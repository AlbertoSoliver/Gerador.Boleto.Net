[32m+[m[32m[assembly: WebResource("BoletoNet.Imagens.070.jpg", "image/jpg")][m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    /// <author>[m[41m  [m
[32m+[m[32m    /// Eduardo Frare[m[41m[m
[32m+[m[32m    /// Stiven[m[41m [m
[32m+[m[32m    /// Diogo[m[41m[m
[32m+[m[32m    /// Miamoto[m[41m[m
[32m+[m[32m    /// </author>[m[41m    [m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    /// <summary>[m[41m[m
[32m+[m[32m    /// Classe referente ao banco Banco_BRB[m[41m[m
[32m+[m[32m    /// </summary>[m[41m[m
[32m+[m[32m    internal class Banco_BRB : AbstractBanco, IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private int _dacBoleto = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal Banco_BRB()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.Codigo = 70;[m[41m[m
[32m+[m[32m            this.Digito = 1;[m[41m[m
[32m+[m[32m            this.Nome = "Banco_BRB";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region IBanco Members[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            // Código de Barras[m[41m[m
[32m+[m[32m            //banco & moeda & fator & valor & carteira & nossonumero & dac_nossonumero & agencia & conta & dac_conta & "000"[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string banco = Utils.FormatCode(Codigo.ToString(), 3);[m[41m[m
[32m+[m[32m            int moeda = boleto.Moeda;[m[41m[m
[32m+[m[32m            //string digito = "";[m[41m[m
[32m+[m[32m            string valorBoleto = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m            valorBoleto = Utils.FormatCode(valorBoleto, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string fatorVencimento = FatorVencimento(boleto).ToString();[m[41m[m
[32m+[m[32m            string chave = boleto.CodigoBarra.Chave;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo =[m[41m[m
[32m+[m[32m                string.Format("{0}{1}{2}{3}{4}", banco, moeda,fatorVencimento,[m[41m[m
[32m+[m[32m                              valorBoleto, boleto.CodigoBarra.Chave);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            _dacBoleto = Banco_BRB.Mod11_CodigoBarra(boleto.CodigoBarra.Codigo, 9);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = Strings.Left(boleto.CodigoBarra.Codigo, 4) + _dacBoleto + Strings.Right(boleto.CodigoBarra.Codigo, 39);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string BBB = Utils.FormatCode(Codigo.ToString(), 3);[m[41m[m
[32m+[m[32m            int M = boleto.Moeda;[m[41m[m
[32m+[m[32m            string CCCCC1 = boleto.CodigoBarra.Chave.Substring(0, 5);[m[41m[m
[32m+[m[32m            int D1 = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string CCCCCCCCCC2 = boleto.CodigoBarra.Chave.Substring(5, 10);[m[41m[m
[32m+[m[32m            int D2 = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string CCCCCCCCCC3 = boleto.CodigoBarra.Chave.Substring(15, 10);[m[41m[m
[32m+[m[32m            int D3 = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            D1 = Mod10(BBB + M + CCCCC1);[m[41m[m
[32m+[m[32m            string Grupo1 = string.Format("{0}.{1}{2} ", BBB + M + CCCCC1.Substring(0, 1), CCCCC1.Substring(1, 4), D1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            D2 = Mod10(CCCCCCCCCC2);[m[41m[m
[32m+[m[32m            string Grupo2 = string.Format("{0}.{1}{2} ", CCCCCCCCCC2.Substring(0, 5), CCCCCCCCCC2.Substring(5, 5), D2);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            D3 = Mod10(CCCCCCCCCC3);[m[41m[m
[32m+[m[32m            string Grupo3 = string.Format("{0}.{1}{2} ", CCCCCCCCCC3.Substring(0, 5), CCCCCCCCCC3.Substring(5, 5), D3);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string Grupo4 = string.Format("{0} {1}{2}", _dacBoleto, FatorVencimento(boleto).ToString(), Utils.FormatCode(boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", ""), 10));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel = Grupo1 + Grupo2 + Grupo3 + Grupo4;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            boleto.NossoNumero = string.Format("{0}{1}{2}", boleto.Categoria, boleto.NossoNumero, Utils.FormatCode(Codigo.ToString(), 3) + boleto.CodigoBarra.Chave.Substring(23, 2));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNumeroDocumento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            boleto.NumeroDocumento = string.Format("{0}", boleto.NumeroDocumento);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            //Verifica se o nosso número é válido[m[41m[m
[32m+[m[32m            if (Utils.ToInt64(boleto.NossoNumero) == 0)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Nosso número inválido");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se o tamanho para o NossoNumero são 12 dígitos[m[41m[m
[32m+[m[32m            if (Convert.ToInt32(boleto.NossoNumero).ToString().Length > 6)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("A quantidade de dígitos do nosso número para a carteira " + boleto.Carteira + ", são 6 números.");[m[41m[m
[32m+[m[32m            else if (Convert.ToInt32(boleto.NossoNumero).ToString().Length < 6)[m[41m[m
[32m+[m[32m                boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 6);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (boleto.Carteira != "COB")[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Carteira não implementada. Utilize a carteira COB.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui o nome do banco ao local de pagamento[m[41m[m
[32m+[m[32m            boleto.LocalPagamento += Nome + "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do processamento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataProcessamento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataProcessamento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do documento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataDocumento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataDocumento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            FormataChave(boleto);[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m            FormataNossoNumero(boleto);[m[41m[m
[32m+[m[32m            FormataNumeroDocumento(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public void FormataChave(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string zeros = "000";[m[41m[m
[32m+[m[32m            string agencia = boleto.Cedente.ContaBancaria.Agencia;[m[41m[m
[32m+[m[32m            string conta = boleto.Cedente.ContaBancaria.Conta + boleto.Cedente.ContaBancaria.DigitoConta;[m[41m[m
[32m+[m[32m            int categoria = 1;[m[41m[m
[32m+[m[32m            boleto.Categoria = categoria;[m[41m[m
[32m+[m[32m            string nossonumero = boleto.NossoNumero;[m[41m[m
[32m+[m[32m            string banco = Utils.FormatCode(Codigo.ToString(), 3);[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m            //Mod10 dentro da classe Banco_BRB pelas particularidades que ela tem.[m[41m[m
[32m+[m[32m            int d1 = Banco_BRB.Mod10(zeros + agencia + conta + categoria + nossonumero + banco);[m[41m[m
[32m+[m[32m            int d2 = Banco_BRB.Mod11_NossoNumero(zeros + agencia + conta + categoria + nossonumero + banco + d1, 7);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (d2 > 10)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                d1 += 1;[m[41m[m
[32m+[m[32m                d2 -= 20;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Chave = zeros + agencia + conta + categoria + nossonumero + banco + d1 + d2;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static int Mod11_CodigoBarra(string value, int Base)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int Digito, Soma = 0, Peso = 2;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = value.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Soma = Soma + (Convert.ToInt32(Strings.Mid(value, i, 1)) * Peso);[m[41m[m
[32m+[m[32m                if (Peso == Base)[m[41m[m
[32m+[m[32m                    Peso = 2;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    Peso = Peso + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (((Soma % 11) == 0) || ((Soma % 11) == 10) || ((Soma % 11) == 1))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Digito = 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Digito = 11 - (Soma % 11);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m            return Digito;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static int Mod11_NossoNumero(string value, int Base)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int Digito, Soma = 0, Peso = 2;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = value.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Soma = Soma + (Convert.ToInt32(Strings.Mid(value, i, 1)) * Peso);[m[41m[m
[32m+[m[32m                if (Peso == Base)[m[41m[m
[32m+[m[32m                    Peso = 2;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    Peso = Peso + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m             if ((Soma % 11) > 1)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Digito = 11 - (Soma % 11);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else if ((Soma % 11) == 1)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                int d1 = Utils.ToInt32(Strings.Mid(value, value.Length, value.Length - 1));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                d1 += 1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (d1 == 10)[m[41m[m
[32m+[m[32m                    d1 = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Digito = Banco_BRB.Mod11_NossoNumero(Strings.Mid(value, 1, value.Length - 1) + d1, 7);[m[41m[m
[32m+[m[32m                Digito += 20;[m[41m               [m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m [m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Digito = (Soma % 11);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return Digito;[m[41m[m
[32m+[m[41m  [m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static int Mod10(string seq)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int Digito, Soma = 0, Peso = 2, res;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = seq.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                res = (Convert.ToInt32(Strings.Mid(seq, i, 1)) * Peso);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (res > 9)[m[41m[m
[32m+[m[32m                    res = (res - 9);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Soma += res;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (Peso == 2)[m[41m[m
[32m+[m[32m                    Peso = 1;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    Peso = Peso + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m            Digito = ((10 - (Soma % 10)) % 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return Digito;[m[41m[m
[32m+[m[32m       }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarHeaderRemessa(string numeroConvenio, Cedente cedente, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new Exception("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override DetalheRetorno LerDetalheRetornoCNAB400(string registro)[m[41m[m
