[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private List<DetalheRetorno> _listaDetalhe = new List<DetalheRetorno>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public List<DetalheRetorno> ListaDetalhe[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _listaDetalhe; }[m[41m[m
[32m+[m[32m            set { _listaDetalhe = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Construtores[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public ArquivoRetornoCNAB400()[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m[32m            this.TipoArquivo = TipoArquivo.CNAB400;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de instância[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void LerArquivoRetorno(IBanco banco, Stream arquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StreamReader stream = new StreamReader(arquivo, System.Text.Encoding.UTF8);[m[41m[m
[32m+[m[32m                string linha = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Lendo o arquivo[m[41m[m
[32m+[m[32m                linha = stream.ReadLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Próxima linha (DETALHE)[m[41m[m
[32m+[m[32m                linha = stream.ReadLine();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                while (DetalheRetorno.PrimeiroCaracter(linha) == "1")[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    DetalheRetorno detalhe = banco.LerDetalheRetornoCNAB400(linha);[m[41m[m
[32m+[m[32m                    ListaDetalhe.Add(detalhe);[m[41m[m
[32m+[m[32m                    OnLinhaLida(detalhe, linha);[m[41m[m
[32m+[m[32m                    linha = stream.ReadLine();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                stream.Close();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao ler arquivo.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
