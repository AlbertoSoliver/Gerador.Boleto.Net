[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Collections.Generic;[m[41m[m
[32m+[m[32musing System.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public class Instrucao : AbstractInstrucao, IInstrucao[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Variaveis[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private IInstrucao _IInstrucao;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Construtores[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal Instrucao()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public Instrucao(int CodigoBanco)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                InstanciaInstrucao(CodigoBanco);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao instanciar objeto.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Métodos Privados[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void InstanciaInstrucao(int codigoBanco)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                switch (codigoBanco)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //104 - Caixa[m[41m[m
[32m+[m[32m                    case 104:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Caixa();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //341 - Itaú[m[41m[m
[32m+[m[32m                    case 341:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Itau();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //1 - Banco do Brasil[m[41m[m
[32m+[m[32m                    case 1:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_BancoBrasil();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //356 - Real[m[41m[m
[32m+[m[32m                    case 356:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Real();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //422 - Safra[m[41m[m
[32m+[m[32m                    case 422:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Safra();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //237 - Bradesco[m[41m[m
[32m+[m[32m                    case 237:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Bradesco();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //347 - Sudameris[m[41m[m
[32m+[m[32m                    case 347:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Sudameris();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //353 - Santander[m[41m[m
[32m+[m[32m                    case 353:[m[41m[m
[32m+[m[32m                    case 33:[m[41m[m
[32m+[m[32m                    case 8:[m[41m[m
[32m+[m[32m                        //case 8:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Santander();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //070 - BRB[m[41m[m
[32m+[m[32m                    case 70:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_BRB();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //479 - BankBoston[m[41m[m
[32m+[m[32m                    case 479:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_BankBoston();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //41 - Banrisul[m[41m[m
[32m+[m[32m                    case 41:[m[41m[m
[32m+[m[32m                        _IInstrucao = new Instrucao_Banrisul();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        throw new Exception("Código do banco não implementando: " + codigoBanco);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a execução da transação.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Propriedades da interface[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override IBanco Banco[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IInstrucao.Banco; }[m[41m[m
[32m+[m[32m            set { _IInstrucao.Banco = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override int Codigo[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IInstrucao.Codigo; }[m[41m[m
[32m+[m[32m            set { _IInstrucao.Codigo = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string Descricao[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IInstrucao.Descricao; }[m[41m[m
[32m+[m[32m            set { _IInstrucao.Descricao = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override int QuantidadeDias[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IInstrucao.QuantidadeDias; }[m[41m[m
[32m+[m[32m        }[m[41m[m
