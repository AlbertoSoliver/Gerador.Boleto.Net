[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Web.UI;[m[41m[m
[32m+[m[32musing BoletoNet;[m[41m[m
[32m+[m[32musing Microsoft.VisualBasic;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m[assembly: WebResource("BoletoNet.Imagens.422.jpg", "image/jpg")][m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    /// <author>[m[41m  [m
[32m+[m[32m    /// Eduardo Frare[m[41m[m
[32m+[m[32m    /// Stiven[m[41m [m
[32m+[m[32m    /// Diogo[m[41m[m
[32m+[m[32m    /// Miamoto[m[41m[m
[32m+[m[32m    /// </author>[m[41m    [m
[32m+[m[41m[m
[32m+[m[32m    ///<summary>[m[41m[m
[32m+[m[32m    /// Classe referente ao Banco Banco_Safra[m[41m[m
[32m+[m[32m    ///</summary>[m[41m[m
[32m+[m[32m    internal class Banco_Safra : AbstractBanco, IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private string _dacNossoNumero = string.Empty;[m[41m[m
[32m+[m[32m        private int _dacContaCorrente = 0;[m[41m[m
[32m+[m[32m        private int _dacBoleto = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Classe responsavel em criar os campos do Banco Banco_Safra.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        internal Banco_Safra()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.Codigo = 422;[m[41m[m
[32m+[m[32m            this.Digito = 7;[m[41m[m
[32m+[m[32m            this.Nome = "Banco_Safra";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Calcula o digito do Nosso Numero[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string CalcularDigitoNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string sfCarteira = boleto.Carteira.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (boleto.NossoNumero.Length < 9)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new IndexOutOfRangeException("Erro. O campo 'Nosso Número' deve ter mais de 9 digitos. Você digitou " + boleto.NossoNumero);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            string sfNossoNumero = boleto.NossoNumero.Substring(0, 8);[m[41m[m
[32m+[m[32m            int sfDigitoNossoNumero = Mod11(sfNossoNumero, 9, 0);[m[41m[m
[32m+[m[32m            string sfDigito = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (sfDigitoNossoNumero == 0)[m[41m[m
[32m+[m[32m                sfDigito = "1";[m[41m[m
[32m+[m[32m            else if (sfDigitoNossoNumero > 1)[m[41m[m
[32m+[m[32m                sfDigito = Convert.ToString(11 - sfDigitoNossoNumero);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return sfDigito;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m       [m
[32m+[m[32m        /// SISTEMA	        020	020	7	FIXO[m[41m[m
[32m+[m[32m        /// CLIENTE	        021	034	CÓDIGO DO CLIENTE	CÓDIGO/AGÊNCIA CEDENTE[m[41m[m
[32m+[m[32m        /// N/NÚMERO	    035	043	NOSSO NÚMERO	NOSSO NÚMERO DO TÍTULO[m[41m[m
[32m+[m[32m        /// TIPO COBRANÇA	044	044	2	FIXO[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string CampoLivre(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string campolivre = "7" + boleto.Cedente.ContaBancaria.Conta.ToString() + boleto.Cedente.ContaBancaria.Agencia.ToString() +[m[41m[m
[32m+[m[32m                                boleto.NossoNumero.Substring(0, 9) + "2";[m[41m[m
[32m+[m[32m            return campolivre;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region IBanco Members[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Calcula o DAC do Nosso Número[m[41m[m
[32m+[m[32m            _dacNossoNumero = CalcularDigitoNossoNumero(boleto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Calcula o DAC da Conta Corrente[m[41m[m
[32m+[m[32m            _dacContaCorrente = Mod10(boleto.Cedente.ContaBancaria.Agencia + boleto.Cedente.ContaBancaria.Conta);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se o nosso número é válido[m[41m[m
[32m+[m[32m            if (Utils.ToInt64(boleto.NossoNumero) == 0)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Nosso número inválido");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do processamento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataProcessamento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataProcessamento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do documento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataDocumento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataDocumento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m            FormataNossoNumero(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNumeroDocumento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            //throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        ///	O código de barra para cobrança contém 44 posições dispostas da seguinte forma:[m[41m[m
[32m+[m[32m        ///    01 a 03 - 3 - Identificação  do  Banco[m[41m[m
[32m+[m[32m        ///    04 a 04 - 1 - Código da Moeda[m[41m[m
[32m+[m[32m        ///    05 a 05  1 - Dígito verificador do Código de Barras[m[41m[m
[32m+[m[32m        ///    06 a 19 - 14 - Valor[m[41m[m
[32m+[m[32m        ///    20 a 44  25 - Campo Livre[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string valorBoleto = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m            valorBoleto = Utils.FormatCode(valorBoleto, 14);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}", Codigo, boleto.Moeda,[m[41m[m
[32m+[m[32m                    FatorVencimento(boleto), valorBoleto, CampoLivre(boleto));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            _dacBoleto = 0;[m[41m[m
[32m+[m[32m            //Mod11(Boleto.CodigoBarra.Codigo.Substring(0, 3) + Boleto.CodigoBarra.Codigo.Substring(5, 43), 9, 0);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = Strings.Left(boleto.CodigoBarra.Codigo, 4) + _dacBoleto + Strings.Right(boleto.CodigoBarra.Codigo, 39);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// A linha digitável será composta por cinco campos:[m[41m[m
[32m+[m[32m        ///    1º CAMPO - Composto pelo código do banco ( sem o dígito verificador = 422 ),[m[41m [m
[32m+[m[32m        ///       código da moeda, as cinco primeiras posições do campo livre ou seja, da[m[41m [m
[32m+[m[32m        ///       posição 20 à 24 do código de barras, e mais um dígito verificador deste campo.[m[41m [m
[32m+[m[32m        ///       Após os 5 primeiros dígitos deste campo separar o conteúdo por um ponto ( . ).[m[41m [m
[32m+[m[32m        ///    2º CAMPO - Composto pelas posições 6 à 15 do campo livre ou seja, da[m[41m [m
[32m+[m[32m        ///       posição 25 à 34 do código de barras e mais um dígito verificador deste campo.[m[41m [m
[32m+[m[32m        ///       Após os 5 primeiros dígitos deste campo separar o conteúdo por um ponto ( . ).[m[41m[m
[32m+[m[32m        ///    3º CAMPO - Composto pelas posições 16 à 25 do campo livre ou seja, da[m[41m [m
[32m+[m[32m        ///       posição 35 à 44 do código de barras, e mais um dígito verificador deste campo.[m[41m [m
[32m+[m[32m        ///       Após os 5 primeiros dígitos deste campo separar o conteúdo por um ponto ( . ).[m[41m[m
[32m+[m[32m        ///    4º CAMPO  - Composto pelo dígito de autoconferência do código de barras.[m[41m[m
[32m+[m[32m        ///    5º CAMPO - Composto pelo valor nominal do documento ou seja, pelas[m[41m [m
[32m+[m[32m        ///       posições 06 à 19 do código de barras, com supressão de zeros a esquerda e[m[41m [m
[32m+[m[32m        ///       sem edição ( sem ponto e vírgula ). Quando se tratar de valor zerado, a[m[41m [m
[32m+[m[32m        ///       representação deverá ser 000 ( três zeros ).[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //AAABC.CCCCX DDDDD.DDDDDY EEEEE.EEEEEZ K VVVVVVVVVVVVVV[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string LD = string.Empty; //Linha Digitável[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Campo 1[m[41m[m
[32m+[m[32m            string AAA = Utils.FormatCode(Codigo.ToString(), 3);[m[41m[m
[32m+[m[32m            string B = boleto.Moeda.ToString();[m[41m[m
[32m+[m[32m            string CCCCC = CampoLivre(boleto).Substring(0, 4);[m[41m[m
[32m+[m[32m            string X = Mod10(AAA + B + CCCCC).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD = string.Format("{0}{1}{2}.", AAA, B, CCCCC.Substring(0, 1));[m[41m[m
[32m+[m[32m            LD += string.Format("{0}{1}", CCCCC.Substring(0, 4), X);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string DDDDDD = CampoLivre(boleto).Substring(6, 15);[m[41m[m
[32m+[m[32m            string Y = Mod10(DDDDDD).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += string.Format("{0}.", DDDDDD.Substring(0, 5));[m[41m[m
[32m+[m[32m            LD += string.Format("{0}{1} ", DDDDDD.Substring(5, 10), Y);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string EEEEE = CampoLivre(boleto).Substring(12, 10);[m[41m[m
[32m+[m[32m            string Z = Mod10(EEEEE).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += string.Format("{0}.", EEEEE.Substring(0, 5));[m[41m[m
[32m+[m[32m            LD += string.Format("{0}{1} ", EEEEE.Substring(5, 5), Z);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string K = _dacBoleto.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += string.Format(" {0} ", K);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Campo 5[m[41m[m
[32m+[m[32m            string VVVVVVVVVVVVVV;[m[41m[m
[32m+[m[32m            if (boleto.ValorBoleto != 0)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                VVVVVVVVVVVVVV = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m                VVVVVVVVVVVVVV = Utils.FormatCode(VVVVVVVVVVVVVV, 14);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                VVVVVVVVVVVVVV = "000";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            LD += VVVVVVVVVVVVVV;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion Campo 5[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel = LD;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion IBanco Members[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
