[32m+[m[32m        public virtual DetalheRetorno LerDetalheRetornoCNAB400(string registro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                int dataOcorrencia = Utils.ToInt32(registro.Substring(110, 6));[m[41m[m
[32m+[m[32m                int dataVencimento = Utils.ToInt32(registro.Substring(146, 6));[m[41m[m
[32m+[m[32m                int dataCredito = Utils.ToInt32(registro.Substring(295, 6));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                DetalheRetorno detalhe = new DetalheRetorno(registro);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                detalhe.CodigoInscricao = Utils.ToInt32(registro.Substring(1, 2));[m[41m[m
[32m+[m[32m                detalhe.NumeroInscricao = registro.Substring(3, 14);[m[41m[m
[32m+[m[32m                detalhe.Agencia = Utils.ToInt32(registro.Substring(17, 4));[m[41m[m
[32m+[m[32m                detalhe.Conta = Utils.ToInt32(registro.Substring(23, 5));[m[41m[m
[32m+[m[32m                detalhe.DACConta = Utils.ToInt32(registro.Substring(28, 1));[m[41m[m
[32m+[m[32m                detalhe.UsoEmpresa = registro.Substring(37, 25);[m[41m[m
[32m+[m[32m                detalhe.NossoNumero = registro.Substring(85, 8);[m[41m[m
[32m+[m[32m                detalhe.DACNossoNumero = Utils.ToInt32(registro.Substring(93, 1));[m[41m[m
[32m+[m[32m                detalhe.Carteira = registro.Substring(107, 1);[m[41m[m
[32m+[m[32m                detalhe.CodigoOcorrencia = Utils.ToInt32(registro.Substring(108, 2));[m[41m[m
[32m+[m[32m                detalhe.DataOcorrencia = Utils.ToDateTime(dataOcorrencia.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                detalhe.NumeroDocumento = registro.Substring(116, 10);[m[41m[m
[32m+[m[32m                detalhe.NossoNumero = registro.Substring(126, 9);[m[41m[m
[32m+[m[32m                detalhe.DataVencimento = Utils.ToDateTime(dataVencimento.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                double valorTitulo = Convert.ToInt64(registro.Substring(152, 13));[m[41m[m
[32m+[m[32m                detalhe.ValorTitulo = valorTitulo / 100;[m[41m[m
[32m+[m[32m                detalhe.CodigoBanco = Utils.ToInt32(registro.Substring(165, 3));[m[41m[m
[32m+[m[32m                detalhe.AgenciaCobradora = Utils.ToInt32(registro.Substring(168, 4));[m[41m[m
[32m+[m[32m                detalhe.Especie = Utils.ToInt32(registro.Substring(173, 2));[m[41m[m
[32m+[m[32m                double tarifaCobranca = Convert.ToUInt64(registro.Substring(174, 13));[m[41m[m
[32m+[m[32m                detalhe.TarifaCobranca = tarifaCobranca / 100;[m[41m[m
[32m+[m[32m                // 26 brancos[m[41m[m
[32m+[m[32m                double iof = Convert.ToUInt64(registro.Substring(214, 13));[m[41m[m
[32m+[m[32m                detalhe.IOF = iof / 100;[m[41m[m
[32m+[m[32m                double valorAbatimento = Convert.ToUInt64(registro.Substring(227, 13));[m[41m[m
[32m+[m[32m                detalhe.ValorAbatimento = valorAbatimento / 100;[m[41m[m
[32m+[m[32m                double valorPrincipal = Convert.ToUInt64(registro.Substring(253, 13));[m[41m[m
[32m+[m[32m                detalhe.ValorPrincipal = valorPrincipal / 100;[m[41m[m
[32m+[m[32m                double jurosMora = Convert.ToUInt64(registro.Substring(266, 13));[m[41m[m
[32m+[m[32m                detalhe.JurosMora = jurosMora / 100;[m[41m[m
[32m+[m[32m                detalhe.DataOcorrencia = Utils.ToDateTime(dataOcorrencia.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                // 293 - 3 brancos[m[41m[m
[32m+[m[32m                detalhe.DataCredito = Utils.ToDateTime(dataCredito.ToString("##-##-##"));[m[41m[m
[32m+[m[32m                detalhe.InstrucaoCancelada = Utils.ToInt32(registro.Substring(301, 4));[m[41m[m
[32m+[m[32m                // 306 - 6 brancos[m[41m[m
[32m+[m[32m                // 311 - 13 zeros[m[41m[m
[32m+[m[32m                detalhe.NomeSacado = registro.Substring(324, 30);[m[41m[m
[32m+[m[32m                // 354 - 23 brancos[m[41m[m
[32m+[m[32m                detalhe.Erros = registro.Substring(377, 8);[m[41m[m
[32m+[m[32m                // 377 - Registros rejeitados ou alegação do sacado[m[41m[m
[32m+[m[32m                // 386 - 7 brancos[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                detalhe.CodigoLiquidacao = registro.Substring(392, 2);[m[41m[m
[32m+[m[32m                detalhe.NumeroSequencial = Utils.ToInt32(registro.Substring(394, 6));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return detalhe;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao ler detalhe do arquivo de RETORNO / CNAB 400.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Fator de vencimento do boleto[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="boleto"></param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        protected static long FatorVencimento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            DateTime dateBase = new DateTime(2000, 7, 3, 0, 0, 0);[m[41m[m
[32m+[m[32m            return Utils.DateDiff(DateInterval.Day, dateBase, boleto.DataVencimento) + 1000;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        //internal static string CalcFatorVencimento(DateTime dtv)[m[41m[m
[32m+[m[32m        //{[m[41m[m
[32m+[m[32m        //    DateTime dateBase = new DateTime(2000, 7, 3, 0, 0, 0);[m[41m[m
[32m+[m[32m        //    return Convert.ToString(Utils.DateDiff(DateInterval.Day, dateBase, dtv) + 1000);[m[41m[m
[32m+[m[32m        //}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Mod[m[41m[m
[32m+[m[32m        internal static int Mod10(string seq)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            /* Variáveis[m[41m[m
[32m+[m[32m             * -------------[m[41m[m
[32m+[m[32m             * d - Dígito[m[41m[m
[32m+[m[32m             * s - Soma[m[41m[m
[32m+[m[32m             * p - Peso[m[41m[m
[32m+[m[32m             * b - Base[m[41m[m
[32m+[m[32m             * r - Resto[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int d, s = 0, p = 2, r;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = seq.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                r = (Convert.ToInt32(Microsoft.VisualBasic.Strings.Mid(seq, i, 1)) * p);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (r > 9)[m[41m[m
[32m+[m[32m                    r = (r / 10) + (r % 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                s += r;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (p == 2)[m[41m[m
[32m+[m[32m                    p = 1;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    p = p + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            d = ((10 - (s % 10)) % 10);[m[41m[m
[32m+[m[32m            return d;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected static int Mod11(string seq)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            /* Variáveis[m[41m[m
[32m+[m[32m             * -------------[m[41m[m
[32m+[m[32m             * d - Dígito[m[41m[m
[32m+[m[32m             * s - Soma[m[41m[m
[32m+[m[32m             * p - Peso[m[41m[m
[32m+[m[32m             * b - Base[m[41m[m
[32m+[m[32m             * r - Resto[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int d, s = 0, p = 2, b = 9;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = 0; i < seq.Length; i++)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                s = s + (Convert.ToInt32(seq[i]) * p);[m[41m[m
[32m+[m[32m                if (p < b)[m[41m[m
[32m+[m[32m                    p = p + 1;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    p = 2;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            d = 11 - (s % 11);[m[41m[m
[32m+[m[32m            if (d > 9)[m[41m[m
[32m+[m[32m                d = 0;[m[41m[m
[32m+[m[32m            return d;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected static int Mod11(string seq, int b)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            /* Variáveis[m[41m[m
[32m+[m[32m             * -------------[m[41m[m
[32m+[m[32m             * d - Dígito[m[41m[m
[32m+[m[32m             * s - Soma[m[41m[m
[32m+[m[32m             * p - Peso[m[41m[m
[32m+[m[32m             * b - Base[m[41m[m
[32m+[m[32m             * r - Resto[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int d, s = 0, p = 2;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = seq.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                s = s + (Convert.ToInt32(Microsoft.VisualBasic.Strings.Mid(seq, i, 1)) * p);[m[41m[m
[32m+[m[32m                if (p == b)[m[41m[m
[32m+[m[32m                    p = 2;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    p = p + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            d = 11 - (s % 11);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if ((d > 9) || (d == 0) || (d == 1))[m[41m[m
[32m+[m[32m                d = 1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return d;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected static int Mod11Base9(string seq)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            /* Variáveis[m[41m[m
[32m+[m[32m             * -------------[m[41m[m
[32m+[m[32m             * d - Dígito[m[41m[m
[32m+[m[32m             * s - Soma[m[41m[m
[32m+[m[32m             * p - Peso[m[41m[m
[32m+[m[32m             * b - Base[m[41m[m
[32m+[m[32m             * r - Resto[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int d, s = 0, p = 2, b = 9;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = seq.Length - 1; i >= 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string aux = Convert.ToString(seq[i]);[m[41m[m
[32m+[m[32m                s += (Convert.ToInt32(aux) * p);[m[41m[m
[32m+[m[32m                if (p >= b)[m[41m[m
[32m+[m[32m                    p = 2;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    p = p + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (s < 11)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                d = 11 - s;[m[41m[m
[32m+[m[32m                return d;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                d = 11 - (s % 11);[m[41m[m
[32m+[m[32m                if ((d > 9) || (d == 0))[m[41m[m
[32m+[m[32m                    d = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return d;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected static int Mod11(string seq, int lim, int flag)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int mult = 0;[m[41m[m
[32m+[m[32m            int total = 0;[m[41m[m
[32m+[m[32m            int pos = 1;[m[41m[m
[32m+[m[32m            //int res = 0;[m[41m[m
[32m+[m[32m            int ndig = 0;[m[41m[m
[32m+[m[32m            int nresto = 0;[m[41m[m
[32m+[m[32m            string num = string.Empty;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            mult = 1 + (seq.Length % (lim - 1));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (mult == 1)[m[41m[m
[32m+[m[32m                mult = lim;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            while (pos <= seq.Length)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                num = Microsoft.VisualBasic.Strings.Mid(seq, pos, 1);[m[41m[m
[32m+[m[32m                total += Convert.ToInt32(num) * mult;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                mult -= 1;[m[41m[m
[32m+[m[32m                if (mult == 1)[m[41m[m
[32m+[m[32m                    mult = lim;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                pos += 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            nresto = (total % 11);[m[41m[m
[32m+[m[32m            if (flag == 1)[m[41m[m
[32m+[m[32m                return nresto;[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (nresto == 0 || nresto == 1 || nresto == 10)[m[41m[m
[32m+[m[32m                    ndig = 1;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    ndig = (11 - nresto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return ndig;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        protected static int Mult10Mod11(string seq, int lim, int flag)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            int mult = 0;[m[41m[m
[32m+[m[32m            int total = 0;[m[41m[m
[32m+[m[32m            int pos = 1;[m[41m[m
[32m+[m[32m            int ndig = 0;[m[41m[m
[32m+[m[32m            int nresto = 0;[m[41m[m
[32m+[m[32m            string num = string.Empty;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            mult = 1 + (seq.Length % (lim - 1));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (mult == 1)[m[41m[m
[32m+[m[32m                mult = lim;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            while (pos <= seq.Length)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                num = Microsoft.VisualBasic.Strings.Mid(seq, pos, 1);[m[41m[m
[32m+[m[32m                total += Convert.ToInt32(num) * mult;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                mult -= 1;[m[41m[m
[32m+[m[32m                if (mult == 1)[m[41m[m
[32m+[m[32m                    mult = lim;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                pos += 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            nresto = ((total * 10) % 11);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (flag == 1)[m[41m[m
[32m+[m[32m                return nresto;[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (nresto == 0 || nresto == 1)[m[41m[m
[32m+[m[32m                    nresto = 0;[m[41m[m
[32m+[m[32m                else if (nresto == 10)[m[41m[m
[32m+[m[32m                    ndig = 1;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    ndig = (11 - nresto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return ndig;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion Mod[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
