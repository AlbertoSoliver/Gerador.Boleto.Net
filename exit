[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion DETALHE[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region TRAILER[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// TRAILER do arquivo CNAB[m[41m[m
[32m+[m[32m        /// Gera o TRAILER do arquivo remessa de acordo com o lay-out informado[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override string GerarTrailerRemessa(int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _trailer = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                base.GerarTrailerRemessa(numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        _trailer = GerarTrailerRemessa240();[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        _trailer = GerarTrailerRemessa400(numeroRegistro);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _trailer;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarTrailerRemessa240()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarTrailerRemessa400(int numeroRegistro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string complemento = new string(' ', 393);[m[41m[m
[32m+[m[32m                string _trailer;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _trailer = "9";[m[41m[m
[32m+[m[32m                _trailer += complemento;[m[41m[m
[32m+[m[32m                _trailer += Utils.FitStringLength(numeroRegistro.ToString(), 6, 6, '0', 0, true, true, true); // Número sequencial do registro no arquivo.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _trailer = Utils.SubstituiCaracteresEspeciais(_trailer);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _trailer;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do registro TRAILER do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion Seygi Gerando Remessa[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
