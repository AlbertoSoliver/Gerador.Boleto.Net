[32m+[m[41m[m
[32m+[m[32m                    //Posição 30[m[41m[m
[32m+[m[32m                    string primeiraConstante = boleto.Carteira;[m[41m[m
[32m+[m[32m                    if (boleto.Carteira == "SR")[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        primeiraConstante = "2"; //1 => cobrança registrada, 2=> cobrança sem registro[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Posição 31 - 33[m[41m[m
