[32m+[m[32m    internal class Banco_Basa : AbstractBanco, IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private string _dacNossoNumero = string.Empty;[m[41m[m
[32m+[m[32m        private int _dacBoleto = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Classe responsavel em criar os campos do Banco Banco_Bradesco.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        internal Banco_Basa()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.Codigo = 3;[m[41m[m
[32m+[m[32m            this.Digito = 5;[m[41m[m
[32m+[m[32m            this.Nome = "Basa";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region IBanco Members[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string nossonumero = boleto.NossoNumero + "000000";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // grupo 1:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string banco = Utils.FormatCode(Codigo.ToString(), 3);[m[41m[m
[32m+[m[32m            string moeda = boleto.Moeda.ToString("0");[m[41m[m
[32m+[m[32m            string agencia = Strings.Right(boleto.Cedente.ContaBancaria.Agencia, 3) + boleto.Cedente.ContaBancaria.DigitoAgencia;[m[41m[m
[32m+[m[32m            string convenio_parte1 = boleto.Cedente.Codigo.ToString().Substring(0, 1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string dv1 = Mod10_LinhaDigitavel(banco + moeda + agencia + convenio_parte1).ToString("0");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string grupo1 = banco + moeda + agencia + convenio_parte1 + dv1;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // grupo 2:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string convenio_parte2 = boleto.Cedente.Codigo.ToString().Substring(1, 3);[m[41m[m
[32m+[m[32m            string nossonumero_parte1 = nossonumero.Substring(0, 7);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string dv2 = Mod10_LinhaDigitavel(convenio_parte2 + nossonumero_parte1).ToString("0");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string grupo2 = convenio_parte2 + nossonumero_parte1 + dv2;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // grupo 3:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string nossonumero_parte2 = nossonumero.Substring(7, 9);[m[41m[m
[32m+[m[32m            string identificadorsistema = "8";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string dv3 = Mod10_LinhaDigitavel(nossonumero_parte2 + identificadorsistema).ToString("0");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string grupo3 = nossonumero_parte2 + identificadorsistema + dv3;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // grupo 4:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string dv_codigobarra = boleto.CodigoBarra.Codigo.Substring(4, 1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string grupo4 = dv_codigobarra;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // grupo 5:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string fatorvencimento = FatorVencimento(boleto).ToString();[m[41m[m
[32m+[m[32m            string valordocumento = Utils.FormatCode(boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", ""), 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string grupo5 = fatorvencimento + valordocumento;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string L = grupo1 + grupo2 + grupo3 + grupo4 + grupo5;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel =[m[41m[m
[32m+[m[32m                L.Substring(0, 5) + "." +[m[41m[m
[32m+[m[32m                L.Substring(5, 5) + " " +[m[41m[m
[32m+[m[32m                L.Substring(10, 5) + "." +[m[41m[m
[32m+[m[32m                L.Substring(15, 6) + "  " +[m[41m[m
[32m+[m[32m                L.Substring(21, 5) + "." +[m[41m[m
[32m+[m[32m                L.Substring(26, 6) + " " +[m[41m[m
[32m+[m[32m                L.Substring(32, 1) + " " +[m[41m[m
[32m+[m[32m                L.Substring(33, 14);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            // Código de Barras[m[41m[m
[32m+[m[32m            //banco & moeda & fator & valor & carteira & nossonumero & dac_nossonumero & agencia & conta & dac_conta & "000"[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string banco = Utils.FormatCode(Codigo.ToString(), 3);[m[41m[m
[32m+[m[32m            int moeda = boleto.Moeda;[m[41m[m
[32m+[m[32m            //string digito = "";[m[41m[m
[32m+[m[32m            string valorBoleto = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m            valorBoleto = Utils.FormatCode(valorBoleto, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string fatorvencimento = FatorVencimento(boleto).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string agencia = Strings.Right(boleto.Cedente.ContaBancaria.Agencia, 3) + boleto.Cedente.ContaBancaria.DigitoAgencia;[m[41m[m
[32m+[m[32m            string convenio = boleto.Cedente.Codigo.ToString("0000");[m[41m[m
[32m+[m[32m            string nossonumero = boleto.NossoNumero + "000000";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string sistemaarrecadacao = "8";  // Este numero foi fornecido pelo BASA para o convenio testado.. nao sei se muda.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}",[m[41m[m
[32m+[m[32m                banco, moeda, fatorvencimento, valorBoleto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo += string.Format("{0}{1}{2}{3}",[m[41m[m
[32m+[m[32m                agencia, convenio, nossonumero, sistemaarrecadacao);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            _dacBoleto = Mod11_CodigoBarra(boleto.CodigoBarra.Codigo, 9);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = Strings.Left(boleto.CodigoBarra.Codigo, 4) + _dacBoleto + Strings.Right(boleto.CodigoBarra.Codigo, 39);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
