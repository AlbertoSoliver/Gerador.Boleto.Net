[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de Instância[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Validações particulares do Banco do Brasil[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (string.IsNullOrEmpty(boleto.Carteira))[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Carteira não informada. Utilize a carteira 16, 17, 18, 18-019, 18-027, 18-035 ou 18-140.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica as carteiras implementadas[m[41m[m
[32m+[m[32m            if (!boleto.Carteira.Equals("16") &[m[41m[m
[32m+[m[32m                !boleto.Carteira.Equals("17") &[m[41m[m
[32m+[m[32m                !boleto.Carteira.Equals("18") &[m[41m[m
[32m+[m[32m                !boleto.Carteira.Equals("18-019") &[m[41m[m
[32m+[m[32m                !boleto.Carteira.Equals("18-027") &[m[41m[m
[32m+[m[32m                !boleto.Carteira.Equals("18-035") &[m[41m[m
[32m+[m[32m                !boleto.Carteira.Equals("18-140"))[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Carteira não informada. Utilize a carteira 16, 17, 18, 18-019, 18-027, 18-035 ou 18-140.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se o nosso número é válido[m[41m[m
[32m+[m[32m            if (Utils.ToString(boleto.NossoNumero) == string.Empty)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Nosso número inválido");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 16[m[41m[m
[32m+[m[32m            //Carteira 18 com nosso número de 11 posições[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("16"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (!boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.NossoNumero.Length > 11)[m[41m[m
[32m+[m[32m                        throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 11 de posições para o nosso número", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 11));[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 11);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.Cedente.Convenio.ToString().Length != 6)[m[41m[m
[32m+[m[32m                        throw new NotImplementedException(string.Format("Para a carteira {0} e o tipo da modalidade 21, o número do convênio são de 6 posições", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 17);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 16[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 17[m[41m[m
[32m+[m[32m            //Carteira 17[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("17"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                switch (boleto.Cedente.Convenio.ToString().Length)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //O BB manda como padrão 7 posições, mas é possível solicitar um convênio com 6 posições na carteira 17[m[41m[m
[32m+[m[32m                    case 6:[m[41m[m
[32m+[m[32m                        if (boleto.NossoNumero.Length > 12)[m[41m[m
[32m+[m[32m                            throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 12 de posições para o nosso número", boleto.Carteira));[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 12);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case 7:[m[41m[m
[32m+[m[32m                        if (boleto.NossoNumero.Length > 17)[m[41m[m
[32m+[m[32m                            throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 10 de posições para o nosso número", boleto.Carteira));[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 10));[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        throw new NotImplementedException(string.Format("Para a carteira {0}, o número do convênio deve ter 6 ou 7 posições", boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 17[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 18[m[41m[m
[32m+[m[32m            //Carteira 18 com nosso número de 11 posições[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18"))[m[41m[m
[32m+[m[32m                boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 11);[m[41m[m
[32m+[m[32m            #endregion Carteira 18[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 18-019[m[41m[m
[32m+[m[32m            //Carteira 18, com variação 019[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18-019"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                /*[m[41m[m
[32m+[m[32m                 * Convênio de 7 posições[m[41m[m
[32m+[m[32m                 * Nosso Número com 17 posições[m[41m[m
[32m+[m[32m                 */[m[41m[m
[32m+[m[32m                if (boleto.Cedente.Convenio.ToString().Length == 7)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.NossoNumero.Length > 10)[m[41m[m
[32m+[m[32m                        throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 10 de posições para o nosso número", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 10));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                /*[m[41m[m
[32m+[m[32m                 * Convênio de 6 posições[m[41m[m
[32m+[m[32m                 * Nosso Número com 11 posições[m[41m[m
[32m+[m[32m                 */[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //Modalidades de Cobrança Sem Registro  Carteira 16 e 18[m[41m[m
[32m+[m[32m                    //Nosso Número com 17 posições[m[41m[m
[32m+[m[32m                    if (!boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        if ((boleto.Cedente.Codigo.ToString().Length + boleto.NossoNumero.Length) > 11)[m[41m[m
[32m+[m[32m                            throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 11 de posições para o nosso número. Onde o nosso número é formado por CCCCCCNNNNN-X: C -> número do convênio fornecido pelo Banco, N -> seqüencial atribuído pelo cliente e X -> dígito verificador do Nosso-Número.", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 5));[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        if (boleto.Cedente.Convenio.ToString().Length != 6)[m[41m[m
[32m+[m[32m                            throw new NotImplementedException(string.Format("Para a carteira {0} e o tipo da modalidade 21, o número do convênio são de 6 posições", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 17);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                /*[m[41m[m
[32m+[m[32m                  * Convênio de 4 posições[m[41m[m
[32m+[m[32m                  * Nosso Número com 11 posições[m[41m[m
[32m+[m[32m                  */[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 4)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.NossoNumero.Length > 7)[m[41m[m
[32m+[m[32m                        throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 7 de posições para o nosso número [{1}]", boleto.Carteira, boleto.NossoNumero));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 7));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 11);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 18-019[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Para atender o cliente Fiemg foi adaptado no código na variação 18-027 as variações 18-035 e 18-140[m[41m[m
[32m+[m[32m            #region Carteira 18-027[m[41m[m
[32m+[m[32m            //Carteira 18, com variação 019[m[41m[m
