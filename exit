[32m+[m[32m        CustasDeProtesto = 8,[m[41m[m
[32m+[m[32m        CustasDeSustacaoDeProtesto = 9,[m[41m[m
[32m+[m[32m        CustasDoCartorioDistribuidor = 10,[m[41m[m
[32m+[m[32m        CustasDeEdital = 11,[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    #endregion[m[41m [m
[32m+[m[41m[m
[32m+[m[32m    public class CodigoTarifas_BancoBrasil: AbstractCodigoTarifas, ICodigoTarifas[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        #region Construtores[m[41m [m
[32m+[m[41m[m
[32m+[m		[32mpublic CodigoTarifas_BancoBrasil()[m[41m[m
[32m+[m		[32m{[m[41m[m
[32m+[m			[32mtry[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m			[32mcatch (Exception ex)[m[41m[m
[32m+[m			[32m{[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao carregar objeto", ex);[m[41m[m
[32m+[m			[32m}[m[41m[m
[32m+[m		[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public CodigoTarifas_BancoBrasil(int codigo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.carregar(codigo);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao carregar objeto", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m		[32m#endregion[m[41m [m
[32m+[m[41m[m
[32m+[m[32m        #region Metodos Privados[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void carregar(int codigo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.Banco = new Banco_Brasil();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch ((EnumCodigoTarifas_BancoBrasil)codigo)[m[41m[m
[32m+[m[32m                {[m[41m[m
