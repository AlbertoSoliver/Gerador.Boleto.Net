[32m+[m[41m[m
[32m+[m[32m                //Espécie Título[m[41m[m
[32m+[m[32m                detalhe.EspecieTitulo = registro.Substring(177, 2);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Despesas de cobrança para os Códigos de Ocorrência (Valor Despesa)[m[41m[m
[32m+[m[32m                double despeasaDeCobranca = Convert.ToUInt64(registro.Substring(179, 13));[m[41m[m
[32m+[m[32m                detalhe.DespeasaDeCobranca = despeasaDeCobranca / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Outras despesas Custas de Protesto (Valor Outras Despesas)[m[41m[m
[32m+[m[32m                double outrasDespesas = Convert.ToUInt64(registro.Substring(192, 13));[m[41m[m
[32m+[m[32m                detalhe.OutrasDespesas = outrasDespesas / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Juros Mora[m[41m[m
[32m+[m[32m                double juros = Convert.ToUInt64(registro.Substring(205, 13));[m[41m[m
[32m+[m[32m                detalhe.Juros = juros / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // IOF[m[41m[m
[32m+[m[32m                double iof = Convert.ToUInt64(registro.Substring(218, 13));[m[41m[m
[32m+[m[32m                detalhe.IOF = iof / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Abatimento Concedido sobre o Título (Valor Abatimento Concedido)[m[41m[m
[32m+[m[32m                double abatimento = Convert.ToUInt64(registro.Substring(231, 13));[m[41m[m
[32m+[m[32m                detalhe.Abatimentos = abatimento / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Desconto Concedido (Valor Desconto Concedido)[m[41m[m
[32m+[m[32m                double desconto = Convert.ToUInt64(registro.Substring(244, 13));[m[41m[m
[32m+[m[32m                detalhe.Descontos = desconto / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Valor Pago[m[41m[m
[32m+[m[32m                double valorPago = Convert.ToUInt64(registro.Substring(257, 13));[m[41m[m
[32m+[m[32m                detalhe.ValorPago = valorPago / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Outros Débitos[m[41m[m
[32m+[m[32m                double outrosDebitos = Convert.ToUInt64(registro.Substring(270, 13));[m[41m[m
[32m+[m[32m                detalhe.OutrosDebitos = outrosDebitos / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Outros Créditos[m[41m[m
[32m+[m[32m                double outrosCreditos = Convert.ToUInt64(registro.Substring(283, 13));[m[41m[m
[32m+[m[32m                detalhe.OutrosCreditos = outrosCreditos / 100;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Data de Liquidação[m[41m[m
[32m+[m[32m                int dataLiquidacao = Utils.ToInt32(registro.Substring(299, 8));[m[41m[m
[32m+[m[32m                detalhe.DataLiquidacao = Utils.ToDateTime(dataLiquidacao.ToString("##-##-####"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Motivo de Rejeição[m[41m[m
[32m+[m[32m                detalhe.MotivosRejeicao = registro.Substring(364, 30);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Motivo de Rejeição[m[41m[m
[32m+[m[32m                detalhe.Sequencial = Utils.ToInt32(registro.Substring(394, 6));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return detalhe;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao ler detalhe do arquivo de RETORNO / CNAB 400.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
