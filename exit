[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Retorna HTML representativo de todo conteudo[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public String GeraHTML(Boolean novaLinha)[m[41m[m
[32m+[m[32m        {[m[41m   [m
[32m+[m[32m            String rtn = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (this.Count > 0)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                foreach (InfoSacado I in this)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    rtn += I.HTML;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                if (!novaLinha) rtn = rtn.Substring(6);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return rtn;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
