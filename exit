[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            private int m_BrowserHeight;[m[41m[m
[32m+[m[32m            public int BrowserHeight[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                get[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return m_BrowserHeight;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                set[m[41m[m
