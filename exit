[32m+[m[32m        {[m[41m[m
[32m+[m[32m            InitializeComponent();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void Progresso_FormClosing(object sender, FormClosingEventArgs e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (e.CloseReason == CloseReason.UserClosing & _allowClose)[m[41m[m
[32m+[m[32m                e.Cancel = true;[m[41m[m
[32m+[m[32m        }[m[41m[m
