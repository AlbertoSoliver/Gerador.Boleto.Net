[32m+[m[32m        public override string GerarTrailerLoteRemessa(int numeroRegistro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _brancos92 = new string('0', 92);[m[41m[m
