[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao carregar objeto", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static EspeciesDocumento CarregaTodas()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            EspeciesDocumento especiesDocumento = new EspeciesDocumento();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            foreach (EnumEspecieDocumento_Sudameris item in Enum.GetValues(typeof(EnumEspecieDocumento_Sudameris)))[m[41m[m
[32m+[m[32m                especiesDocumento.Add(new EspecieDocumento_Sudameris((int)item));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return especiesDocumento;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
