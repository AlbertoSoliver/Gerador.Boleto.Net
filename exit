[32m+[m[32m        #region Propriedades da interface[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override IBanco Banco[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IEspecieDocumento.Banco; }[m[41m[m
[32m+[m[32m            set { _IEspecieDocumento.Banco = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override int Codigo[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _IEspecieDocumento.Codigo; }[m[41m[m
[32m+[m[32m            set { _IEspecieDocumento.Codigo = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
