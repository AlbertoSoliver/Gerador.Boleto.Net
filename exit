[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Métodos Privados[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private void InstanciaCarteira(int codigoBanco, int codigoCarteira)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                switch (codigoBanco)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //341 - Itaú[m[41m[m
[32m+[m[32m                    case 341:[m[41m[m
[32m+[m[32m                        _ICarteira = new Carteira_Itau(0);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //356 - BankBoston[m[41m[m
[32m+[m[32m                    case 479:[m[41m[m
[32m+[m[32m                        _ICarteira = new Carteira_BankBoston(0);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    //422 - Safra[m[41m[m
[32m+[m[32m                    case 1:[m[41m[m
[32m+[m[32m                        _ICarteira = new Carteira_BancoBrasil(codigoCarteira);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        throw new Exception("Código do banco não implementando: " + codigoBanco);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a execução da transação.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static Carteiras CarregaTodas(int codigoBanco)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                switch (codigoBanco)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case 1:[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        Carteiras alCarteiras = new Carteiras();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        Carteira_BancoBrasil obj;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        obj = new Carteira_BancoBrasil((int)EnumCarteiras_BancoBrasil.CobrancaSimples);[m[41m[m
[32m+[m[32m                        alCarteiras.Add(obj);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        obj = new Carteira_BancoBrasil((int)EnumCarteiras_BancoBrasil.CobrancaVinculada);[m[41m[m
[32m+[m[32m                        alCarteiras.Add(obj);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        obj = new Carteira_BancoBrasil((int)EnumCarteiras_BancoBrasil.CobrancaCaucionada);[m[41m[m
[32m+[m[32m                        alCarteiras.Add(obj);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        obj = new Carteira_BancoBrasil((int)EnumCarteiras_BancoBrasil.CobrancaDescontada);[m[41m[m
[32m+[m[32m                        alCarteiras.Add(obj);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        obj = new Carteira_BancoBrasil((int)EnumCarteiras_BancoBrasil.CobrançaDiretaEspecialCarteira17);[m[41m[m
[32m+[m[32m                        alCarteiras.Add(obj);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        return alCarteiras;[m[41m[m
[32m+[m[32m                    default:[m[41m[m
[32m+[m[32m                        throw new Exception("Código do banco não implementando: " + codigoBanco);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao listar objetos", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
