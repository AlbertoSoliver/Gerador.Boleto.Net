[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string CPFCNPJ[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return _cpfcnpj.Replace(".", "").Replace("-", "").Replace("/", "");[m[41m[m
[32m+[m[32m                throw new ArgumentException("Informe um CPF ou CNPJ Valido.");[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string o = value.Replace(".", "").Replace("-", "").Replace("/", "");[m[41m[m
[32m+[m[32m                if (o == null || (o.Length != 11 && o.Length != 14))[m[41m[m
[32m+[m[32m                    throw new ArgumentException("O CPF/CNPJ inválido. Utilize 11 dígitos para CPF ou 14 para CPNJ.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _cpfcnpj = value;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
