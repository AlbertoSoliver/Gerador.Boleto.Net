[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                strline = banco.GerarTrailerRemessa(numeroRegistro, TipoArquivo.CNAB400);[m[41m[m
[32m+[m[32m                incluiLinha.WriteLine(strline);[m[41m[m
