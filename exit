[32m+[m[32m    public partial class Main : Form[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        #region ImpressaoBoleto[m[41m[m
[32m+[m[32m        private short _codigoBanco = 0;[m[41m[m
[32m+[m[32m        private Progresso _progresso;[m[41m[m
[32m+[m[32m        string _arquivo = string.Empty;[m[41m[m
[32m+[m[32m        private ImpressaoBoleto _impressaoBoleto = new ImpressaoBoleto();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public short CodigoBanco[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _codigoBanco; }[m[41m[m
[32m+[m[32m            set { _codigoBanco = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        void _impressaoBoleto_FormClosed(object sender, FormClosedEventArgs e)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region GERA LAYOUT DO BOLETO[m[41m[m
[32m+[m[32m        private void GeraLayout(List<BoletoBancario> boletos)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                StringBuilder html = new StringBuilder();[m[41m[m
[32m+[m[32m                foreach (BoletoBancario o in boletos)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    html.Append(o.MontaHtml());[m[41m[m
[32m+[m[32m                    html.Append("</br></br></br></br></br></br></br></br></br></br>");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _arquivo = System.IO.Path.GetTempFileName();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                using (FileStream f = new FileStream(_arquivo, FileMode.Create))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    StreamWriter w = new StreamWriter(f, System.Text.Encoding.Default);[m[41m[m
[32m+[m[32m                    w.Write(html.ToString());[m[41m[m
[32m+[m[32m                    w.Close();[m[41m[m
[32m+[m[32m                    f.Close();[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Bancos[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region BOLETO Caixa[m[41m[m
[32m+[m[32m        private void GeraBoletoCaixa(int qtde)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                // Cria o boleto, e passa os parâmetros usuais[m[41m[m
[32m+[m[32m                BoletoBancario bb;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                List<BoletoBancario> boletos = new List<BoletoBancario>();[m[41m[m
[32m+[m[32m                for (int i = 0; i < qtde; i++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    #region Classes[m[41m[m
[32m+[m[32m                    CedenteDTO dtoced = new CedenteDTO();[m[41m[m
[32m+[m[32m                    dtoced.nome = NOMEtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoced.cpfcnpj = CPFtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoced.agencia = AGtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoced.conta = CCtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    SacadoDTO dtosac = new SacadoDTO();[m[41m[m
[32m+[m[32m                    dtosac.cpfcnpj = cpfsactextBox.Text;[m[41m[m
[32m+[m[32m                    dtosac.nome = nomesactextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    EnderecoDTO dtoend = new EnderecoDTO();[m[41m[m
[32m+[m[32m                    dtoend.End = endtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.Bairro = bairrotextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.Cidade = CidadetextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.CEP = CEPtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.UF = UFtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    //Instrucao_Caixa item1 = new Instrucao_Caixa(9, 5);[m[41m[m
[32m+[m[32m                    //Instrucao_Caixa item2 = new Instrucao_Caixa(81, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    double valorboleto = Convert.ToDouble(ValorBoletotextBox.Text);[m[41m[m
[32m+[m[41m                    [m
[32m+[m[32m                    string NossoNumero = NossoNumerotextBox.Text;[m[41m[m
[32m+[m[32m                    string NumeroDocumento = NumeroDocumentotextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb = new BoletoBancario();[m[41m[m
[32m+[m[32m                    bb.CodigoBanco = _codigoBanco;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Cedente c = new Cedente(dtoced.cpfcnpj, dtoced.nome, dtoced.agencia, dtoced.conta);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    #region Vencimento[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        DateTime vencimento = DateTime.Now;[m[41m[m
[32m+[m[32m                        string datatextbox = VencimentoTextbox.Text.Replace(" ", "").Replace("/", "");[m[41m[m
[32m+[m[32m                        DateTime vencimentodotextbox = Convert.ToDateTime(datatextbox);[m[41m[m
[32m+[m[32m                        vencimento = vencimentodotextbox;[m[41m[m
[32m+[m[32m                        DateTime _dia = DateTime.Now;[m[41m[m
[32m+[m[32m                        DateTime vencimentoem5dias = Convert.ToDateTime(_dia.AddDays(5).ToString("dd/MM/yyyy"));[m[41m[m
[32m+[m[41m[m
[32m+[m[41m                   [m
[32m+[m[32m                    #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Boleto b = new Boleto(vencimento, valorboleto, "SR", NossoNumero, c);[m[41m[m
[32m+[m[32m                    b.NumeroDocumento = NumeroDocumento;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    b.Sacado = new Sacado(dtosac.cpfcnpj, dtosac.nome);[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.End = dtoend.End;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Bairro = dtoend.Bairro;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Cidade = dtoend.Cidade;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.CEP = dtoend.CEP;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.UF = dtoend.UF;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    string instrucoes = instrucoestextBox.Text.ToUpper();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Instrucao instr = new Instrucao(001);[m[41m[m
[32m+[m[32m                    instr.Descricao = instrucoes;[m[41m[m
[32m+[m[32m                    b.Instrucoes.Add(instr);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb.Boleto = b;[m[41m[m
[32m+[m[32m                    bb.Boleto.Valida();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boletos.Add(bb);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                GeraLayout(boletos);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region BOLETO ITAÚ[m[41m[m
[32m+[m[32m        private void GeraBoletoItau(int qtde)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                // Cria o boleto, e passa os parâmetros usuais[m[41m[m
[32m+[m[32m                BoletoBancario bb;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                List<BoletoBancario> boletos = new List<BoletoBancario>();[m[41m[m
[32m+[m[32m                for (int i = 0; i < qtde; i++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    CedenteDTO dtoced = new CedenteDTO();[m[41m[m
[32m+[m[32m                    dtoced.nome = NOMEtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoced.cpfcnpj = CPFtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoced.agencia = AGtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoced.conta = CCtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    SacadoDTO dtosac = new SacadoDTO();[m[41m[m
[32m+[m[32m                    dtosac.cpfcnpj = cpfsactextBox.Text;[m[41m[m
[32m+[m[32m                    dtosac.nome = nomesactextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    EnderecoDTO dtoend = new EnderecoDTO();[m[41m[m
[32m+[m[32m                    dtoend.End = endtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.Bairro = bairrotextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.Cidade = CidadetextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.CEP = CEPtextBox.Text;[m[41m[m
[32m+[m[32m                    dtoend.UF = UFtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb = new BoletoBancario();[m[41m[m
[32m+[m[32m                    bb.CodigoBanco = _codigoBanco;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    DateTime _dia = DateTime.Now;[m[41m[m
[32m+[m[32m                    DateTime vencimento = Convert.ToDateTime(_dia.AddDays(5).ToString("dd/MM/yyyy"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Cedente c = new Cedente(dtoced.cpfcnpj, dtoced.nome, dtoced.agencia, dtoced.conta);[m[41m[m
[32m+[m[32m                    //Na carteira 198 o código do Cedente é a conta bancária[m[41m[m
[32m+[m[32m                    c.Codigo = Convert.ToInt32(dtoced.conta.ToString());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    string carteira = "198";[m[41m[m
[32m+[m[32m                    double valorboleto = Convert.ToDouble(ValorBoletotextBox.Text);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Boleto b = new Boleto(vencimento, valorboleto, carteira, "00000865425", c, new EspecieDocumento(341, 1));[m[41m[m
[32m+[m[32m                    b.NumeroDocumento = "000032548";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    b.Sacado = new Sacado(dtosac.cpfcnpj, dtosac.nome);[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.End = dtoend.End;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Bairro = dtoend.Bairro;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Cidade = dtoend.Cidade;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.CEP = dtoend.CEP;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.UF = dtoend.UF;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Instrucao instr = new Instrucao(001);[m[41m[m
[32m+[m[32m                    instr.Descricao = "Não Receber após o vencimento";[m[41m[m
[32m+[m[32m                    b.Instrucoes.Add(instr);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb.Boleto = b;[m[41m[m
[32m+[m[32m                    bb.Boleto.Valida();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boletos.Add(bb);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                GeraLayout(boletos);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region BOLETO SUDAMERIS[m[41m[m
[32m+[m[32m        public void GeraBoletoSudameris(int qtde)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                CedenteDTO dtoced = new CedenteDTO();[m[41m[m
[32m+[m[32m                dtoced.nome = NOMEtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.cpfcnpj = CPFtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.agencia = AGtextBox.Text;[m[41m[m
[32m+[m[32m                dtoced.conta = CCtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                SacadoDTO dtosac = new SacadoDTO();[m[41m[m
[32m+[m[32m                dtosac.cpfcnpj = cpfsactextBox.Text;[m[41m[m
[32m+[m[32m                dtosac.nome = nomesactextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                EnderecoDTO dtoend = new EnderecoDTO();[m[41m[m
[32m+[m[32m                dtoend.End = endtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Bairro = bairrotextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.Cidade = CidadetextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.CEP = CEPtextBox.Text;[m[41m[m
[32m+[m[32m                dtoend.UF = UFtextBox.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Cria o boleto, e passa os parâmetros usuais[m[41m[m
[32m+[m[32m                BoletoBancario bb;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                List<BoletoBancario> boletos = new List<BoletoBancario>();[m[41m[m
[32m+[m[32m                for (int i = 0; i < qtde; i++)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb = new BoletoBancario();[m[41m[m
[32m+[m[32m                    bb.CodigoBanco = _codigoBanco;[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    DateTime _dia = DateTime.Now;[m[41m[m
[32m+[m[32m                    DateTime vencimento = Convert.ToDateTime(_dia.AddDays(5).ToString("dd/MM/yyyy"));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Cedente c = new Cedente(dtoced.cpfcnpj, dtoced.nome, dtoced.agencia, dtoced.conta);[m[41m[m
[32m+[m[32m                    c.Codigo = Convert.ToInt32(dtoced.conta.ToString());[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    //Nosso número com 7 dígitos[m[41m[m
[32m+[m[32m                    string nn = "0003020";[m[41m[m
[32m+[m[32m                    //Nosso número com 13 dígitos[m[41m[m
[32m+[m[32m                    //nn = "0000000003025";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    double valorboleto = Convert.ToDouble(ValorBoletotextBox.Text);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Boleto b = new Boleto(vencimento, valorboleto, "198", nn, c);// EnumEspecieDocumento_Sudameris.DuplicataMercantil);[m[41m[m
[32m+[m[32m                    b.NumeroDocumento = "1008073";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    b.Sacado = new Sacado(dtosac.cpfcnpj, dtosac.nome);[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.End = dtoend.End;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Bairro = dtoend.Bairro;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.Cidade = dtoend.Cidade;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.CEP = dtoend.CEP;[m[41m[m
[32m+[m[32m                    b.Sacado.Endereco.UF = dtoend.UF;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    Instrucao instr = new Instrucao(001);[m[41m[m
[32m+[m[32m                    instr.Descricao = "Não Receber após o vencimento";[m[41m[m
[32m+[m[32m                    b.Instrucoes.Add(instr);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    bb.Boleto = b;[m[41m[m
[32m+[m[32m                    bb.Boleto.Valida();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boletos.Add(bb);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                GeraLayout(boletos);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                MessageBox.Show(ex.Message);[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m            [m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
