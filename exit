[32m+[m[32m                    string terceiraParteNossoNumero = boleto.NossoNumero.Substring(8, 9);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    //Posição 44[m[41m[m
[32m+[m[32m                    string ccc = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                                               codigoCedente,[m[41m[m
[32m+[m[32m                                               dvCodigoCedente,[m[41m[m
[32m+[m[32m                                               primeiraParteNossoNumero,[m[41m[m
[32m+[m[32m                                               primeiraConstante,[m[41m[m
[32m+[m[32m                                               segundaParteNossoNumero,[m[41m[m
[32m+[m[32m                                               segundaConstante,[m[41m[m
[32m+[m[32m                                               terceiraParteNossoNumero);[m[41m[m
[32m+[m[32m                    string dvCampoLivre = Mod11Base9(ccc).ToString();[m[41m[m
[32m+[m[32m                    campoLivre = string.Format("{0}{1}", ccc, dvCampoLivre);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                //Posição 20 - 25[m[41m[m
[32m+[m[32m                string codigoCedente = Utils.FormatCode(boleto.Cedente.Codigo.ToString(), 6);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Posição 26[m[41m[m
[32m+[m[32m                string dvCodigoCedente = Mod11Base9(codigoCedente).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Posição 27 - 29[m[41m[m
[32m+[m[32m                string primeiraParteNossoNumero = boleto.NossoNumero.Substring(0, 3);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //104 - Caixa Econômica Federal S.A.[m[41m [m
[32m+[m[32m                //Carteira 01.[m[41m [m
[32m+[m[32m                //Cobrança rápida.[m[41m [m
[32m+[m[32m                //Cobrança sem registro.[m[41m [m
[32m+[m[32m                //Cobrança sem registro, nosso número com 16 dígitos.[m[41m [m
[32m+[m[32m                //Cobrança simples[m[41m [m
[32m+[m[41m[m
[32m+[m[32m                //Posição 30[m[41m[m
[32m+[m[32m                string primeiraConstante = boleto.Carteira;[m[41m[m
[32m+[m[32m                if (boleto.Carteira == "SR")[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    primeiraConstante = "2";[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    primeiraConstante = boleto.Carteira;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Posição 31 - 33[m[41m[m
[32m+[m[32m                string segundaParteNossoNumero = boleto.NossoNumero.Substring(0, 3); //(3, 3);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Posição 24[m[41m[m
[32m+[m[32m                string segundaConstante = EMISSAO_CEDENTE.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Posição 35 - 43[m[41m[m
[32m+[m[32m                string terceiraParteNossoNumero = boleto.NossoNumero.Substring(3, 7) + segundaConstante +[m[41m[m
[32m+[m[32m                                                  segundaConstante; //(6, 9);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //Posição 44[m[41m[m
[32m+[m[32m                string ccc = string.Format("{0}{1}{2}{3}{4}{5}{6}", codigoCedente, dvCodigoCedente,[m[41m[m
[32m+[m[32m                                           primeiraParteNossoNumero,[m[41m[m
[32m+[m[32m                                           primeiraConstante, segundaParteNossoNumero, segundaConstante,[m[41m[m
[32m+[m[32m                                           terceiraParteNossoNumero);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string dvCampoLivre = Mod11Base9(ccc).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                campoLivre = string.Format("{0}{1}", ccc, dvCampoLivre);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string xxxx = string.Format("{0}{1}{2}{3}{4}", banco, moeda, fatorVencimento, valorDocumento, campoLivre);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string dvGeral = Mod11(xxxx, 9).ToString();[m[41m[m
[32m+[m[32m            // Posição 5[m[41m[m
[32m+[m[32m            _dacBoleto = dvGeral;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}",[m[41m[m
[32m+[m[32m                   banco,[m[41m[m
[32m+[m[32m                   moeda,[m[41m[m
[32m+[m[32m                   dvGeral,[m[41m[m
[32m+[m[32m                   fatorVencimento,[m[41m[m
[32m+[m[32m                   valorDocumento,[m[41m[m
[32m+[m[32m                   campoLivre[m[41m[m
[32m+[m[32m                   );[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string Grupo1 = string.Empty;[m[41m[m
[32m+[m[32m            string Grupo2 = string.Empty;[m[41m[m
[32m+[m[32m            string Grupo3 = string.Empty;[m[41m[m
[32m+[m[32m            string Grupo4 = string.Empty;[m[41m[m
[32m+[m[32m            string Grupo5 = string.Empty;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string str1 = string.Empty;[m[41m[m
[32m+[m[32m            string str2 = string.Empty;[m[41m[m
[32m+[m[32m            string str3 = string.Empty;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            /// <summary>[m[41m[m
[32m+[m[32m            ///   IMPLEMENTAÇÃO PARA NOSSO NÚMERO COM 17 POSIÇÕES[m[41m[m
[32m+[m[32m            ///   Autor.: Fábio Marcos[m[41m[m
[32m+[m[32m            ///   E-Mail: fabiomarcos@click21.com.br[m[41m[m
[32m+[m[32m            ///   Data..: 01/03/2011[m[41m[m
[32m+[m[32m            /// </summary>[m[41m[m
[32m+[m[32m            if (boleto.NossoNumero.Length == 17)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                #region Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //POSIÇÃO 1 A 4 DO CODIGO DE BARRAS[m[41m[m
[32m+[m[32m                str1 = boleto.CodigoBarra.Codigo.Substring(0, 4);[m[41m[m
[32m+[m[32m                //POSICAO 20 A 24 DO CODIGO DE BARRAS[m[41m[m
[32m+[m[32m                str2 = boleto.CodigoBarra.Codigo.Substring(19, 5);[m[41m[m
[32m+[m[32m                //CALCULO DO DIGITO[m[41m[m
[32m+[m[32m                str3 = Mod10(str1 + str2).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo1 = str1 + str2 + str3;[m[41m[m
[32m+[m[32m                Grupo1 = Grupo1.Substring(0, 5) + "." + Grupo1.Substring(5) + " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //POSIÇÃO 25 A 34 DO COD DE BARRAS[m[41m[m
[32m+[m[32m                str1 = boleto.CodigoBarra.Codigo.Substring(24, 10);[m[41m[m
[32m+[m[32m                //DIGITO[m[41m[m
[32m+[m[32m                str2 = Mod10(str1).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo2 = string.Format("{0}.{1}{2} ", str1.Substring(0, 5), str1.Substring(5, 5), str2);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //POSIÇÃO 35 A 44 DO CODIGO DE BARRAS[m[41m[m
[32m+[m[32m                str1 = boleto.CodigoBarra.Codigo.Substring(34, 10);[m[41m[m
[32m+[m[32m                //DIGITO[m[41m[m
[32m+[m[32m                str2 = Mod10(str1).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo3 = string.Format("{0}.{1}{2} ", str1.Substring(0, 5), str1.Substring(5, 5), str2);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string D4 = _dacBoleto.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo4 = string.Format("{0} ", D4);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 5[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //POSICAO 6 A 9 DO CODIGO DE BARRAS[m[41m[m
[32m+[m[32m                str1 = boleto.CodigoBarra.Codigo.Substring(5, 4);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                //POSICAO 10 A 19 DO CODIGO DE BARRAS[m[41m[m
[32m+[m[32m                str2 = boleto.CodigoBarra.Codigo.Substring(9, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo5 = string.Format("{0}{1}", str1, str2);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 5[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                #region Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string BBB = boleto.CodigoBarra.Codigo.Substring(0, 3);[m[41m[m
[32m+[m[32m                string M = boleto.CodigoBarra.Codigo.Substring(3, 1);[m[41m[m
[32m+[m[32m                string CCCCC = boleto.CodigoBarra.Codigo.Substring(19, 5);[m[41m[m
[32m+[m[32m                string D1 = Mod10(BBB + M + CCCCC).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo1 = string.Format("{0}{1}{2}.{3}{4} ",[m[41m[m
[32m+[m[32m                    BBB,[m[41m[m
[32m+[m[32m                    M,[m[41m[m
[32m+[m[32m                    CCCCC.Substring(0, 1),[m[41m[m
[32m+[m[32m                    CCCCC.Substring(1, 4), D1);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 1[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string CCCCCCCCCC2 = boleto.CodigoBarra.Codigo.Substring(24, 10);[m[41m[m
[32m+[m[32m                string D2 = Mod10(CCCCCCCCCC2).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo2 = string.Format("{0}.{1}{2} ", CCCCCCCCCC2.Substring(0, 5), CCCCCCCCCC2.Substring(5, 5), D2);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 2[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string CCCCCCCCCC3 = boleto.CodigoBarra.Codigo.Substring(34, 10);[m[41m[m
[32m+[m[32m                string D3 = Mod10(CCCCCCCCCC3).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo3 = string.Format("{0}.{1}{2} ", CCCCCCCCCC3.Substring(0, 5), CCCCCCCCCC3.Substring(5, 5), D3);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 3[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string D4 = _dacBoleto.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo4 = string.Format(" {0} ", D4);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 4[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #region Campo 5[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                long FFFF = FatorVencimento(boleto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string VVVVVVVVVV = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m                VVVVVVVVVV = Utils.FormatCode(VVVVVVVVVV, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (Utils.ToInt64(VVVVVVVVVV) == 0)[m[41m[m
[32m+[m[32m                    VVVVVVVVVV = "000";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Grupo5 = string.Format("{0}{1}", FFFF, VVVVVVVVVV);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                #endregion Campo 5[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //MONTA OS DADOS DA INHA DIGITÁVEL DE ACORDO COM OS DADOS OBTIDOS ACIMA[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel = Grupo1 + Grupo2 + Grupo3 + Grupo4 + Grupo5;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("SR"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.NossoNumero.Length == 14)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = "8" + boleto.NossoNumero;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.NossoNumero = string.Format("{0}-{1}", boleto.NossoNumero, Mod11Base9(boleto.NossoNumero)); //[m[41m[m
[32m+[m[32m            //boleto.NossoNumero = string.Format("{0}{1}/{2}-{3}", boleto.Carteira, EMISSAO_CEDENTE, boleto.NossoNumero, Mod11Base9(boleto.Carteira + EMISSAO_CEDENTE + boleto.NossoNumero));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNumeroDocumento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("SR"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if ((boleto.NossoNumero.Length != 14) && (boleto.NossoNumero.Length != 17))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    throw new Exception("Nosso Número inválido, Para Caixa Econômica - Carteira SR o Nosso Número deve conter 14 ou 17 posições.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.NossoNumero.Length != 10)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    throw new Exception([m[41m[m
[32m+[m[32m                        "Nosso Número inválido, Para Caixa Econômica carteira indefinida, o Nosso Número deve conter 10 caracteres.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (!boleto.Cedente.Codigo.Equals(0))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    string codigoCedente = Utils.FormatCode(boleto.Cedente.Codigo.ToString(), 6);[m[41m[m
[32m+[m[32m                    string dvCodigoCedente = Mod10(codigoCedente).ToString(); //Base9[m[41m [m
[32m+[m[41m[m
[32m+[m[32m                    if (boleto.Cedente.DigitoCedente.Equals(-1))[m[41m[m
[32m+[m[32m                        boleto.Cedente.DigitoCedente = Convert.ToInt32(dvCodigoCedente);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    throw new Exception("Informe o código do cedente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui o nome do banco ao local de pagamento[m[41m[m
[32m+[m[32m            boleto.LocalPagamento = "PREFERENCIALMENTE NAS CASAS LOTÉRICAS E AGÊNCIAS DA CAIXA";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m            FormataNossoNumero(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de geração do arquivo remessa[m[41m[m
[32m+[m[32m        public override string GerarHeaderRemessa(Cedente cedente, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return GerarHeaderRemessa("0", cedente, tipoArquivo);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// HEADER do arquivo CNAB[m[41m[m
[32m+[m[32m        /// Gera o HEADER do arquivo remessa de acordo com o lay-out informado[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override string GerarHeaderRemessa(string numeroConvenio, Cedente cedente, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _header = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                base.GerarHeaderRemessa("0", cedente, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        _header = GerarHeaderRemessaCNAB240(cedente);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        _header = GerarHeaderRemessaCNAB400(0, cedente);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _header;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do HEADER do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarDetalheSegmentoPRemessa(Boleto boleto, int numeroRegistro, string numeroConvenio, Cedente cedente)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return GerarDetalheSegmentoPRemessaCNAB240(boleto, numeroRegistro, numeroConvenio, cedente);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarDetalheSegmentoQRemessa(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return GerarDetalheSegmentoQRemessaCNAB240(boleto, numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarDetalheSegmentoRRemessa(Boleto boleto, int numeroRegistroDetalhe, TipoArquivo CNAB240)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return GerarDetalheSegmentoRRemessaCNAB240(boleto, numeroRegistroDetalhe, CNAB240);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarTrailerLoteRemessa(int numeroRegistro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return GerarTrailerLoteRemessaCNAB240(numeroRegistro);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarTrailerArquivoRemessa(int numeroRegistro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return GerarTrailerArquivoRemessaCNAB240(numeroRegistro);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarHeaderRemessaCNAB240(Cedente cedente)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = Utils.FormatCode(Codigo.ToString(), "0", 3, true);                      // código do banco na compensação[m[41m[m
[32m+[m[32m                header += "0000";                                                                       // Lote de Serviço[m[41m [m
[32m+[m[32m                header += "0";                                                                          // Tipo de Registro[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 9);                                                 // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += (cedente.CPFCNPJ.Length == 11 ? "1" : "2");                                   // Tipo de Inscrição[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.CPFCNPJ, "0", 15);                                   // CPF/CNPJ do cedente[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.Codigo.ToString() + cedente.DigitoCedente, "0", 16); // Código do Convênio no Banco[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", "0", 4);                                                 // Uso Exclusivo CAIXA[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.Agencia, "0", 5);                      // Agência Mantenedora da Conta[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.DigitoAgencia, "0", 5);                // Dígito Verificador da Agência[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.Conta, "0", 12);                       // Código do Cedente (sem operação)[m[41m  [m
[32m+[m[32m                header += cedente.ContaBancaria.DigitoConta;                                            // Díg. Verif. Cedente (sem operação)[m[41m [m
[32m+[m[32m                header += Banco.Mod11(cedente.ContaBancaria.Agencia + cedente.ContaBancaria.Conta).ToString();// Dígito Verif. Ag./Ced  (sem operação)[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(cedente.Nome, " ", 30);                                      // Nome do cedente[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("CAIXA ECONOMICA FEDERAL", " ", 30);                         // Nome do Banco[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 10);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += "1";                                                                          // Código 1 - Remessa / 2 - Retorno[m[41m [m
[32m+[m[32m                header += DateTime.Now.ToString("ddMMyyyy");                                            // Data de Geração do Arquivo[m[41m[m
[32m+[m[32m                header += string.Format("{0:hh:mm:ss}", DateTime.Now).Replace(":", "");                  // Hora de Geração do Arquivo[m[41m[m
[32m+[m[32m                header += "000001";                                                                     // Número Seqüencial do Arquivo[m[41m [m
[32m+[m[32m                header += "030";                                                                        // Número da Versão do Layout do Arquivo[m[41m [m
[32m+[m[32m                header += "0";                                                                          // Densidade de Gravação do Arquivo[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 20);                                                // Para Uso Reservado do Banco[m[41m[m
[32m+[m[32m                // Na fase de teste deve conter "remessa-produção", após aprovado deve conter espaços em branco[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("remessa-produção", " ", 20);                                // Para Uso Reservado da Empresa[m[41m  [m
[32m+[m[32m                //header += Utils.FormatCode("", " ", 20);                                              // Para Uso Reservado da Empresa[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 29);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar HEADER do arquivo de remessa do CNAB240.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarHeaderLoteRemessa(string numeroConvenio, Cedente cedente, int numeroArquivoRemessa, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        header = GerarHeaderLoteRemessaCNAB240(cedente, numeroArquivoRemessa);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        header = GerarHeaderLoteRemessaCNAB400(0, cedente, numeroArquivoRemessa);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do HEADER DO LOTE do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string GerarHeaderLoteRemessaCNAB400(int numeroConvenio, Cedente cedente, int numeroArquivoRemessa)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new Exception("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string GerarHeaderLoteRemessaCNAB240(Cedente cedente, int numeroArquivoRemessa)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = Utils.FormatCode(Codigo.ToString(), "0", 3, true);                      // código do banco na compensação[m[41m[m
[32m+[m[32m                header += "0001";                                                                       // Lote de Serviço[m[41m[m
[32m+[m[32m                header += "1";                                                                          // Tipo de Registro[m[41m [m
[32m+[m[32m                header += "R";                                                                          // Tipo de Operação[m[41m [m
[32m+[m[32m                header += "01";                                                                         // Tipo de Serviço '01' = Cobrança, '03' = Bloqueto Eletrônico[m[41m [m
[32m+[m[32m                header += "  ";                                                                         // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += "020";                                                                        // Número da Versão do Layout do Arquivo[m[41m [m
[32m+[m[32m                header += " ";                                                                          // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += (cedente.CPFCNPJ.Length == 11 ? "1" : "2");                                   // Tipo de Inscrição[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.CPFCNPJ, "0", 15);                                   // CPF/CNPJ do cedente[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(cedente.Codigo.ToString() + cedente.DigitoCedente, "0", 16); // Código do Convênio no Banco[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 4);                                                 // Uso Exclusivo CAIXA[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.Agencia, "0", 5);                      // Agência Mantenedora da Conta[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.DigitoAgencia, "0", 5);                // Dígito Verificador da Agência[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.Conta, "0", 12);                       // Número da Conta Corrente[m[41m [m
[32m+[m[32m                header += cedente.ContaBancaria.DigitoConta;                                            // Digito Verificador da Conta Corrente[m[41m [m
[32m+[m[32m                header += Banco.Mod11(cedente.ContaBancaria.Agencia + cedente.ContaBancaria.Conta).ToString();// Dígito Verif. Ag./Ced  (sem operação)[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(cedente.Nome, " ", 30);                                      // Nome do cedente[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 40);                                                // Mensagem 1[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 40);                                                // Mensagem 2[m[41m[m
[32m+[m[32m                header += numeroArquivoRemessa.ToString("00000000");                                    // Número Remessa/Retorno[m[41m[m
[32m+[m[32m                header += DateTime.Now.ToString("ddMMyyyy");                                            // Data de Gravação Remessa/Retorno[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", "0", 8);                                                 // Data do Crédito[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 33);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar HEADER DO LOTE do arquivo de remessa.", e);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarHeaderRemessaCNAB400(int numeroConvenio, Cedente cedente)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new Exception("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarDetalheSegmentoPRemessaCNAB240(Boleto boleto, int numeroRegistro, string numeroConvenio, Cedente cedente)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                validaInstrucoes(boleto); // Para protestar, devolver ou desconto.[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string header = Utils.FormatCode(Codigo.ToString(), "0", 3, true);                      // código do banco na compensação[m[41m[m
[32m+[m[32m                header += "0001";                                                                       // Lote de Serviço[m[41m[m
[32m+[m[32m                header += "3";                                                                          // Tipo de Registro[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(numeroRegistro.ToString(), "0", 5);                          // Nº Sequencial do Registro no Lote[m[41m [m
[32m+[m[32m                header += "P";                                                                          // Cód. Segmento do Registro Detalhe[m[41m[m
[32m+[m[32m                header += " ";                                                                          // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += "01";                                                                         // Código de Movimento Remessa[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.Agencia, "0", 5);                      // Agência Mantenedora da Conta[m[41m [m
[32m+[m[32m                header += cedente.ContaBancaria.DigitoAgencia;                                          // Dígito Verificador da Agência[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.Conta, "0", 12);                       // Número da Conta Corrente[m[41m [m
[32m+[m[32m                header += cedente.ContaBancaria.DigitoConta;                                            // Digito Verificador da Conta Corrente[m[41m [m
[32m+[m[32m                header += Banco.Mod11(cedente.ContaBancaria.Agencia + cedente.ContaBancaria.Conta).ToString(); // Dígito Verif. Ag./Ced  (sem operação)[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 9);                                                 // Uso Exclusivo CAIXA[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(boleto.NossoNumero, "0", 11);                                // Identificação do Título no Banco[m[41m [m
[32m+[m[32m                header += "01";                                                                         // Código da Carteira[m[41m [m
[32m+[m[32m                header += (boleto.Carteira == "14" ? "2" : "1");                                        // Forma de Cadastr. do Título no Banco[m[41m [m
[32m+[m[32m                // '1' = Com Cadastramento (Cobrança Registrada)[m[41m [m
[32m+[m[32m                // '2' = Sem Cadastramento (Cobrança sem Registro)[m[41m [m
[32m+[m[32m                header += "2";                                                                          // Tipo de Documento[m[41m [m
[32m+[m[32m                header += "2";                                                                          // Identificação da Emissão do Bloqueto[m[41m [m
[32m+[m[32m                header += "2";                                                                          // Identificação da Distribuição[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(boleto.NumeroDocumento, "0", 11);                            // Número do Documento de Cobrança[m[41m [m
[32m+[m[32m                header += "    ";                                                                       // Uso Exclusivo CAIXA[m[41m[m
[32m+[m[32m                header += boleto.DataVencimento.ToString("ddMMyyyy");                                   // Data de Vencimento do Título[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(boleto.ValorBoleto.ToString().Replace(",", "").Replace(".", ""), "0", 13); // Valor Nominal do Título 13[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(cedente.ContaBancaria.Agencia, "0", 5);                      // Agência Encarregada da Cobrança[m[41m [m
[32m+[m[32m                header += cedente.ContaBancaria.DigitoAgencia;                                          // Dígito Verificador da Agência[m[41m [m
[32m+[m[32m                header += boleto.EspecieDocumento.Codigo.ToString("00");                                // Espécie do Título[m[41m [m
[32m+[m[32m                header += boleto.Aceite;                                                                // Identific. de Título Aceito/Não Aceito[m[41m[m
[32m+[m[32m                // Data da Emissão do Título[m[41m [m
[32m+[m[32m                header += (boleto.DataProcessamento.ToString("ddMMyyyy") == "01010001" ? DateTime.Now.ToString("ddMMyyyy") : boleto.DataProcessamento.ToString("ddMMyyyy"));[m[41m[m
[32m+[m[32m                header += "1";                                                                          // Código do Juros de Mora '1' = Valor por Dia - '2' = Taxa Mensal[m[41m [m
[32m+[m[32m                header += (boleto.DataMulta.ToString("ddMMyyyy") == "01010001" ? "00000000" : boleto.DataMulta.ToString("ddMMyyyy")); // Data do Juros de Mora[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(boleto.ValorMulta.ToString().Replace(",", "").Replace(".", ""), "0", 13); // Juros de Mora por Dia/Taxa[m[41m [m
[32m+[m[32m                header += (desconto ? "1" : "0");                                                       // Código do Desconto[m[41m [m
[32m+[m[32m                header += (boleto.DataDesconto.ToString("ddMMyyyy") == "01010001" ? "00000000" : boleto.DataDesconto.ToString("ddMMyyyy")); // Data do Desconto[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(boleto.ValorDesconto.ToString().Replace(",", "").Replace(".", ""), "0", 13); // Valor/Percentual a ser Concedido[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(boleto.IOF.ToString().Replace(",", "").Replace(".", ""), "0", 13); // Valor do IOF a ser Recolhido[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(boleto.Abatimento.ToString().Replace(",", "").Replace(".", ""), "0", 13); // Valor do Abatimento[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 25);                                                // Identificação do Título na Empresa[m[41m[m
[32m+[m[32m                header += (protestar ? "1" : "3");                                                      // Código para Protesto[m[41m[m
[32m+[m[32m                header += diasProtesto.ToString("00");                                                  // Número de Dias para Protesto 2 posi[m[41m[m
[32m+[m[32m                header += (baixaDevolver ? "1" : "2");                                                  // Código para Baixa/Devolução 1 posi[m[41m[m
[32m+[m[32m                header += diasDevolucao.ToString("00");                                                 // Número de Dias para Baixa/Devolução 3 posi[m[41m[m
[32m+[m[32m                header += boleto.Moeda.ToString("00");                                                  // Código da Moeda[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 10);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 1);                                                 // Uso Exclusivo FEBRABAN/CNAB[m[41m [m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar SEGMENTO P do arquivo de remessa.", e);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarDetalheSegmentoQRemessaCNAB240(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = Utils.FormatCode(Codigo.ToString(), "0", 3, true);                      // código do banco na compensação[m[41m[m
[32m+[m[32m                header += "0001";                                                                       // Lote de Serviço[m[41m[m
[32m+[m[32m                header += "3";                                                                          // Tipo de Registro[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(numeroRegistro.ToString(), "0", 5);                          // Nº Sequencial do Registro no Lote[m[41m [m
[32m+[m[32m                header += "Q";                                                                          // Cód. Segmento do Registro Detalhe[m[41m[m
[32m+[m[32m                header += " ";                                                                          // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += "01";                                                                         // Código de Movimento Remessa[m[41m[m
[32m+[m[32m                header += (boleto.Sacado.CPFCNPJ.Length == 11 ? "1" : "2");                             // Tipo de Inscrição[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(boleto.Sacado.CPFCNPJ, "0", 15);                             // Número de Inscrição[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(boleto.Sacado.Nome, " ", 40);                                // Nome[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(boleto.Sacado.Endereco.End, " ", 40);                        // Endereço[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(boleto.Sacado.Endereco.Bairro, " ", 15);                     // Bairro[m[41m [m
[32m+[m[32m                header += boleto.Sacado.Endereco.CEP;                                                   // CEP + Sufixo do CEP[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(boleto.Sacado.Endereco.Cidade, " ", 15);                     // Cidade[m[41m [m
[32m+[m[32m                header += boleto.Sacado.Endereco.UF;                                                    // Unidade da Federação[m[41m[m
[32m+[m[32m                // Estes campos deverão estar preenchidos quando não for o Cedente original do título.[m[41m[m
[32m+[m[32m                header += "0";                                                                          // Tipo de Inscrição[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", "0", 15);                                                // Número de Inscrição CPF/CNPJ[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 40);                                                // Nome do Sacador/Avalista[m[41m [m
[32m+[m[32m                //*********[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 31);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar SEGMENTO Q do arquivo de remessa.", e);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarDetalheSegmentoRRemessaCNAB240(Boleto boleto, int numeroRegistroDetalhe, TipoArquivo CNAB240)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = Utils.FormatCode(Codigo.ToString(), "0", 3, true);                      // código do banco na compensação[m[41m[m
[32m+[m[32m                header += "0001";                                                                       // Lote de Serviço[m[41m[m
[32m+[m[32m                header += "3";                                                                          // Tipo de Registro[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(numeroRegistroDetalhe.ToString(), "0", 5);                   // Nº Sequencial do Registro no Lote[m[41m [m
[32m+[m[32m                header += "R";                                                                          // Cód. Segmento do Registro Detalhe[m[41m[m
[32m+[m[32m                header += " ";                                                                          // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += "01";                                                                         // Código de Movimento Remessa[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 48);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m [m
[32m+[m[32m                header += "1";                                          // Código da Multa '1' = Valor Fixo,'2' = Percentual,'0' = Sem Multa[m[41m [m
[32m+[m[32m                header += boleto.DataMulta.ToString("ddMMyyyy");                                        // Data da Multa[m[41m [m
[32m+[m[32m                header += Utils.FormatCode(boleto.ValorMulta.ToString().Replace(",", "").Replace(".", ""), "0", 13); // Valor/Percentual a Ser Aplicado[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 10);                                                // Informação ao Sacado[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 40);                                                // Mensagem 3[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 40);                                                // Mensagem 4[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 61);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m [m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar SEGMENTO Q do arquivo de remessa.", e);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarTrailerLoteRemessaCNAB240(int numeroRegistro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = Utils.FormatCode(Codigo.ToString(), "0", 3, true);                      // código do banco na compensação[m[41m[m
[32m+[m[32m                header += "0001";                                                                       // Lote de Serviço[m[41m[m
[32m+[m[32m                header += "5";                                                                          // Tipo de Registro[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 61);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(numeroRegistro.ToString(), "0", 5);                          // Nº Sequencial do Registro no Lote[m[41m [m
[32m+[m[41m[m
[32m+[m[32m                // Totalização da Cobrança Simples[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 6);                                                 // Quantidade de Títulos em Cobrança[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 15);                                                // Valor Total dos Títulos em Carteiras[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 6);                                                 // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 15);                                                // Uso Exclusivo FEBRABAN/CNAB[m[41m [m
[32m+[m[41m[m
[32m+[m[32m                // Totalização da Cobrança Caucionada[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 6);                                                 // Quantidade de Títulos em Cobrança[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 15);                                                // Valor Total dos Títulos em Carteiras[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                // Totalização da Cobrança Descontada[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 6);                                                 // Quantidade de Títulos em Cobrança[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", "0", 15);                                                // Valor Total dos Títulos em Carteiras[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 8);                                                 // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 117);                                               // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar Trailer de Lote do arquivo de remessa.", e);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarTrailerArquivoRemessaCNAB240(int numeroRegistro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = Utils.FormatCode(Codigo.ToString(), "0", 3, true);                      // código do banco na compensação[m[41m[m
[32m+[m[32m                header += "9999";                                                                       // Lote de Serviço[m[41m[m
[32m+[m[32m                header += "9";                                                                          // Tipo de Registro[m[41m [m
[32m+[m[32m                header += Utils.FormatCode("", " ", 9);                                                 // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += "000001";                                                                     // Quantidade de Lotes do Arquivo[m[41m[m
[32m+[m[32m                header += Utils.FormatCode(numeroRegistro.ToString(), "0", 6);                          // Quantidade de Registros do Arquivo[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 6);                                                 // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[32m                header += Utils.FormatCode("", " ", 205);                                               // Uso Exclusivo FEBRABAN/CNAB[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception e)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao gerar Trailer de arquivo de remessa.", e);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion Métodos de geração do arquivo remessa[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Varre as instrucoes para inclusao no Segmento P[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="boleto"></param>[m[41m[m
[32m+[m[32m        private void validaInstrucoes(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (boleto.Instrucoes.Count.Equals(0))[m[41m[m
[32m+[m[32m                return;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            foreach (IInstrucao instrucao in boleto.Instrucoes)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (instrucao.Codigo.Equals(9) || instrucao.Codigo.Equals(42) || instrucao.Codigo.Equals(81) || instrucao.Codigo.Equals(82))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    protestar = true;[m[41m[m
[32m+[m[32m                    diasProtesto = instrucao.QuantidadeDias;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (instrucao.Codigo.Equals(91) || instrucao.Codigo.Equals(92))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    baixaDevolver = true;[m[41m[m
[32m+[m[32m                    diasDevolucao = instrucao.QuantidadeDias;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (instrucao.Codigo.Equals(999))[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    desconto = true;[m[41m[m
[32m+[m[32m                    diasDesconto = instrucao.QuantidadeDias;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de Leitura do Arquivo de Retorno[m[41m[m
[32m+[m[32m        public override DetalheSegmentoTRetornoCNAB240 LerDetalheSegmentoTRetornoCNAB240(string registro)[m[41m[m
