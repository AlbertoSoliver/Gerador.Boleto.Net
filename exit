[32m+[m[32m                set[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    m_ThumbnailWidth = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            private int m_ThumbnailHeight;[m[41m[m
[32m+[m[32m            public int ThumbnailHeight[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                get[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return m_ThumbnailHeight;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                set[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    m_ThumbnailHeight = value;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            private int m_BrowserWidth;[m[41m[m
[32m+[m[32m            public int BrowserWidth[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                get[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    return m_BrowserWidth;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                set[m[41m[m
