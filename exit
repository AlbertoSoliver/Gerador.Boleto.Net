[32m+[m[32m        TarifaDeSustacao = 3,[m[41m[m
[32m+[m[32m        TarifaDeProtesto = 4,[m[41m[m
[32m+[m[32m        TarifaDeOutrasInstrucoes = 5,[m[41m[m
[32m+[m[32m        TarifaDeOutrasOcorrencias = 6,[m[41m[m
