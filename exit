[32m+[m[32m                _trailerArquivo = "00199999         000001";[m[41m[m
[32m+[m[32m                _trailerArquivo += Utils.FitStringLength(numeroRegistro.ToString(), 6, 6, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _trailerArquivo += "000000";[m[41m[m
[32m+[m[32m                _trailerArquivo += _brancos205;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _trailerArquivo = Utils.SubstituiCaracteresEspeciais(_trailerArquivo);[m[41m[m
