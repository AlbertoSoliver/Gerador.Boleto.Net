[32m+[m[41m[m
[32m+[m[32m        #region Construtores[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal Banco_Brasil()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this.Codigo = 1;[m[41m[m
[32m+[m[32m                this.Digito = 9;[m[41m[m
[32m+[m[32m                this.Nome = "Banco do Brasil";[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao instanciar objeto.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
