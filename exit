[32m+[m[32m        private void InstanciaBanco(int codigoBanco)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                switch (codigoBanco)[m[41m[m
[32m+[m[32m                {[m[41m[m
