[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            // Posição 01-03[m[41m[m
[32m+[m[32m            string banco = Codigo.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Posição 04[m[41m[m
[32m+[m[32m            string moeda = "9";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Posição 05 - No final ...[m[41m   [m
[32m+[m[41m[m
[32m+[m[32m            // Posição 06 - 09[m[41m[m
[32m+[m[32m            long fatorVencimento = FatorVencimento(boleto);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Posição 10 - 19[m[41m     [m
[32m+[m[32m            string valorDocumento = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m            valorDocumento = Utils.FormatCode(valorDocumento, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Inicio Campo livre[m[41m[m
[32m+[m[32m            string campoLivre = string.Empty;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //ESSA IMPLEMENTAÇÃO FOI FEITA PARA CARTEIAS SIGCB "SR" COM NOSSO NUMERO DE 14 e 17 POSIÇÕES[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("SR"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                //14 POSIÇOES[m[41m[m
[32m+[m[32m                if (boleto.NossoNumero.Length == 14)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //Posição 20 - 24[m[41m[m
[32m+[m[32m                    string contaCedente = Utils.FormatCode(boleto.Cedente.ContaBancaria.Conta, 5);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    // Posição 25 - 28[m[41m[m
[32m+[m[32m                    string agenciaCedente = Utils.FormatCode(boleto.Cedente.ContaBancaria.Agencia, 4);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    //Posição 29[m[41m[m
[32m+[m[32m                    string codigoCarteira = "8";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    //Posição 30[m[41m[m
[32m+[m[32m                    string constante = "7";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    //Posição 31 - 44[m[41m[m
[32m+[m[32m                    string nossoNumero = boleto.NossoNumero;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    campoLivre = string.Format("{0}{1}{2}{3}{4}", contaCedente, agenciaCedente, codigoCarteira,[m[41m[m
[32m+[m[32m                                               constante, nossoNumero);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                //17 POSIÇÕES[m[41m[m
[32m+[m[32m                if (boleto.NossoNumero.Length == 17)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //104 - Caixa Econômica Federal S.A.[m[41m [m
