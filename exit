[32m+[m[32m        {[m[41m[m
[32m+[m[32m            EspeciesDocumento especiesDocumento = new EspeciesDocumento();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            foreach (EnumEspecieDocumento_Itau item in Enum.GetValues(typeof(EnumEspecieDocumento_Itau)))[m[41m[m
