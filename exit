[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de Instância[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public void LerDetalheSegmentoTRetornoCNAB240(string registro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                _registro = registro;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (registro.Substring(13, 1) != "T")[m[41m[m
[32m+[m[32m                    throw new Exception("Registro inválida. O detalhe não possuí as características do segmento T.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                int dataVencimento = Convert.ToInt32(registro.Substring(69, 8));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                CodigoBanco = Convert.ToInt32(registro.Substring(0, 3));[m[41m[m
[32m+[m[32m                idCodigoMovimento = Convert.ToInt32(registro.Substring(15, 2));[m[41m[m
[32m+[m[32m                Agencia = Convert.ToInt32(registro.Substring(17, 4));[m[41m[m
[32m+[m[32m                DigitoAgencia = registro.Substring(21, 1);[m[41m[m
[32m+[m[32m                Conta = Convert.ToInt32(registro.Substring(22, 9));[m[41m[m
[32m+[m[32m                DigitoConta = registro.Substring(31, 1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                NossoNumero = registro.Substring(40, 13);[m[41m[m
[32m+[m[32m                CodigoCarteira = Convert.ToInt32(registro.Substring(53, 1));[m[41m[m
[32m+[m[32m                NumeroDocumento = registro.Substring(54, 15);[m[41m[m
[32m+[m[32m                DataVencimento = Convert.ToDateTime(dataVencimento.ToString("##-##-####"));[m[41m[m
[32m+[m[32m                double valorTitulo = Convert.ToInt64(registro.Substring(77, 15));[m[41m[m
[32m+[m[32m                ValorTitulo = valorTitulo / 100;[m[41m[m
[32m+[m[32m                IdentificacaoTituloEmpresa = registro.Substring(100, 25);[m[41m[m
[32m+[m[32m                TipoInscricao = Convert.ToInt32(registro.Substring(127, 1));[m[41m[m
[32m+[m[32m                NumeroInscricao = registro.Substring(128, 15);[m[41m[m
[32m+[m[32m                NomeSacado = registro.Substring(143, 40);[m[41m[m
[32m+[m[32m                double valorTarifas = Convert.ToUInt64(registro.Substring(193, 15));[m[41m[m
[32m+[m[32m                ValorTarifas = valorTarifas / 100;[m[41m[m
[32m+[m[32m                CodigoRejeicao = Convert.ToInt32(registro.Substring(208, 10));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao processar arquivo de RETORNO - SEGMENTO T.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
