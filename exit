[32m+[m[32m        {[m[41m [m
[32m+[m[32m            get { return _banco; }[m[41m[m
[32m+[m[32m            set { this._banco = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public ContaBancaria contaBancaria[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _contaBancaria; }[m[41m[m
[32m+[m[32m            set { this._contaBancaria = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public Cedente Cedente[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _cedente; }[m[41m[m
[32m+[m[32m            set { this._cedente = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Métodos[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Verifica se já existe o arquivo relativo a remessa, caso não exista é criado um arquivo ".rem".[m[41m[m
[32m+[m[32m        ///[m[41m [m
[32m+[m[32m        /// O padrão dos arquivos de Remessa e Retorno, obedece as regras estabelecidas pelo C.N.A.B. (Centro Nacional[m[41m[m
[32m+[m[32m        /// de Automação Bancária) e deverá ser gravado contendo:[m[41m[m
[32m+[m[32m        /// Registro Header : Primeiro registro do Arquivo contendo a identificação da empresa[m[41m[m
[32m+[m[32m        /// Registro Detalhe : Registro contendo as informações de Pagamentos :[m[41m[m
[32m+[m[32m        /// - Inclusão de compromissos[m[41m[m
[32m+[m[32m        /// - Alteração de Compromissos[m[41m[m
[32m+[m[32m        /// - Pagamentos Efetuados[m[41m[m
[32m+[m[32m        /// - Bloqueios / Desbloqueios[m[41m[m
[32m+[m[32m        /// Registro Trailer : Último registro indicando finalização do Arquivo[m[41m[m
[32m+[m[32m        /// Caracteres obrigatórios = 0D 0A (Final de Registro) 0D 0A 1A (Final de Arquivo)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private new void Add(Boleto item)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (item.Banco == null)[m[41m[m
[32m+[m[32m                throw new Exception("Boleto não possui Banco.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (item.ContaBancaria == null)[m[41m[m
[32m+[m[32m                throw new Exception("Boleto não possui conta bancária.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (item.Cedente == null)[m[41m[m
[32m+[m[32m                throw new Exception("Boleto não possui cedente.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            item.Valida();[m[41m[m
[32m+[m[32m            this.Add(item);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
