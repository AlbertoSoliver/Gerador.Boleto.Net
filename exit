[32m+[m[32m    sealed class Utils[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        internal static long DateDiff(DateInterval Interval, System.DateTime StartDate, System.DateTime EndDate)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            long lngDateDiffValue = 0;[m[41m[m
[32m+[m[32m            System.TimeSpan TS = new System.TimeSpan(EndDate.Ticks - StartDate.Ticks);[m[41m[m
[32m+[m[32m            switch (Interval)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                case DateInterval.Day:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)TS.Days;[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DateInterval.Hour:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)TS.TotalHours;[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DateInterval.Minute:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)TS.TotalMinutes;[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DateInterval.Month:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)(TS.Days / 30);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DateInterval.Quarter:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)((TS.Days / 30) / 3);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DateInterval.Second:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)TS.TotalSeconds;[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DateInterval.Week:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)(TS.Days / 7);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m                case DateInterval.Year:[m[41m[m
[32m+[m[32m                    lngDateDiffValue = (long)(TS.Days / 365);[m[41m[m
[32m+[m[32m                    break;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return (lngDateDiffValue);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        // uislcs: Acho que a função FormatCode() deveria ser renomeada para Completar().[m[41m[m
[32m+[m[32m        /*[m[41m[m
[32m+[m[32m         * "Para os registros tipo A (Alfanumérico) preencher com caracteres caixa alta e com espaços à direita[m[41m[m
[32m+[m[32m         * preenchendo todo o espaço do campo. Para os registros tipo N (Numérico) preencher com zeros à[m[41m[m
[32m+[m[32m         * esquerda preenchendo todo o campo." (p.9)[m[41m[m
[32m+[m[32m         *[m[41m [m
[32m+[m[32m         * Disponível em: http://www.sicoobpr.com.br/download/manualcobranca/Manual_Cedentes_Sistema_Proprio.doc[m[41m[m
[32m+[m[32m         */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Função para completar um string com zeros ou espacos em branco. Pode servir para criar a remessa.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="text">O valor recebe os zeros ou espaços em branco</param>[m[41m[m
[32m+[m[32m        /// <param name="with">caractere a ser inserido</param>[m[41m[m
[32m+[m[32m        /// <param name="size">Tamanho do campo</param>[m[41m[m
[32m+[m[32m        /// <param name="left">Indica se caracteres serão inseridos à esquerda ou à direita, o valor default é inicializar pela esquerda (left)</param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        internal static string FormatCode(string text, string with, int length, bool left)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            length -= text.Length;[m[41m[m
[32m+[m[32m            if (left)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                for (int i = 0; i < length; ++i)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    text = with + text;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                for (int i = 0; i < length; ++i)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    text += with;[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            return text;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static string FormatCode(string text, string with, int length)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return FormatCode(text, with, length, false);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static string FormatCode(string text, int length)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return FormatCode(text, "0", length, true);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Remove todos os acentos das palavras.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="value">palavra acentuada</param>[m[41m[m
[32m+[m[32m        /// <returns>palavra sem acento</returns>[m[41m[m
[32m+[m[32m        internal static String RemoveAcento(String value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String normalizedString = value.Normalize(NormalizationForm.FormD);[m[41m[m
[32m+[m[32m            StringBuilder stringBuilder = new StringBuilder();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = 0; i < normalizedString.Length; i++)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Char c = normalizedString[i];[m[41m[m
[32m+[m[32m                if (CharUnicodeInfo.GetUnicodeCategory(c) != UnicodeCategory.NonSpacingMark)[m[41m[m
[32m+[m[32m                    stringBuilder.Append(c);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return stringBuilder.ToString();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// retorna um array de strings de tamanho variável com os dados da linha (pode ser usado para qualquer leitura de arquivos de retorno || remessa)[m[41m[m
[32m+[m[32m        /// os dados no string pattern correspondem a intervalos fechados na matemática ex: [2-19] (fechado de 2 a 19)[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="linha">string de onde os dados serão extraídos. por exemplo, uma linha de um arquivo de retorno</param>[m[41m[m
[32m+[m[32m        /// <param name="pattern">obrigatóriamente é necessário numero PAR de valores NUMÉRICOS no string pattern. ex: 1-1,2-19</param>[m[41m[m
[32m+[m[32m        /// <returns>um array de strings de tamanho variável contendo os dados lidos na linha: string[]</returns>[m[41m[m
[32m+[m[32m        /// <example>[m[41m[m
[32m+[m[32m        /// string[] dados = getDados(sLine, "1-1,2-394,395-400");[m[41m[m
[32m+[m[32m        /// </example>[m[41m[m
[32m+[m[32m        internal static string[] GetDados(string linha, string pattern)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            // separa os números[m[41m[m
[32m+[m[32m            pattern = pattern.Replace('-', ',');[m[41m[m
[32m+[m[32m            string[] coord = pattern.Split(',');[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //cria objeto para armazenágem, buffer.[m[41m[m
[32m+[m[32m            string[] dados = new string[coord.Length / 2];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //pega os números de 2 em 2 e preenche o array[m[41m[m
[32m+[m[32m            int x = 0;[m[41m[m
[32m+[m[32m            for (int i = 0; i < coord.Length; i += 2)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                dados[x] = linha.Substring(Convert.ToInt32(coord[i]) - 1, Convert.ToInt32(coord[i + 1]) - Convert.ToInt32(coord[i]) + 1);[m[41m[m
[32m+[m[32m                //arg[x] = linha.Substring(Convert.ToInt32(coord[i]), Convert.ToInt32(coord[i + 1]));[m[41m[m
[32m+[m[32m                x++;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            //retorna os dados[m[41m[m
[32m+[m[32m            return dados;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /* Exemplo de Leitura de um arquivo de remessa[m[41m[m
[32m+[m[32m        private void button1_Click(object sender, EventArgs e)[m[41m[m
[32m+[m[32m        { //ler arquivo de texto[m[41m[m
[32m+[m[32m            StreamReader objReader = new StreamReader("C:\\Documents and Settings\\uis\\Desktop\\bancos\\CED006877211081.REM");[m[41m[m
[32m+[m[32m            string sLine = "";[m[41m[m
[32m+[m[32m            string[] dados;[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m            while (sLine != null)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                sLine = objReader.ReadLine();[m[41m[m
[32m+[m[32m                if (sLine != null){[m[41m[m
[32m+[m[32m                    dados = getDados(sLine, "1-1,2-394,395-400");[m[41m[m
[32m+[m[32m                    // adicionar os dados a um string[m[41m[m
[32m+[m[32m                    textBox1.Text += " posição:<" + dados[2] + ">";[m[41m[m
[32m+[m[32m                    // poderia ser[m[41m[m
[32m+[m[32m                    //new boleto_dados(dados[0],dados[1],dados[2]);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            objReader.Close();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static bool IsNumber(string value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            Regex objNotNumberPattern = new Regex("[^0-9.-]");[m[41m[m
[32m+[m[32m            Regex objTwoDotPattern = new Regex("[0-9]*[.][0-9]*[.][0-9]*");[m[41m[m
[32m+[m[32m            Regex objTwoMinusPattern = new Regex("[0-9]*[-][0-9]*[-][0-9]*");[m[41m[m
[32m+[m[32m            String strValidRealPattern = "^([-]|[.]|[-.]|[0-9])[0-9]*[.]*[0-9]+$";[m[41m[m
[32m+[m[32m            String strValidIntegerPattern = "^([-]|[0-9])[0-9]*$";[m[41m[m
[32m+[m[32m            Regex objNumberPattern = new Regex("(" + strValidRealPattern + ")|(" + strValidIntegerPattern + ")");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return !objNotNumberPattern.IsMatch(value) &&[m[41m[m
[32m+[m[32m                   !objTwoDotPattern.IsMatch(value) &&[m[41m[m
[32m+[m[32m                   !objTwoMinusPattern.IsMatch(value) &&[m[41m[m
[32m+[m[32m                   objNumberPattern.IsMatch(value);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static bool ToBool(object value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return Convert.ToBoolean(value);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return false;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static int ToInt32(string value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return Convert.ToInt32(value);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return 0;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static long ToInt64(string value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return Convert.ToInt64(value);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return 0;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static string ToString(object value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return Convert.ToString(value).Trim();[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Empty;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static DateTime ToDateTime(object value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return Convert.ToDateTime(value);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return new DateTime(1, 1, 1);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o CPF ou CNPJ do Cedente ou do Sacado no formato: 000.000.000-00, 00.000.000/0001-00 respectivamente.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="value"></param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        internal static string FormataCPFCPPJ(string value)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (value.Trim().Length == 11)[m[41m[m
[32m+[m[32m                return FormataCPF(value);[m[41m[m
[32m+[m[32m            else if (value.Trim().Length == 14)[m[41m[m
[32m+[m[32m                return FormataCNPJ(value);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            throw new Exception(string.Format("O CPF ou CNPJ: {0} é inválido.", value));[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o número do CPF 92074286520 para 920.742.865-20[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="cpf">Sequencia numérica de 11 dígitos. Exemplo: 00000000000</param>[m[41m[m
[32m+[m[32m        /// <returns>CPF formatado</returns>[m[41m[m
[32m+[m[32m        internal static string FormataCPF(string cpf)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Format("{0}.{1}.{2}-{3}", cpf.Substring(0, 3), cpf.Substring(3, 3), cpf.Substring(6, 3), cpf.Substring(9, 2));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Empty;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o CNPJ. Exemplo 00.316.449/0001-63[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="cnpj">Sequencia numérica de 14 dígitos. Exemplo: 00000000000000</param>[m[41m[m
[32m+[m[32m        /// <returns>CNPJ formatado</returns>[m[41m[m
[32m+[m[32m        internal static string FormataCNPJ(string cnpj)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Format("{0}.{1}.{2}/{3}-{4}", cnpj.Substring(0, 2), cnpj.Substring(2, 3), cnpj.Substring(5, 3), cnpj.Substring(8, 4), cnpj.Substring(12, 2));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Empty;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formato o CEP em 00.000-000[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="cep">Sequencia numérica de 8 dígitos. Exemplo: 00000000</param>[m[41m[m
[32m+[m[32m        /// <returns>CEP formatado</returns>[m[41m[m
[32m+[m[32m        internal static string FormataCEP(string cep)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Format("{0}{1}-{2}", cep.Substring(0, 2), cep.Substring(2, 3), cep.Substring(5, 3));[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Empty;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata agência e conta[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="agencia">Código da agência</param>[m[41m[m
[32m+[m[32m        /// <param name="digitoAgencia">Dígito verificador da agência. Pode ser vazio.</param>[m[41m[m
[32m+[m[32m        /// <param name="conta">Código da conta</param>[m[41m[m
[32m+[m[32m        /// <param name="digitoConta">dígito verificador da conta. Pode ser vazio.</param>[m[41m[m
[32m+[m[32m        /// <returns>Agência e conta formatadas</returns>[m[41m[m
[32m+[m[32m        internal static string FormataAgenciaConta(string agencia, string digitoAgencia, string conta, string digitoConta)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string agenciaConta = string.Empty;[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                agenciaConta = agencia;[m[41m[m
[32m+[m[32m                if (digitoAgencia != string.Empty)[m[41m[m
[32m+[m[32m                    agenciaConta += "-" + digitoAgencia;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                agenciaConta += "/" + conta;[m[41m[m
[32m+[m[32m                if (digitoConta != string.Empty)[m[41m[m
[32m+[m[32m                    agenciaConta += "-" + digitoConta;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return agenciaConta;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return string.Empty;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o campo de acordo com o tipo e o tamanho[m[41m [m
[32m+[m[32m        /// </summary>[m[41m        [m
[32m+[m[32m        public static string FitStringLength(string SringToBeFit, int maxLength, int minLength, char FitChar, int maxStartPosition, bool maxTest, bool minTest, bool isNumber)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string result = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (maxTest == true)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    // max[m[41m[m
[32m+[m[32m                    if (SringToBeFit.Length > maxLength)[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        result += SringToBeFit.Substring(maxStartPosition, maxLength);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (minTest == true)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    // min[m[41m[m
[32m+[m[32m                    if (SringToBeFit.Length <= minLength)[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        if (isNumber == true)[m[41m[m
[32m+[m[32m                        {[m[41m[m
[32m+[m[32m                            result += (string)(new string(FitChar, (minLength - SringToBeFit.Length)) + SringToBeFit);[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                        else[m[41m[m
[32m+[m[32m                        {[m[41m[m
[32m+[m[32m                            result += (string)(SringToBeFit + new string(FitChar, (minLength - SringToBeFit.Length)));[m[41m[m
[32m+[m[32m                        }[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                return result;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Exception tmpEx = new Exception("Problemas ao Formatar a string. String = " + SringToBeFit, ex);[m[41m[m
[32m+[m[32m                throw tmpEx;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static string SubstituiCaracteresEspeciais(string strline)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                strline = strline.Replace("ã", "a");[m[41m[m
[32m+[m[32m                strline = strline.Replace('Ã', 'A');[m[41m[m
[32m+[m[32m                strline = strline.Replace('â', 'a');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Â', 'A');[m[41m[m
[32m+[m[32m                strline = strline.Replace('á', 'a');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Á', 'A');[m[41m[m
[32m+[m[32m                strline = strline.Replace('à', 'a');[m[41m[m
[32m+[m[32m                strline = strline.Replace('À', 'A');[m[41m[m
[32m+[m[32m                strline = strline.Replace('ç', 'c');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Ç', 'C');[m[41m[m
[32m+[m[32m                strline = strline.Replace('é', 'e');[m[41m[m
[32m+[m[32m                strline = strline.Replace('É', 'E');[m[41m[m
[32m+[m[32m                strline = strline.Replace('õ', 'o');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Õ', 'O');[m[41m[m
[32m+[m[32m                strline = strline.Replace('ó', 'o');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Ó', 'O');[m[41m[m
[32m+[m[32m                strline = strline.Replace('ô', 'o');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Ô', 'O');[m[41m[m
[32m+[m[32m                strline = strline.Replace('ú', 'u');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Ú', 'U');[m[41m[m
[32m+[m[32m                strline = strline.Replace('ü', 'u');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Ü', 'U');[m[41m[m
[32m+[m[32m                strline = strline.Replace('í', 'i');[m[41m[m
[32m+[m[32m                strline = strline.Replace('Í', 'I');[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return strline;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                Exception tmpEx = new Exception("Erro ao formatar string.", ex);[m[41m[m
[32m+[m[32m                throw tmpEx;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Converte uma imagem em array de bytes.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="image"></param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        public static byte[] ConvertImageToByte(Image image)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (image == null)[m[41m[m
[32m+[m[32m                return null;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            byte[] bytes;[m[41m[m
[32m+[m[32m            if (image.GetType().ToString() == "System.Drawing.Image")[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                ImageConverter converter = new ImageConverter();[m[41m[m
[32m+[m[32m                bytes = (byte[])converter.ConvertTo(image, typeof(byte[]));[m[41m[m
[32m+[m[32m                return bytes;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else if (image.GetType().ToString() == "System.Drawing.Bitmap")[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                bytes = (byte[])TypeDescriptor.GetConverter(image).ConvertTo(image, typeof(byte[]));[m[41m[m
[32m+[m[32m                return bytes;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                throw new NotImplementedException("ConvertImageToByte invalid type " + image.GetType().ToString());[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal static bool DataValida(DateTime dateTime)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (dateTime.ToString("dd/MM/yyyy") == "01/01/1900" | dateTime.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                return false;[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                return true;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Retorna uma String com a qtde de casas pedidas da direita para a esquerda.[m[41m[m
[32m+[m[32m        /// <param name="seq">Sequencia de Dados</param>[m[41m[m
[32m+[m[32m        /// <param name="qtde">Quantidade de Char's à retornar</param>[m[41m[m
[32m+[m[32m        /// <param name="ch">Caracter que deseja usar para completar</param>[m[41m[m
[32m+[m[32m        /// <param name="completaPelaEsquerda">True: completa pela esquerda; False: completa pela direita</param>[m[41m[m
[32m+[m[32m        /// </summary>[m[41m        [m
[32m+[m[32m        public static string Right(string seq, int qtde, char ch, bool completaPelaEsquerda)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string final;[m[41m[m
[32m+[m[32m            final = Strings.Right(seq, qtde);[m[41m[m
[32m+[m[32m            return FitStringLength(final, qtde, qtde, ch, 0, true, true, completaPelaEsquerda);[m[41m[m
[32m+[m[32m            ;[m[41m[m
[32m+[m[32m        }[m[41m   [m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
