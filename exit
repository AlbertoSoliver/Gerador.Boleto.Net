[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    /// <summary>[m[41m[m
[32m+[m[32m    /// Classe referente ao Banco do Brasil[m[41m[m
[32m+[m[32m    /// </summary>[m[41m[m
[32m+[m[32m    internal class Banco_Brasil : AbstractBanco, IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Variáveis[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string _dacNossoNumero = string.Empty;[m[41m[m
[32m+[m[32m        private int _dacBoleto = 0;[m[41m[m
