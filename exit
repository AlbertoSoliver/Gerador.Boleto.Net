[32m+[m[32m                /*[m[41m[m
[32m+[m[32m                 * Convênio de 6 posições[m[41m[m
[32m+[m[32m                 * Nosso Número com 11 posições[m[41m[m
[32m+[m[32m                 */[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    //Modalidades de Cobrança Sem Registro  Carteira 16 e 18[m[41m[m
[32m+[m[32m                    //Nosso Número com 17 posições[m[41m[m
[32m+[m[32m                    if (!boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        if ((boleto.Cedente.Codigo.ToString().Length + boleto.NossoNumero.Length) > 11)[m[41m[m
[32m+[m[32m                            throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 11 de posições para o nosso número. Onde o nosso número é formado por CCCCCCNNNNN-X: C -> número do convênio fornecido pelo Banco, N -> seqüencial atribuído pelo cliente e X -> dígito verificador do Nosso-Número.", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 5));[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                    {[m[41m[m
[32m+[m[32m                        if (boleto.Cedente.Convenio.ToString().Length != 6)[m[41m[m
[32m+[m[32m                            throw new NotImplementedException(string.Format("Para a carteira {0} e o tipo da modalidade 21, o número do convênio são de 6 posições", boleto.Carteira));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                        boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 17);[m[41m[m
[32m+[m[32m                    }[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                /*[m[41m[m
[32m+[m[32m                  * Convênio de 4 posições[m[41m[m
[32m+[m[32m                  * Nosso Número com 11 posições[m[41m[m
[32m+[m[32m                  */[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 4)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.NossoNumero.Length > 7)[m[41m[m
[32m+[m[32m                        throw new NotImplementedException(string.Format("Para a carteira {0}, a quantidade máxima são de 7 de posições para o nosso número [{1}]", boleto.Carteira, boleto.NossoNumero));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = string.Format("{0}{1}", boleto.Cedente.Convenio, Utils.FormatCode(boleto.NossoNumero, 7));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 11);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 18-140[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Agência e Conta Corrente[m[41m[m
[32m+[m[32m            //Verificar se a Agencia esta correta[m[41m[m
[32m+[m[32m            if (boleto.Cedente.ContaBancaria.Agencia.Length > 4)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("A quantidade de dígitos da Agência " + boleto.Cedente.ContaBancaria.Agencia + ", são de 4 números.");[m[41m[m
[32m+[m[32m            else if (boleto.Cedente.ContaBancaria.Agencia.Length < 4)[m[41m[m
[32m+[m[32m                boleto.Cedente.ContaBancaria.Agencia = Utils.FormatCode(boleto.Cedente.ContaBancaria.Agencia, 4);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verificar se a Conta esta correta[m[41m[m
[32m+[m[32m            if (boleto.Cedente.ContaBancaria.Conta.Length > 8)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("A quantidade de dígitos da Conta " + boleto.Cedente.ContaBancaria.Conta + ", são de 8 números.");[m[41m[m
[32m+[m[32m            else if (boleto.Cedente.ContaBancaria.Conta.Length < 8)[m[41m[m
[32m+[m[32m                boleto.Cedente.ContaBancaria.Conta = Utils.FormatCode(boleto.Cedente.ContaBancaria.Conta, 8);[m[41m[m
[32m+[m[32m            #endregion Agência e Conta Corrente[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui o nome do banco ao local de pagamento[m[41m[m
[32m+[m[32m            boleto.LocalPagamento += Nome + "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do processamento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataProcessamento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataProcessamento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do documento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataDocumento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataDocumento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.QuantidadeMoeda = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m            FormataNossoNumero(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string LimparCarteira(string carteira)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            return carteira.Split('-')[0];[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de formatação do boleto[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string valorBoleto = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m            valorBoleto = Utils.FormatCode(valorBoleto, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 16[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("16"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.Cedente.Convenio,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            "21");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                        boleto.Carteira);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 16[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 17[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("17"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.Cedente.Convenio.ToString().Length == 7)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        "000000",[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        Utils.FormatCode(LimparCarteira(boleto.Carteira), 2));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        Strings.Mid(boleto.NossoNumero, 1, 11),[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                        boleto.Carteira);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                        boleto.Carteira);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 17[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 18[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                    Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                    boleto.Moeda,[m[41m[m
[32m+[m[32m                    FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                    valorBoleto,[m[41m[m
[32m+[m[32m                    boleto.NossoNumero,[m[41m[m
[32m+[m[32m                    boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                    boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                    boleto.Carteira);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 18[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 18-019[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18-019"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.Cedente.Convenio.ToString().Length == 7)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    #region Especificação Convênio 7 posições[m[41m[m
[32m+[m[32m                    /*[m[41m[m
[32m+[m[32m                    Posição     Tamanho     Picture     Conteúdo[m[41m[m
[32m+[m[32m                    01 a 03         03      9(3)            Código do Banco na Câmara de Compensação = 001[m[41m[m
[32m+[m[32m                    04 a 04         01      9(1)            Código da Moeda = '9'[m[41m[m
[32m+[m[32m                    05 a 05         01      9(1)            DV do Código de Barras (Anexo 10)[m[41m[m
[32m+[m[32m                    06 a 09         04      9(04)           Fator de Vencimento (Anexo 8)[m[41m[m
[32m+[m[32m                    10 a 19         10      9(08)           V(2) Valor[m[41m[m
[32m+[m[32m                    20 a 25         06      9(6)            Zeros[m[41m[m
[32m+[m[32m                    26 a 42         17      9(17)           Nosso-Número, sem o DV[m[41m[m
[32m+[m[32m                    26 a 32         9       (7)             Número do Convênio fornecido pelo Banco (CCCCCCC)[m[41m[m
[32m+[m[32m                    33 a 42         9       (10)            Complemento do Nosso-Número, sem DV (NNNNNNNNNN)[m[41m[m
[32m+[m[32m                    43 a 44         02      9(2)            Tipo de Carteira/Modalidade de Cobrança[m[41m[m
[32m+[m[32m                     */[m[41m[m
[32m+[m[32m                    #endregion Especificação Convênio 7 posições[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        "000000",[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        Utils.FormatCode(LimparCarteira(boleto.Carteira), 2));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.Cedente.Convenio,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            "21");[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                            LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 4)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                        LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 18-019[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Para atender o cliente Fiemg foi adptado no código na variação 18-027 as variações 18-035 e 18-140[m[41m[m
[32m+[m[32m            #region Carteira 18-027[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18-027"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.Cedente.Convenio.ToString().Length == 7)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    #region Especificação Convênio 7 posições[m[41m[m
[32m+[m[32m                    /*[m[41m[m
[32m+[m[32m                    Posição     Tamanho     Picture     Conteúdo[m[41m[m
[32m+[m[32m                    01 a 03         03      9(3)            Código do Banco na Câmara de Compensação = 001[m[41m[m
[32m+[m[32m                    04 a 04         01      9(1)            Código da Moeda = '9'[m[41m[m
[32m+[m[32m                    05 a 05         01      9(1)            DV do Código de Barras (Anexo 10)[m[41m[m
[32m+[m[32m                    06 a 09         04      9(04)           Fator de Vencimento (Anexo 8)[m[41m[m
[32m+[m[32m                    10 a 19         10      9(08)           V(2) Valor[m[41m[m
[32m+[m[32m                    20 a 25         06      9(6)            Zeros[m[41m[m
[32m+[m[32m                    26 a 42         17      9(17)           Nosso-Número, sem o DV[m[41m[m
[32m+[m[32m                    26 a 32         9       (7)             Número do Convênio fornecido pelo Banco (CCCCCCC)[m[41m[m
[32m+[m[32m                    33 a 42         9       (10)            Complemento do Nosso-Número, sem DV (NNNNNNNNNN)[m[41m[m
[32m+[m[32m                    43 a 44         02      9(2)            Tipo de Carteira/Modalidade de Cobrança[m[41m[m
[32m+[m[32m                     */[m[41m[m
[32m+[m[32m                    #endregion Especificação Convênio 7 posições[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto).ToString("0000"),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        "000000",[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        Utils.FormatCode(LimparCarteira(boleto.Carteira), 2));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.Cedente.Convenio,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            "21");[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                            LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 4)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                        LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 18-027[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 18-035[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18-035"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.Cedente.Convenio.ToString().Length == 7)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    #region Especificação Convênio 7 posições[m[41m[m
[32m+[m[32m                    /*[m[41m[m
[32m+[m[32m                    Posição     Tamanho     Picture     Conteúdo[m[41m[m
[32m+[m[32m                    01 a 03         03      9(3)            Código do Banco na Câmara de Compensação = 001[m[41m[m
[32m+[m[32m                    04 a 04         01      9(1)            Código da Moeda = '9'[m[41m[m
[32m+[m[32m                    05 a 05         01      9(1)            DV do Código de Barras (Anexo 10)[m[41m[m
[32m+[m[32m                    06 a 09         04      9(04)           Fator de Vencimento (Anexo 8)[m[41m[m
[32m+[m[32m                    10 a 19         10      9(08)           V(2) Valor[m[41m[m
[32m+[m[32m                    20 a 25         06      9(6)            Zeros[m[41m[m
[32m+[m[32m                    26 a 42         17      9(17)           Nosso-Número, sem o DV[m[41m[m
[32m+[m[32m                    26 a 32         9       (7)             Número do Convênio fornecido pelo Banco (CCCCCCC)[m[41m[m
[32m+[m[32m                    33 a 42         9       (10)            Complemento do Nosso-Número, sem DV (NNNNNNNNNN)[m[41m[m
[32m+[m[32m                    43 a 44         02      9(2)            Tipo de Carteira/Modalidade de Cobrança[m[41m[m
[32m+[m[32m                     */[m[41m[m
[32m+[m[32m                    #endregion Especificação Convênio 7 posições[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto).ToString("0000"),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        "000000",[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        Utils.FormatCode(LimparCarteira(boleto.Carteira), 2));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.Cedente.Convenio,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            "21");[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                            LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 4)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                        LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 18-035[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region Carteira 18-140[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18-140"))[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                if (boleto.Cedente.Convenio.ToString().Length == 7)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    #region Especificação Convênio 7 posições[m[41m[m
[32m+[m[32m                    /*[m[41m[m
[32m+[m[32m                    Posição     Tamanho     Picture     Conteúdo[m[41m[m
[32m+[m[32m                    01 a 03         03      9(3)            Código do Banco na Câmara de Compensação = 001[m[41m[m
[32m+[m[32m                    04 a 04         01      9(1)            Código da Moeda = '9'[m[41m[m
[32m+[m[32m                    05 a 05         01      9(1)            DV do Código de Barras (Anexo 10)[m[41m[m
[32m+[m[32m                    06 a 09         04      9(04)           Fator de Vencimento (Anexo 8)[m[41m[m
[32m+[m[32m                    10 a 19         10      9(08)           V(2) Valor[m[41m[m
[32m+[m[32m                    20 a 25         06      9(6)            Zeros[m[41m[m
[32m+[m[32m                    26 a 42         17      9(17)           Nosso-Número, sem o DV[m[41m[m
[32m+[m[32m                    26 a 32         9       (7)             Número do Convênio fornecido pelo Banco (CCCCCCC)[m[41m[m
[32m+[m[32m                    33 a 42         9       (10)            Complemento do Nosso-Número, sem DV (NNNNNNNNNN)[m[41m[m
[32m+[m[32m                    43 a 44         02      9(2)            Tipo de Carteira/Modalidade de Cobrança[m[41m[m
[32m+[m[32m                     */[m[41m[m
[32m+[m[32m                    #endregion Especificação Convênio 7 posições[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto).ToString("0000"),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        "000000",[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        Utils.FormatCode(LimparCarteira(boleto.Carteira), 2));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    if (boleto.TipoModalidade.Equals("21"))[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.Cedente.Convenio,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            "21");[m[41m[m
[32m+[m[32m                    else[m[41m[m
[32m+[m[32m                        boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                            Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                            boleto.Moeda,[m[41m[m
[32m+[m[32m                            FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                            valorBoleto,[m[41m[m
[32m+[m[32m                            boleto.NossoNumero,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                            boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                            LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else if (boleto.Cedente.Convenio.ToString().Length == 4)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    boleto.CodigoBarra.Codigo = string.Format("{0}{1}{2}{3}{4}{5}{6}{7}",[m[41m[m
[32m+[m[32m                        Utils.FormatCode(Codigo.ToString(), 3),[m[41m[m
[32m+[m[32m                        boleto.Moeda,[m[41m[m
[32m+[m[32m                        FatorVencimento(boleto),[m[41m[m
[32m+[m[32m                        valorBoleto,[m[41m[m
[32m+[m[32m                        boleto.NossoNumero,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                        boleto.Cedente.ContaBancaria.Conta,[m[41m[m
[32m+[m[32m                        LimparCarteira(boleto.Carteira));[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            #endregion Carteira 18-140[m[41m[m
[32m+[m[32m            _dacBoleto = Mod11(boleto.CodigoBarra.Codigo, 9);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = Strings.Left(boleto.CodigoBarra.Codigo, 4) + _dacBoleto + Strings.Right(boleto.CodigoBarra.Codigo, 39);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string cmplivre = string.Empty;[m[41m[m
[32m+[m[32m            string campo1 = string.Empty;[m[41m[m
[32m+[m[32m            string campo2 = string.Empty;[m[41m[m
[32m+[m[32m            string campo3 = string.Empty;[m[41m[m
[32m+[m[32m            string campo4 = string.Empty;[m[41m[m
[32m+[m[32m            string campo5 = string.Empty;[m[41m[m
[32m+[m[32m            long icampo5 = 0;[m[41m[m
[32m+[m[32m            int digitoMod = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            /*[m[41m[m
[32m+[m[32m            Campos 1 (AAABC.CCCCX):[m[41m[m
[32m+[m[32m            A = Código do Banco na Câmara de Compensação 001[m[41m[m
[32m+[m[32m            B = Código da moeda "9" (*)[m[41m[m
[32m+[m[32m            C = Posição 20 a 24 do código de barras[m[41m[m
[32m+[m[32m            X = DV que amarra o campo 1 (Módulo 10, contido no Anexo 7)[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            cmplivre = Strings.Mid(boleto.CodigoBarra.Codigo, 20, 25);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            campo1 = Strings.Left(boleto.CodigoBarra.Codigo, 4) + Strings.Mid(cmplivre, 1, 5);[m[41m[m
[32m+[m[32m            digitoMod = Mod10(campo1);[m[41m[m
[32m+[m[32m            campo1 = campo1 + digitoMod.ToString();[m[41m[m
[32m+[m[32m            campo1 = Strings.Mid(campo1, 1, 5) + "." + Strings.Mid(campo1, 6, 5);[m[41m[m
[32m+[m[32m            /*[m[41m[m
[32m+[m[32m            Campo 2 (DDDDD.DDDDDY)[m[41m[m
[32m+[m[32m            D = Posição 25 a 34 do código de barras[m[41m[m
[32m+[m[32m            Y = DV que amarra o campo 2 (Módulo 10, contido no Anexo 7)[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[32m            campo2 = Strings.Mid(cmplivre, 6, 10);[m[41m[m
[32m+[m[32m            digitoMod = Mod10(campo2);[m[41m[m
[32m+[m[32m            campo2 = campo2 + digitoMod.ToString();[m[41m[m
[32m+[m[32m            campo2 = Strings.Mid(campo2, 1, 5) + "." + Strings.Mid(campo2, 6, 6);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            /*[m[41m[m
[32m+[m[32m            Campo 3 (EEEEE.EEEEEZ)[m[41m[m
[32m+[m[32m            E = Posição 35 a 44 do código de barras[m[41m[m
[32m+[m[32m            Z = DV que amarra o campo 3 (Módulo 10, contido no Anexo 7)[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[32m            campo3 = Strings.Mid(cmplivre, 16, 10);[m[41m[m
[32m+[m[32m            digitoMod = Mod10(campo3);[m[41m[m
[32m+[m[32m            campo3 = campo3 + digitoMod;[m[41m[m
[32m+[m[32m            campo3 = Strings.Mid(campo3, 1, 5) + "." + Strings.Mid(campo3, 6, 6);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            /*[m[41m[m
[32m+[m[32m            Campo 4 (K)[m[41m[m
[32m+[m[32m            K = DV do Código de Barras (Módulo 11, contido no Anexo 10)[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[32m            campo4 = Strings.Mid(boleto.CodigoBarra.Codigo, 5, 1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            /*[m[41m[m
[32m+[m[32m            Campo 5 (UUUUVVVVVVVVVV)[m[41m[m
[32m+[m[32m            U = Fator de Vencimento ( Anexo 10)[m[41m[m
[32m+[m[32m            V = Valor do Título (*)[m[41m[m
[32m+[m[32m             */[m[41m[m
[32m+[m[32m            icampo5 = Convert.ToInt64(Strings.Mid(boleto.CodigoBarra.Codigo, 6, 14));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            if (icampo5 == 0)[m[41m[m
[32m+[m[32m                campo5 = "000";[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                campo5 = icampo5.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel = campo1 + "  " + campo2 + "  " + campo3 + "  " + campo4 + "  " + campo5;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o nosso número para ser mostrado no boleto.[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <remarks>[m[41m[m
[32m+[m[32m        /// Última a atualização por Transis em 26/09/2011[m[41m[m
[32m+[m[32m        /// </remarks>[m[41m[m
[32m+[m[32m        /// <param name="boleto"></param>[m[41m[m
[32m+[m[32m        public override void FormataNossoNumero(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            if (boleto.Carteira.Equals("18-019"))[m[41m[m
[32m+[m[32m                boleto.NossoNumero = string.Format("{0}/{1}", LimparCarteira(boleto.Carteira), boleto.NossoNumero);[m[41m[m
[32m+[m[32m            else if (boleto.Carteira.Equals("18-027"))[m[41m[m
[32m+[m[32m                boleto.NossoNumero = string.Format("{0}", boleto.NossoNumero);[m[41m[m
[32m+[m[32m            //Para atender o cliente Fiemg foi adptado no código na variação 18-027 as variações 18-035 e 18-140[m[41m[m
[32m+[m[32m            else if (boleto.Carteira.Equals("18-035"))[m[41m[m
[32m+[m[32m                boleto.NossoNumero = string.Format("{0}", boleto.NossoNumero);[m[41m[m
[32m+[m[32m            else if (boleto.Carteira.Equals("18-140"))[m[41m[m
[32m+[m[32m                boleto.NossoNumero = string.Format("{0}", boleto.NossoNumero);[m[41m[m
[32m+[m[32m            //somente monta o digito verificador no nosso numero se o convenio tiver 6 posições[m[41m[m
[32m+[m[32m            else if (boleto.Cedente.Convenio.ToString().Length == 6)[m[41m[m
[32m+[m[32m                boleto.NossoNumero = string.Format("{0}/{1}-{2}", LimparCarteira(boleto.Carteira), boleto.NossoNumero, Mod11BancoBrasil(boleto.NossoNumero));[m[41m[m
[32m+[m[32m            else[m[41m[m
[32m+[m[32m                boleto.NossoNumero = string.Format("{0}", boleto.NossoNumero);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNumeroDocumento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Métodos de geração do arquivo remessa CNAB240[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region HEADER[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// HEADER do arquivo CNAB[m[41m[m
[32m+[m[32m        /// Gera o HEADER do arquivo remessa de acordo com o lay-out informado[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override string GerarHeaderRemessa(string numeroConvenio, Cedente cedente, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _header = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                base.GerarHeaderRemessa(numeroConvenio, cedente, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        _header = GerarHeaderRemessaCNAB240(cedente);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        _header = GerarHeaderRemessaCNAB400(cedente);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _header;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do HEADER do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public string GerarHeaderRemessaCNAB240(Cedente cedente)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Função não implementada!");[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do TRAILER do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarHeaderRemessa(Cedente cedente, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                base.GerarHeaderRemessa(cedente, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string _brancos20 = new string(' ', 20);[m[41m[m
[32m+[m[32m                string _brancos10 = new string(' ', 10);[m[41m[m
[32m+[m[32m                string _header;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _header = "00100000         ";[m[41m[m
[32m+[m[32m                if (cedente.CPFCNPJ.Length <= 11)[m[41m[m
[32m+[m[32m                    _header += "1";[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    _header += "2";[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength(cedente.CPFCNPJ, 14, 14, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _header += _brancos20;[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength(cedente.ContaBancaria.Agencia, 5, 5, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength(cedente.ContaBancaria.DigitoAgencia, 1, 1, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength(cedente.ContaBancaria.Conta, 12, 12, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength(cedente.ContaBancaria.DigitoConta, 1, 1, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _header += " "; // DÍGITO VERIFICADOR DA AG./CONTA[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength(cedente.Nome, 30, 30, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _header += Utils.FitStringLength("BANCO DO BRASIL S.A.", 30, 30, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _header += _brancos10;[m[41m[m
[32m+[m[32m                _header += "1";[m[41m[m
[32m+[m[32m                _header += DateTime.Now.ToString("ddMMyyyy");[m[41m[m
[32m+[m[32m                _header += DateTime.Now.ToString("hhMMss");[m[41m[m
[32m+[m[32m                _header += "000001"; // NÚMERO SEQUENCIAL DO ARQUIVO *EVOLUIR UM NÚMERO A CADA HEADER DE ARQUIVO[m[41m[m
[32m+[m[32m                _header += "03000000";[m[41m[m
[32m+[m[32m                _header += _brancos20;[m[41m[m
[32m+[m[32m                _header += _brancos20;[m[41m[m
[32m+[m[32m                _header += _brancos10;[m[41m[m
[32m+[m[32m                _header += "    ";[m[41m[m
[32m+[m[32m                _header += "000  ";[m[41m[m
[32m+[m[32m                _header += _brancos10;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _header = Utils.SubstituiCaracteresEspeciais(_header);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _header;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do HEADER DE ARQUIVO do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m                             //GerarHeaderLoteRemessaCNAB240[m[41m[m
[32m+[m[32m        //[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarHeaderLoteRemessa(string numeroConvenio, Cedente cedente, int numeroArquivoRemessa, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string header = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        header = GerarHeaderLoteRemessaCNAB240(numeroConvenio,cedente, numeroArquivoRemessa);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        header = "";//GerarHeaderLoteRemessaCNAB400(0, cedente, numeroArquivoRemessa);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return header;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do HEADER DO LOTE do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string GerarHeaderLoteRemessaCNAB240(string numeroConvenio, Cedente cedente, int numeroArquivoRemessa)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _brancos40 = new string(' ', 40);[m[41m[m
[32m+[m[32m                string _brancos33 = new string(' ', 33);[m[41m[m
[32m+[m[32m                int _tamanho = numeroConvenio.Length;[m[41m[m
[32m+[m[32m                string _numeroConvenio = Utils.FitStringLength(numeroConvenio.Substring(0, _tamanho), 18, 18, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                string _headerLote;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _headerLote = "00100011R0100020 ";[m[41m[m
[32m+[m[32m                if (cedente.CPFCNPJ.Length <= 11)[m[41m[m
[32m+[m[32m                    _headerLote += "1";[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    _headerLote += "2";[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(cedente.CPFCNPJ, 15, 15, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(_numeroConvenio, 20, 20, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(cedente.ContaBancaria.Agencia, 5, 5, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(cedente.ContaBancaria.DigitoAgencia, 1, 1, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(cedente.ContaBancaria.Conta, 12, 12, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(cedente.ContaBancaria.DigitoConta, 1, 1, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _headerLote += " "; // DÍGITO VERIFICADOR DA AG./CONTA[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(cedente.Nome, 30, 30, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _headerLote += _brancos40;[m[41m[m
[32m+[m[32m                _headerLote += _brancos40;[m[41m[m
[32m+[m[32m                _headerLote += Utils.FitStringLength(numeroArquivoRemessa.ToString(), 8, 8, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _headerLote += DateTime.Now.ToString("ddMMyyyy");[m[41m[m
[32m+[m[32m                _headerLote += "00000000";[m[41m[m
[32m+[m[32m                _headerLote += _brancos33;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _headerLote = Utils.SubstituiCaracteresEspeciais(_headerLote);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _headerLote;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do HEADER DE LOTE do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region DETALHE[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// DETALHE do arquivo CNAB[m[41m[m
[32m+[m[32m        /// Gera o DETALHE do arquivo remessa de acordo com o lay-out informado[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public override string GerarDetalheRemessa(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _detalhe = " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                base.GerarDetalheRemessa(boleto, numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                switch (tipoArquivo)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB240:[m[41m[m
[32m+[m[32m                        _detalhe = GerarDetalheRemessaCNAB240(boleto, numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.CNAB400:[m[41m[m
[32m+[m[32m                        _detalhe = GerarDetalheRemessaCNAB400(boleto, numeroRegistro, tipoArquivo);[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case TipoArquivo.Outro:[m[41m[m
[32m+[m[32m                        throw new Exception("Tipo de arquivo inexistente.");[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _detalhe;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do DETALHE arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarDetalheSegmentoPRemessa(Boleto boleto, int numeroRegistro, string numeroConvenio)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _segmentoP;[m[41m[m
[32m+[m[32m                string _nossoNumero;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoP = "00100013";[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(numeroRegistro.ToString(), 5, 5, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoP += "P 01";[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.Cedente.ContaBancaria.Agencia, 5, 5, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.Cedente.ContaBancaria.DigitoAgencia, 1, 1, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.Cedente.ContaBancaria.Conta, 12, 12, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.Cedente.ContaBancaria.DigitoConta, 1, 1, '0', 0, true, true, false);[m[41m[m
[32m+[m[32m                _segmentoP += " ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                int totalCaracteres = numeroConvenio.Length - 9;[m[41m[m
[32m+[m[32m                _segmentoP += numeroConvenio.Substring(0, totalCaracteres);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _nossoNumero = Utils.FitStringLength(boleto.NumeroDocumento, 10, 10, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                int _total = numeroConvenio.Substring(0, totalCaracteres).Length + _nossoNumero.Length;[m[41m[m
[32m+[m[32m                int subtotal = 0;[m[41m[m
[32m+[m[32m                subtotal = 20 - _total;[m[41m[m
[32m+[m[32m                string _comnplemento = new string(' ', subtotal);[m[41m[m
[32m+[m[32m                _segmentoP += _nossoNumero;[m[41m[m
[32m+[m[32m                _segmentoP += _comnplemento;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(LimparCarteira(boleto.Carteira), 1, 1, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoP += "1111";[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.NumeroDocumento, 15, 15, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.DataVencimento.ToString("ddMMyyyy"), 8, 8, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(Convert.ToString(boleto.ValorBoleto * 100), 15, 15, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoP += "000000";[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.EspecieDocumento.Codigo.ToString(), 2, 2, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoP += "N";[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.DataDocumento.ToString("ddMMyyyy"), 8, 8, ' ', 0, true, true, false);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (boleto.JurosMora > 0)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _segmentoP += "1";[m[41m[m
[32m+[m[32m                    _segmentoP += Utils.FitStringLength(boleto.DataVencimento.ToString("ddMMyyyy"), 8, 8, '0', 0, true, true, false);[m[41m[m
[32m+[m[32m                    _segmentoP += Utils.FitStringLength(Convert.ToString(boleto.JurosMora * 100), 15, 15, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _segmentoP += "3";[m[41m[m
[32m+[m[32m                    _segmentoP += "00000000";[m[41m[m
[32m+[m[32m                    _segmentoP += "000000000000000";[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (boleto.ValorDesconto > 0)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _segmentoP += "1";[m[41m[m
[32m+[m[32m                    _segmentoP += Utils.FitStringLength(boleto.DataVencimento.ToString("ddMMyyyy"), 8, 8, '0', 0, true, true, false);[m[41m[m
[32m+[m[32m                    _segmentoP += Utils.FitStringLength(Convert.ToString(boleto.ValorDesconto * 100), 15, 15, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    _segmentoP += "000000000000000000000000";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoP += "000000000000000";[m[41m[m
[32m+[m[32m                _segmentoP += "000000000000000";[m[41m[m
[32m+[m[32m                _segmentoP += Utils.FitStringLength(boleto.NumeroDocumento, 25, 25, ' ', 0, true, true, false);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (boleto.Instrucoes.Count > 1 && boleto.Instrucoes[0].QuantidadeDias > 0)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    _segmentoP += "2";[m[41m[m
[32m+[m[32m                    _segmentoP += Utils.FitStringLength(boleto.Instrucoes[0].QuantidadeDias.ToString(), 2, 2, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    _segmentoP += "300";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoP += "2000090000000000 ";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoP = Utils.SubstituiCaracteresEspeciais(_segmentoP);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _segmentoP;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do SEGMENTO P DO DETALHE do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarDetalheSegmentoQRemessa(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _nossoNumero = new string('0', 20);[m[41m[m
[32m+[m[32m                string _zeros16 = new string('0', 16);[m[41m[m
[32m+[m[32m                string _brancos40 = new string(' ', 40);[m[41m[m
[32m+[m[32m                string _brancos28 = new string(' ', 28);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                string _segmentoQ;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoQ = "00100013";[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength(numeroRegistro.ToString(), 5, 5, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoQ += "Q 01";[m[41m[m
[32m+[m[32m                if (boleto.Sacado.CPFCNPJ.Length <= 11)[m[41m[m
[32m+[m[32m                    _segmentoQ += "1";[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    _segmentoQ += "2";[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength(boleto.Sacado.CPFCNPJ, 15, 15, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength(boleto.Sacado.Nome.ToUpper(), 40, 40, ' ', 0, true, true, false);[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength((boleto.Sacado.Endereco.End + " " + boleto.Sacado.Endereco.Numero + " - " + boleto.Sacado.Endereco.Complemento), 40, 40, ' ', 0, true, true, true).ToUpper();[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength(boleto.Sacado.Endereco.Bairro, 15, 15, ' ', 0, true, true, false).ToUpper();[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength(boleto.Sacado.Endereco.CEP, 8, 8, ' ', 0, true, true, false).ToUpper(); ;[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength(boleto.Sacado.Endereco.Cidade, 15, 15, ' ', 0, true, true, false).ToUpper();[m[41m[m
[32m+[m[32m                _segmentoQ += Utils.FitStringLength(boleto.Sacado.Endereco.UF, 2, 2, ' ', 0, true, true, false).ToUpper();[m[41m[m
[32m+[m[32m                _segmentoQ += _zeros16;[m[41m[m
[32m+[m[32m                _segmentoQ += _brancos40;[m[41m[m
[32m+[m[32m                _segmentoQ += "000";[m[41m[m
[32m+[m[32m                _segmentoQ += _brancos28;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoQ = Utils.SubstituiCaracteresEspeciais(_segmentoQ);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _segmentoQ;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do SEGMENTO Q DO DETALHE do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override string GerarDetalheSegmentoRRemessa(Boleto boleto, int numeroRegistro, TipoArquivo tipoArquivo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string _brancos90 = new string(' ', 90);[m[41m[m
[32m+[m[32m                string _brancos33 = new string(' ', 33);[m[41m[m
[32m+[m[32m                string _zeros27 = new string('0', 27);[m[41m[m
[32m+[m[32m                string _segmentoR;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoR = "00100013";[m[41m[m
[32m+[m[32m                _segmentoR += Utils.FitStringLength(numeroRegistro.ToString(), 5, 5, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoR += "R ";[m[41m[m
[32m+[m[32m                _segmentoR += "01";[m[41m[m
[32m+[m[32m                // Código do desconto 2 - percentual até a data informada[m[41m[m
[32m+[m[32m                // Data do desconto[m[41m[m
[32m+[m[32m                // Valor/Percentual do desconto[m[41m[m
[32m+[m[32m                // Campos: Desconto 2 e Desconto 3[m[41m[m
[32m+[m[32m                _segmentoR += "000000000000000000000000";[m[41m[m
[32m+[m[32m                _segmentoR += "000000000000000000000000";[m[41m[m
[32m+[m[32m                // Código da multa 2 - percentual[m[41m[m
[32m+[m[32m                _segmentoR += "2";[m[41m[m
[32m+[m[32m                _segmentoR += Utils.FitStringLength(boleto.DataMulta.ToString("ddMMyyyy"), 8, 8, '0', 0, true, true, false);[m[41m[m
[32m+[m[32m                _segmentoR += Utils.FitStringLength(Convert.ToString(boleto.ValorMulta * 100), 15, 15, '0', 0, true, true, true);[m[41m[m
[32m+[m[32m                _segmentoR += _brancos90;[m[41m[m
[32m+[m[32m                _segmentoR += _zeros27;[m[41m[m
[32m+[m[32m                _segmentoR += _brancos33;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                _segmentoR = Utils.SubstituiCaracteresEspeciais(_segmentoR);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return _segmentoR;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro durante a geração do SEGMENTO R DO DETALHE do arquivo de REMESSA.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
