[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Retorna o UF[m[41m[m
[32m+[m[32m        /// Exemplo :[m[41m[m
[32m+[m[32m        /// SP - São Paulo[m[41m[m
[32m+[m[32m        /// SC - Santa Catarina[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string UF[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return _uf;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this._uf = value;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Retorna o número do CEP[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string CEP[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                //return _cep.Replace(".", "").Replace("-", "");[m[41m[m
[32m+[m[32m                return _cep;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            //Flavio(fhlviana@hotmail.com) - o metodo "Set" acontece menos vezes do que o get, por estimativa. Sendo assim, armazenar[m[41m[m
[32m+[m[32m            //sem o "." e o "-" faz com que o código tenda a executar os dois Replace uma vez só.[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                //this._cep = value;[m[41m[m
[32m+[m[32m                this._cep = value.Replace(".", "").Replace("-", "");[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Retorna o E-Mail[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string Email[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return _email;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                this._email = value;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
