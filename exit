[32m+[m[32m{[m[41m[m
[32m+[m[32m    public partial class ImpressaoBoleto : Form[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        public ImpressaoBoleto()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            InitializeComponent();[m[41m[m
