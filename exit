[1mindex 0000000..f0be5be[m
[1m--- /dev/null[m
[1m+++ b/Boleto.Net/Banco/Banco_Sudameris.cs[m
[36m@@ -0,0 +1,190 @@[m
[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Web.UI;[m[41m[m
[32m+[m[32musing Microsoft.VisualBasic;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m[assembly: WebResource("BoletoNet.Imagens.347.jpg", "image/jpg")][m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    /// <author>[m[41m  [m
[32m+[m[32m    /// Eduardo Frare[m[41m[m
[32m+[m[32m    /// Stiven[m[41m [m
[32m+[m[32m    /// Diogo(Allmatech)[m[41m[m
[32m+[m[32m    /// Miamoto(Allmatech)[m[41m[m
[32m+[m[32m    /// Rodrigo(Allmatech)[m[41m[m
[32m+[m[32m    /// </author>[m[41m    [m
[32m+[m[41m[m
[32m+[m[32m    ///<summary>[m[41m[m
[32m+[m[32m    /// Classe referente ao Banco Banco_Sudameris[m[41m[m
[32m+[m[32m    ///</summary>[m[41m[m
[32m+[m[32m    internal class Banco_Sudameris : AbstractBanco, IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private int _dacDigitaoCobranca = 0;[m[41m[m
[32m+[m[32m        private int _dacBoleto = 0;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        internal Banco_Sudameris()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            this.Codigo = 347;[m[41m[m
[32m+[m[32m            this.Digito = 6;[m[41m[m
[32m+[m[32m            this.Nome = "Banco_Sudameris";[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region IBanco Members[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void ValidaBoleto(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            //Verifica se o nosso número é válido[m[41m[m
[32m+[m[32m            if (Utils.ToInt32(boleto.NossoNumero) == 0)[m[41m[m
[32m+[m[32m                throw new NotImplementedException("Nosso número inválido");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //O número da conta corrente são 7 dígitos[m[41m[m
[32m+[m[32m            if (boleto.Cedente.ContaBancaria.Conta.Length != 7)[m[41m[m
[32m+[m[32m                throw new Exception("O número da conta corrente do cedente são 7 números.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se o tamanho para o NossoNumero[m[41m[m
[32m+[m[32m            // 7 para cobrança registrada[m[41m[m
[32m+[m[32m            // 13 para cobrança sem registro[m[41m[m
[32m+[m[32m            boleto.NossoNumero = Utils.FormatCode(boleto.NossoNumero, 13);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            // Calcula o digitão de cobrança DAC (Nosso Número/Agência/Conta Corrente)[m[41m[m
[32m+[m[32m            _dacDigitaoCobranca = Mod10(boleto.NossoNumero + boleto.Cedente.ContaBancaria.Agencia + boleto.Cedente.ContaBancaria.Conta);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Atribui o nome do banco ao local de pagamento[m[41m[m
[32m+[m[32m            boleto.LocalPagamento += Nome;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do processamento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataProcessamento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataProcessamento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            //Verifica se data do documento é valida[m[41m[m
[32m+[m[32m            if (boleto.DataDocumento.ToString("dd/MM/yyyy") == "01/01/0001")[m[41m[m
[32m+[m[32m                boleto.DataDocumento = DateTime.Now;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            FormataCodigoBarra(boleto);[m[41m[m
[32m+[m[32m            FormataLinhaDigitavel(boleto);[m[41m[m
[32m+[m[32m            FormataNumeroDocumento(boleto);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataLinhaDigitavel(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            #region Definições[m[41m[m
[32m+[m[32m            /* AAABC.CCCDDX.DDDDD.DEFFFY.FGGGG.GGHHHZ.K.UUUUVVVVVVVVVV[m[41m[m
[32m+[m[32m              * ------------------------------------------------------[m[41m[m
[32m+[m[32m              * Campo 1[m[41m[m
[32m+[m[32m              * AAABC.CCCDX[m[41m[m
[32m+[m[32m              * AAA - Código do Banco[m[41m[m
[32m+[m[32m              * B   - Moeda[m[41m[m
[32m+[m[32m              * CCCC - Agência[m[41m[m
[32m+[m[32m              * D  - 1 primeiro número da Conta Corrente[m[41m[m
[32m+[m[32m              * X   - DAC Campo 1 (AAABC.CCDD) Mod10[m[41m[m
[32m+[m[32m              *[m[41m [m
[32m+[m[32m              * Campo 2[m[41m[m
[32m+[m[32m              * DDDDD.DEFFFY[m[41m[m
[32m+[m[32m              * DDDDD.D - Restante do Conta Corrente[m[41m[m
[32m+[m[32m              * E       - DAC (Nosso Número/Agência/Conta)[m[41m[m
[32m+[m[32m              * FFF     - Três primeiros do Nosso Número[m[41m[m
[32m+[m[32m              * Y       - DAC Campo 2 (DDDDD.DEFFF) Mod10[m[41m[m
[32m+[m[32m              *[m[41m [m
[32m+[m[32m              * Campo 3[m[41m[m
[32m+[m[32m              * FFFFF.FFFFZ[m[41m[m
[32m+[m[32m              * FFFFFFFFF- Restante Nosso Número[m[41m[m
[32m+[m[32m              * Z       - DAC Campo 3[m[41m[m
[32m+[m[32m              *[m[41m [m
[32m+[m[32m              * Campo 4[m[41m[m
[32m+[m[32m              * K       - DAC Código de Barras[m[41m[m
[32m+[m[32m              *[m[41m [m
[32m+[m[32m              * Campo 5[m[41m[m
[32m+[m[32m              * UUUUVVVVVVVVVV[m[41m[m
[32m+[m[32m              * UUUU       - Fator Vencimento[m[41m[m
[32m+[m[32m              * VVVVVVVVVV - Valor do Título[m[41m [m
[32m+[m[32m              */[m[41m[m
[32m+[m[32m            #endregion Definições[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string numeroDocumento = Utils.FormatCode(boleto.NumeroDocumento.ToString(), 7);[m[41m[m
[32m+[m[32m            string codigoCedente = Utils.FormatCode(boleto.Cedente.Codigo.ToString(), 5);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string AAA = Utils.FormatCode(Codigo.ToString(), 3);[m[41m[m
[32m+[m[32m            string B = boleto.Moeda.ToString();[m[41m[m
[32m+[m[32m            string CCCC = boleto.Cedente.ContaBancaria.Agencia;[m[41m[m
[32m+[m[32m            string D = boleto.Cedente.ContaBancaria.Conta.Substring(0, 1);[m[41m[m
[32m+[m[32m            string X = Mod10(AAA + B + CCCC + D).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string DDDDDD = boleto.Cedente.ContaBancaria.Conta.Substring(1, 6);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string K = string.Format(" {0} ", _dacDigitaoCobranca);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string UUUU = FatorVencimento(boleto).ToString();[m[41m[m
[32m+[m[32m            string VVVVVVVVVV = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string C1 = string.Empty;[m[41m[m
[32m+[m[32m            string C2 = string.Empty;[m[41m[m
[32m+[m[32m            string C3 = string.Empty;[m[41m[m
[32m+[m[32m            string C4 = string.Empty;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region AAABC.CCDDX[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            C1 = string.Format("{0}{1}{2}.", AAA, B, CCCC.Substring(0, 1));[m[41m[m
[32m+[m[32m            C1 += string.Format("{0}{1}{2} ", CCCC.Substring(1, 3), D, X);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion AAABC.CCDDX[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region UUUUVVVVVVVVVV[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            VVVVVVVVVV = Utils.FormatCode(VVVVVVVVVV, 10);[m[41m[m
[32m+[m[32m            C4 = UUUU + VVVVVVVVVV;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion UUUUVVVVVVVVVV[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region DDDDD.DEFFFY[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string E = _dacDigitaoCobranca.ToString();[m[41m[m
[32m+[m[32m            string FFF = boleto.NossoNumero.Substring(0, 3);[m[41m[m
[32m+[m[32m            string Y = Mod10(DDDDDD + E + FFF).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            C2 = string.Format("{0}.", DDDDDD.Substring(0, 5));[m[41m[m
[32m+[m[32m            C2 += string.Format("{0}{1}{2}{3} ", DDDDDD.Substring(5, 1), E, FFF, Y);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion DDDDD.DEFFFY[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #region FFFFF.FFFFFZ[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string FFFFFFFFF = boleto.NossoNumero.Substring(3, 10);[m[41m[m
[32m+[m[32m            string Z = Mod10(FFFFFFFFF).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            C3 = string.Format("{0}.{1}{2}", FFFFFFFFF.Substring(0, 5), FFFFFFFFF.Substring(5, 5), Z);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            #endregion FGGGG.GGHHHZ[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.LinhaDigitavel = C1 + C2 + C3 + K + C4;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public void FormataNossoNumero()[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataNumeroDocumento(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            boleto.NumeroDocumento = Utils.FormatCode(boleto.NossoNumero, 13);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public override void FormataCodigoBarra(Boleto boleto)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            // Código de Barras[m[41m[m
[32m+[m[32m            //Código do Banco/Moeda/ DAC /Fator Vencimento/Valor/Agência/Conta + Digitão/Nosso Número[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            string valorBoleto = boleto.ValorBoleto.ToString("f").Replace(",", "").Replace(".", "");[m[41m[m
[32m+[m[32m            valorBoleto = Utils.FormatCode(valorBoleto, 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo =[m[41m[m
[32m+[m[32m                    string.Format("{0}{1}{2}{3}{4}{5}{6}{7}", Codigo, boleto.Moeda,[m[41m[m
[32m+[m[32m                                  FatorVencimento(boleto), valorBoleto, boleto.Cedente.ContaBancaria.Agencia,[m[41m[m
[32m+[m[32m                                  boleto.Cedente.ContaBancaria.Conta, _dacDigitaoCobranca, boleto.NossoNumero);[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            _dacBoleto = Mod11(boleto.CodigoBarra.Codigo, 9, 0);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            boleto.CodigoBarra.Codigo = Strings.Left(boleto.CodigoBarra.Codigo, 4) + _dacBoleto + Strings.Right(boleto.CodigoBarra.Codigo, 39);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m        #endregion IBanco Members[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
