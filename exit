[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Drawing;[m[41m[m
[32m+[m[32musing Microsoft.VisualBasic;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public class CodigoBarra[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        private string _codigo = "";[m[41m[m
[32m+[m[32m        private string _linhaDigitavel = "";[m[41m[m
[32m+[m[32m        private Image _imagem = null;[m[41m[m
[32m+[m[32m        private string _chave = "";[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Código de Barra[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string Codigo[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _codigo; }[m[41m[m
[32m+[m[32m            set { _codigo = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public Image Imagem[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _imagem; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Retorna a representação numérica do código de barra[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string LinhaDigitavel[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get{return _linhaDigitavel;}[m[41m[m
[32m+[m[32m            set{_linhaDigitavel = value;}[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Chave para montar Codigo de Barra[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        public string Chave[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get { return _chave; }[m[41m[m
[32m+[m[32m            set { _chave = value; }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private string FormataLinhaDigitavel(string _codigo)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                string cmplivre;[m[41m[m
[32m+[m[32m                string campo1;[m[41m[m
[32m+[m[32m                string campo2;[m[41m[m
[32m+[m[32m                string campo3;[m[41m[m
[32m+[m[32m                string campo4;[m[41m[m
[32m+[m[32m                string campo5;[m[41m[m
[32m+[m[32m                long icampo5;[m[41m[m
[32m+[m[32m                int digitoMod;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                cmplivre = Strings.Mid(_codigo, 20, 25);[m[41m[m
[32m+[m[32m                campo1 = Strings.Left(_codigo, 4) + Strings.Mid(cmplivre, 1, 5);[m[41m[m
[32m+[m[32m                digitoMod = AbstractBanco.Mod10(campo1);[m[41m[m
[32m+[m[32m                campo1 = campo1 + digitoMod.ToString();[m[41m[m
[32m+[m[32m                campo1 = Strings.Mid(campo1, 1, 5) + "." + Strings.Mid(campo1, 6, 5);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                campo2 = Strings.Mid(cmplivre, 6, 10);[m[41m[m
[32m+[m[32m                digitoMod = AbstractBanco.Mod10(campo2);[m[41m[m
[32m+[m[32m                campo2 = campo2 + digitoMod.ToString();[m[41m[m
[32m+[m[32m                campo2 = Strings.Mid(campo2, 1, 5) + "." + Strings.Mid(campo2, 6, 6);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                campo3 = Strings.Mid(cmplivre, 16, 10);[m[41m[m
[32m+[m[32m                digitoMod = AbstractBanco.Mod10(campo3);[m[41m[m
[32m+[m[32m                campo3 = campo3 + digitoMod;[m[41m[m
[32m+[m[32m                campo3 = Strings.Mid(campo3, 1, 5) + "." + Strings.Mid(campo3, 6, 6);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                campo4 = Strings.Mid(_codigo, 5, 1);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                icampo5 = Convert.ToInt64(Strings.Mid(_codigo, 6, 14));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (icampo5 == 0)[m[41m[m
[32m+[m[32m                    campo5 = "000";[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    campo5 = icampo5.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                return campo1 + "  " + campo2 + "  " + campo3 + "  " + campo4 + "  " + campo5;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Código de barras inválido");[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[1mdiff --git a/Boleto.Net/Boleto/CodigoLiquidacao/AbstractCodigoLiquidacao.cs b/Boleto.Net/Boleto/CodigoLiquidacao/AbstractCodigoLiquidacao.cs[m
[1mnew file mode 100644[m
[1mindex 0000000..80a30cb[m
[1m--- /dev/null[m
[1m+++ b/Boleto.Net/Boleto/CodigoLiquidacao/AbstractCodigoLiquidacao.cs[m
[36m@@ -0,0 +1,85 @@[m
[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Collections.Generic;[m[41m[m
[32m+[m[32musing System.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public abstract class AbstractCodigoLiquidacao : ICodigoLiquidacao[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Variaveis[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        private IBanco _banco;[m[41m[m
[32m+[m[32m        private int _enum;[m[41m[m
[32m+[m[32m        private string _codigo;[m[41m[m
[32m+[m[32m        private string _descricao;[m[41m[m
[32m+[m[32m        private string _recurso;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # region Propriedades[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public virtual IBanco Banco[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return _banco;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                _banco = value;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public virtual int Enumerado[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                return _enum;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            set[m[41m[m
