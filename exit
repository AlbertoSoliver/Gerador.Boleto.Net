[32m+[m[32m                        this.Tipo = "";[m[41m[m
[32m+[m[32m                        this.Descricao = "Cobrança Simples Com Registro";[m[41m[m
[32m+[m[32m                        break;[m[41m[m
[32m+[m[32m                    case EnumCarteiras_Santander.CobrancaSimplesSemRegistro:[m[41m[m
