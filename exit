[32m+[m[32m        public void LerDetalheSegmentoURetornoCNAB240(string registro)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            try[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                _registro = Registro;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (registro.Substring(13, 1) != "U")[m[41m[m
[32m+[m[32m                    throw new Exception("Registro inválida. O detalhe não possuí as características do segmento U.");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                int dataOcorrencia = Convert.ToInt32(registro.Substring(137, 8));[m[41m[m
[32m+[m[32m                int dataCredito = Convert.ToInt32(registro.Substring(145, 8));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                int dataOcorrenciaSacado = 0;[m[41m[m
[32m+[m[32m                if (registro.Substring(153, 4) != "    ")[m[41m[m
[32m+[m[32m                    dataOcorrenciaSacado = Convert.ToInt32(registro.Substring(157, 8));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                double jurosMultaEncargos = Convert.ToInt64(registro.Substring(17, 15));[m[41m[m
[32m+[m[32m                JurosMultaEncargos = jurosMultaEncargos / 100;[m[41m[m
[32m+[m[32m                double valorDescontoConcedido = Convert.ToInt64(registro.Substring(32, 15));[m[41m[m
[32m+[m[32m                ValorDescontoConcedido = valorDescontoConcedido / 100;[m[41m[m
[32m+[m[32m                double valorAbatimentoConcedido = Convert.ToInt64(registro.Substring(47, 15));[m[41m[m
[32m+[m[32m                ValorAbatimentoConcedido = valorAbatimentoConcedido / 100;[m[41m[m
[32m+[m[32m                double valorIOFRecolhido = Convert.ToInt64(registro.Substring(62, 15));[m[41m[m
[32m+[m[32m                ValorIOFRecolhido = valorIOFRecolhido / 100;[m[41m[m
[32m+[m[32m                double valorPagoPeloSacado = Convert.ToInt64(registro.Substring(77, 15));[m[41m[m
[32m+[m[32m                ValorPagoPeloSacado = valorPagoPeloSacado / 100;[m[41m[m
[32m+[m[32m                double valorLiquidoASerCreditado = Convert.ToInt64(registro.Substring(92, 15));[m[41m[m
[32m+[m[32m                ValorLiquidoASerCreditado = valorLiquidoASerCreditado / 100;[m[41m[m
[32m+[m[32m                double valorOutrasDespesas = Convert.ToInt64(registro.Substring(107, 15));[m[41m[m
[32m+[m[32m                ValorOutrasDespesas = valorOutrasDespesas / 100;[m[41m[m
[32m+[m[32m                double valorOutrosCreditos = Convert.ToInt64(registro.Substring(122, 15));[m[41m[m
[32m+[m[32m                ValorOutrosCreditos = valorOutrosCreditos / 100;[m[41m[m
[32m+[m[32m                DataOcorrencia = Convert.ToDateTime(dataOcorrencia.ToString("##-##-####"));[m[41m[m
[32m+[m[32m                DataCredito = Convert.ToDateTime(dataCredito.ToString("##-##-####"));[m[41m[m
[32m+[m[32m                CodigoOcorrenciaSacado = registro.Substring(153, 4);[m[41m[m
[32m+[m[32m                if (dataOcorrenciaSacado != 0)[m[41m[m
[32m+[m[32m                    DataOcorrenciaSacado = Convert.ToDateTime(dataOcorrenciaSacado.ToString("##-##-####"));[m[41m[m
[32m+[m[32m                double valorOcorrenciaSacado = Convert.ToInt64(registro.Substring(165, 15));[m[41m[m
[32m+[m[32m                ValorOcorrenciaSacado = valorOcorrenciaSacado / 100;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            catch (Exception ex)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                throw new Exception("Erro ao processar arquivo de RETORNO - SEGMENTO U.", ex);[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
