[1mnew file mode 100644[m
[1mindex 0000000..5e6febd[m
[1m--- /dev/null[m
[1m+++ b/Boleto.Net/Banco/IBanco.cs[m
[36m@@ -0,0 +1,91 @@[m
[32m+[m[32musing System;[m[41m[m
[32m+[m[32musing System.Collections.Generic;[m[41m[m
[32m+[m[32musing System.Text;[m[41m[m
[32m+[m[41m[m
[32m+[m[32mnamespace BoletoNet[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public interface IBanco[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o código de barras[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        void FormataCodigoBarra(Boleto boleto);[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata a linha digital[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        void FormataLinhaDigitavel(Boleto boleto);[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o nosso número[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        void FormataNossoNumero(Boleto boleto);[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Formata o número do documento, alguns bancos exige uma formatação. Tipo: 123-4[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        void FormataNumeroDocumento(Boleto boleto);[m[41m        [m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// Responsável pela validação de todos os dados referente ao banco, que serão usados no boleto[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
