[32m+[m[32m    public class InfoSacado[m[41m [m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        String[] _data;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary></summary>[m[41m[m
[32m+[m[32m        /// <param name="info">Texto da informaÃ§Ã£o</param>[m[41m[m
[32m+[m[32m        public InfoSacado(String info)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            _data = new String[] { info };[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m        [m
[32m+[m[32m        /// <summary></summary>[m[41m[m
[32m+[m[32m        /// <param name="linha1">Texto da primeira linha</param>[m[41m[m
[32m+[m[32m        /// <param name="linha2">Texto da segunda linha</param>[m[41m[m
[32m+[m[32m        public InfoSacado(String linha1, String linha2)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            _data = new String[]{linha1,linha2};[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary></summary>[m[41m[m
[32m+[m[32m        /// <param name="linhas">Vetor com as infomaÃ§oes do Sacado, onde cada posiÃ§Ã£o Ã© uma linha da informaÃ§Ã£o no boleto</param>[m[41m[m
[32m+[m[32m        public InfoSacado(String[] linhas)[m[41m [m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            _data = linhas;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public String HTML[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            get[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                String rtn = "";[m[41m[m
[32m+[m[32m                foreach (String S in _data)[m[41m[m
[32m+[m[32m                {[m[41m[m
[32m+[m[32m                    rtn += "<br />" + S;[m[41m [m
[32m+[m[41m                [m
[32m+[m[32m                }[m[41m[m
[32m+[m[32m                return rtn;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static String Render(String linha1, String linha2, Boolean novaLinha)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m          return Render(new String[] { linha1, linha2 }, novaLinha);[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        public static String Render(String[] linhas, Boolean novaLinha)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            String rtn = "";[m[41m[m
[32m+[m[32m            foreach (String S in linhas)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                rtn += "<br />" + S;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[32m            if (!novaLinha) rtn = rtn.Substring(6);[m[41m[m
[32m+[m[32m            return rtn;[m[41m[m
[32m+[m[32m        }[m[41m[m
