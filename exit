[32m+[m[32m        {[m[41m[m
[32m+[m[32m            throw new NotImplementedException("Função não implementada.");[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        # endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos de processamento do arquivo retorno CNAB400[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #region Métodos privados[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// DV DAC Mod11[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="cVariavel"></param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        private string DVDAC(string cVariavel)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string lRetorno = "0";[m[41m[m
[32m+[m[32m            int nSoma = 0, nMult = 2, nIndice;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (nIndice = cVariavel.Length - 1; nIndice >= 0; nIndice--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                nSoma += (Convert.ToByte(cVariavel[nIndice]) - 48) * nMult;[m[41m[m
[32m+[m[32m                if (nMult == 9) nMult = 2;[m[41m[m
[32m+[m[32m                else nMult++;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            nSoma = nSoma % 11;[m[41m[m
[32m+[m[32m            if (nSoma < 2 || nSoma > 9) lRetorno = "1";[m[41m[m
[32m+[m[32m            else lRetorno = (11 - nSoma).ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return lRetorno;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// DV NossoNumero Mod11[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="cVariavel"></param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        private string DVNossoNumero(string cVariavel)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[32m            string lRetorno = "0";[m[41m[m
[32m+[m[32m            int nSoma = 0, nMult = 9, nIndice;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (nIndice = cVariavel.Length - 1; nIndice >= 0; nIndice--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                nSoma += (Convert.ToByte(cVariavel[nIndice]) - 48) * nMult;[m[41m[m
[32m+[m[32m                if (nMult == 2) nMult = 9;[m[41m[m
[32m+[m[32m                else nMult--;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            nSoma = nSoma % 11;[m[41m[m
[32m+[m[32m            if (nSoma > 9) lRetorno = "0";[m[41m[m
[32m+[m[32m            else lRetorno = nSoma.ToString();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return lRetorno;[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        /// <summary>[m[41m[m
[32m+[m[32m        /// DV Linha Digitavel Mod10[m[41m[m
[32m+[m[32m        /// </summary>[m[41m[m
[32m+[m[32m        /// <param name="seq"></param>[m[41m[m
[32m+[m[32m        /// <returns></returns>[m[41m[m
[32m+[m[32m        private string DVLinhaDigitavel(string seq)[m[41m[m
[32m+[m[32m        {[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            int Digito, Soma = 0, Peso = 2, res;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            for (int i = seq.Length; i > 0; i--)[m[41m[m
[32m+[m[32m            {[m[41m[m
[32m+[m[32m                res = (Convert.ToInt32(Strings.Mid(seq, i, 1)) * Peso);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (res > 9)[m[41m[m
[32m+[m[32m                    res = (res - 9);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                Soma += res;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m                if (Peso == 2)[m[41m[m
[32m+[m[32m                    Peso = 1;[m[41m[m
[32m+[m[32m                else[m[41m[m
[32m+[m[32m                    Peso = Peso + 1;[m[41m[m
[32m+[m[32m            }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            Digito = ((10 - (Soma % 10)) % 10);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            return Digito.ToString();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        #endregion[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
